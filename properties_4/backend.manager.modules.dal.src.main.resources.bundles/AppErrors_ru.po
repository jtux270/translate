#: VALIDATION_GLUSTER_VOLUME_NAME_NOT_NULL
msgid "Volume Name is required."
msgstr "Требуется имя тома."

#: VALIDATION_GLUSTER_VOLUME_TYPE_NOT_NULL
msgid "Volume Type is required."
msgstr "Требуется тип тома."

#: VALIDATION_GLUSTER_VOLUME_REPLICA_COUNT_NOT_NULL
msgid "Replica Count is required."
msgstr "Требуется число реплик."

#: VALIDATION_GLUSTER_VOLUME_STRIPE_COUNT_NOT_NULL
