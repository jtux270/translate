<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:org.ovirt.engine.ui.webadmin.widget.form">

	<ui:style>
	.frame {
		overflow: auto;
		width: 100%;
		height: 100%;
		position: absolute;
	}
	</ui:style>

	<g:HTMLPanel>
		<d:PostableFrame ui:field="reportPostableFrame" addStyleNames="{style.frame}"></d:PostableFrame>
	</g:HTMLPanel>
</ui:UiBinder>
