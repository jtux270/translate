<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:e="urn:import:org.ovirt.engine.ui.common.widget.editor.generic">

    <ui:with field="constants" type="org.ovirt.engine.ui.webadmin.ApplicationConstants" />

    <ui:style type="org.ovirt.engine.ui.webadmin.section.main.view.popup.qos.HostNetworkQosWidget.Style">
        .paddingLeft {
            padding-left: 5px;
	    }

	    .lineHeight {
            line-height: 20px;
	    }

        .valueBox {
            width: 100px;
	    }
    </ui:style>

    <g:FlowPanel ui:field="mainPanel" >
        <e:StringEntityModelLabelEditor ui:field="qosOutLabel" addStyleNames="{style.lineHeight} {style.paddingLeft}" />
        <e:StringEntityModelTextBoxEditor ui:field="outAverageLinkshare" addStyleNames="{style.paddingLeft}" />
        <e:StringEntityModelTextBoxEditor ui:field="outAverageUpperlimit" addStyleNames="{style.paddingLeft}" />
        <e:StringEntityModelTextBoxEditor ui:field="outAverageRealtime" addStyleNames="{style.paddingLeft}" />
    </g:FlowPanel>

</ui:UiBinder>
