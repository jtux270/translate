<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog" xmlns:ge="urn:import:org.ovirt.engine.ui.common.widget.editor.generic">

	<ui:style>
		.messageLabel {
			padding-bottom: 25px;
			margin-top: 5px;
			margin-left: 15px;
			margin-right: 15px;
		}
		.noteLabel {
			padding-bottom: 25px;
			margin-left: 15px;
			margin-right: 10px;
		}

	</ui:style>

	<d:SimpleDialogPanel width="380px" height="270px">
		<d:content>
			<g:FlowPanel>
				<g:HTML ui:field="messageLabel" addStyleNames="{style.messageLabel}"/>
				<g:HTML ui:field="noteLabel" addStyleNames="{style.noteLabel}"/>
				<ge:EntityModelCheckBoxEditor ui:field="latch" />
			</g:FlowPanel>
		</d:content>
	</d:SimpleDialogPanel>

</ui:UiBinder>
