msgctxt "FailedToUnPlugDisk"
msgid "Failed to unplug a disk"
msgstr "Сбой отключения диска"

msgctxt "VOLUME_WAS_NOT_PREPARED_BEFORE_TEARDOWN"
msgid "Volume was not prepared before being torn down"
msgstr "Том не был подготовлен для уничтожения"

msgctxt "IMAGES_NOT_SUPPORTED_ERROR"
msgid "This domain does not support images"
msgstr "Этот домен не поддерживает изображения"

msgctxt "GET_FILE_LIST_ERROR"
msgid "Cannot get files list"
msgstr "Не удалось получить список файлов"

msgctxt "STORAGE_DOMAIN_REFRESH_ERROR"
msgid "Cannot refresh storage domain"
msgstr "Не удалось обновить сведения о домене хранилища"

msgctxt "VOLUME_GROUP_BLOCK_SIZE_ERROR"
msgid "Volume Group block size error"
msgstr "Ошибка размера блока группы томов"

msgctxt "DEVICE_BLOCK_SIZE_NOT_SUPPORTED"
msgid ""
"Volume Group block size error, please check your Volume Group configuration, "
"Supported block size is 512 bytes."
msgstr ""
"Ошибка размера блока группы томов, проверьте параметры группы томов. "
"Поддерживаемый размер блока — 512 байтов."

msgctxt "HOST_ALREADY_EXISTS"
msgid "Cannot add Host. Host with same characteristics already exists."
msgstr ""
"Не удалось добавить хост, хост с такими характеристиками уже существует"

msgctxt "NO_ACTIVE_ISO_DOMAIN_IN_DATA_CENTER"
msgid "There is no active ISO Domain in Data Center."
msgstr "В дата-центре нет активного домена ISO"

msgctxt "PROVIDER_FAILURE"
msgid ""
"Failed to communicate with the external provider, see log for additional "
"details."
msgstr ""
"Не удалось установить связь с внешним поставщиком, подробности "
"см. в журнале."

msgctxt "PROVIDER_IMPORT_CERTIFICATE_ERROR"
msgid "Failed to import provider certificate."
msgstr "Сбой импорта сертификата поставщика"

msgctxt "PROVIDER_SSL_FAILURE"
msgid "SSL problem while trying to connect to the external provider."
msgstr "Проблема SSL при попытке подключения к внешнему поставщику"

msgctxt "PROVIDER_AUTHENTICATION_FAILURE"
msgid ""
"Failed to authenticate with the external provider. Please confirm username "
"and passowrd."
msgstr ""
"Сбой аутентификации у внешнего поставщика. Подтвердите имя пользователя "
"и пароль."

msgctxt "FAILED_UPDATE_RUNNING_VM"
msgid ""
"Failed to update VM while it is running, please try again when the VM is "
"Down."
msgstr ""
"Сбой обновления ВМ во время её выполнения, повторите попытку, когда "
"машина будет выключена."

msgctxt "VM_NOT_QUALIFIED_FOR_SNAPSHOT_MERGE"
msgid "To merge snapshots, a VM must be Down, Up or Paused."
msgstr ""
"Во время слияния снимков машина должна иметь статус "
"«не запущена», «запущена» или «приостановлена»."

msgctxt "VM_HOST_CANNOT_LIVE_MERGE"
msgid ""
"The host on which this VM is running does not support live merging snapshots."
msgstr ""
"Хост, на котором работает ВМ, не поддерживает динамическое слияние снимков."

msgctxt "MIGRATION_DEST_INVALID_HOSTNAME"
msgid "Migration destination has an invalid hostname"
msgstr "У места назначения миграции недопустимое имя хоста"

msgctxt "MIGRATION_CANCEL_ERROR"
msgid "Migration not in progress"
msgstr "Миграция не выполняется"

msgctxt "DB"
msgid "Database error."
msgstr "Ошибка базы данных"

msgctxt "MAC_POOL_NO_MACS_LEFT"
msgid "No available MAC addresses left in the MAC Address Pool."
msgstr "В пуле MAC адресов не осталось доступных адресов"

msgctxt "MAC_POOL_NOT_INITIALIZED"
msgid ""
"MAC Address Pool is not defined. Please check your configuration using "
"Configuration Tool."
msgstr ""
"Пул MAC адресов не определён. Проверьте параметры с помощью "
"инструмента конфигурации engine-config"

msgctxt "MAC_ADDRESS_IS_IN_USE"
msgid "Mac Address is in use."
msgstr "MAC адрес используется"

msgctxt "RESOURCE_MANAGER_CANT_ALLOC_VDS_MIGRATION"
msgid "Migration operation canceled, no available Destination Host."
msgstr "Миграция отменена, отсутствует целевой хост"

msgctxt "RESOURCE_MANAGER_MIGRATION_FAILED_AT_DST"
msgid "Migration failed. Probably due to an error on destination Host."
msgstr "Сбой миграции. Возможно, возникла ошибка на целевом хосте."

msgctxt "RESOURCE_MANAGER_VDS_NOT_FOUND"
msgid "Host not found."
msgstr "Хост не найден"

msgctxt "VM_TEMPLATE_CANT_LOCATE_DISKS_IN_DB"
msgid "Cannot locate the Templates' disks in the database."
msgstr "Невозможно найти диски шаблонов в базе данных"

msgctxt "VM_WITH_SAME_NAME_EXIST"
msgid "VM with the same name already exists."
msgstr "ВМ с таким именем уже существует"

msgctxt "SNAPSHOT_FAILED"
msgid "Live snapshot failed."
msgstr "Сбой динамического создания снимка."

msgctxt "ACTIVATE_NIC_FAILED"
msgid "Failed to activate VM Network Interface."
msgstr "Сбой активации сетевого интерфейса ВМ"

msgctxt "DEACTIVATE_NIC_FAILED"
msgid "Failed to deactivate VM Network Interface."
msgstr "Сбой деактивации сетевого интерфейса ВМ"

msgctxt "UPDATE_VNIC_FAILED"
msgid "Failed to update VM Network Interface."
msgstr "Сбой обновления сетевого интерфейса ВМ"

msgctxt "CINDER_ERROR"
msgid "An error occurred on Cinder - '${cinderException}'"
msgstr "Возникла ошибка Cinder — '${cinderException}'"

msgctxt "SecretBadRequestError"
msgid "Bad secret request"
msgstr "Неверный запрос пароля"

msgctxt "SecretRegisterError"
msgid "Error registering Libvirt secret"
msgstr "Ошибка регистрации пароля Libvirt"

msgctxt "SecretUnregisterError"
msgid "Error un-registering Libvirt secret"
msgstr "Ошибка удаления регистрации пароля Libvirt"

msgctxt "VMCantBeObtained"
msgid "VM can't be obtained"
msgstr "Невозможно получить ВМ"

msgctxt "GraphicsConsoleCantBeObtained"
msgid ""
"Graphics console can't be obtained. Is vm ready for connecting to console?"
msgstr ""
"Невозможно получить графическую консоль. Готова ли ВМ для подключения "
"к консоли?"

msgctxt "FailedToCreateWebsocketProxyTicket"
msgid "Failed to create signed websocket proxy ticket"
msgstr "Сбой создания подписанного билета прокси websocket"

msgctxt "DefaultIconPairNotFound"
msgid "Default icon pair not found"
msgstr "Пара иконок по умолчанию не найдена"

msgctxt "HOST_DEVICES_TAKEN_BY_OTHER_VM"
msgid "Some of the configured host devices were acquired by other VM."
msgstr "Некоторые из настроенных устройств хоста были получены другой ВМ."


