msgid "Permission denied. Engine Administrator permission is required."
msgstr "Доступ запрещен. Требуются права доступа администратора ЦУ."

msgctxt "USER_CANNOT_LOGIN_SESSION_MISSING"
msgid "Cannot Login. Session timeout."
msgstr "Невозможно выполнить вход. Время сеанса истекло."

msgctxt "USER_CANNOT_REMOVE_ADMIN_USER"
msgid "Cannot remove internal admin user or its permissions."
msgstr ""
"Невозможно удалить внутреннего администратора или его "
"права доступа."

msgctxt "USER_MUST_EXIST_IN_DB"
msgid "User must exist in Database."
msgstr "Пользователь должен существовать в базе данных."

msgctxt "USER_MUST_EXIST_IN_DIRECTORY"
msgid "User must exist in Directory."
msgstr "Пользователь должен существовать в каталоге."

msgctxt "USER_IS_ALREADY_LOGGED_IN"
msgid "User is already logged in."
msgstr "Пользователь уже выполнил вход в систему."

msgctxt "USER_IS_NOT_LOGGED_IN"
msgid "User is not logged in."
msgstr "Пользователь не выполнил вход в систему"

msgctxt "USER_DOES_NOT_HAVE_A_VALID_EMAIL"
msgid "User does not have a valid e-mail address."
msgstr "У пользователя нет действительного адреса e-mail."

msgctxt "USB_NATIVE_SUPPORT_ONLY_AVAILABLE_ON_CLUSTER_LEVEL"
msgid "Native USB support is only available on cluster level 3.1 or higher."
msgstr ""
"Встроенная поддержка USB доступна только на кластере уровня 3.1 "
"или выше."

msgctxt "USB_LEGACY_NOT_SUPPORTED_ON_LINUX_VMS"
msgid "Legacy USB support is not available on Linux VMs."
msgstr "Поддержка устаревших версий USB недоступна на Linux ВМ."

msgctxt "VM_POOL_CANNOT_CREATE_FROM_BLANK_TEMPLATE"
msgid "VM-Pool must be based on  non-blank Template."
msgstr "Пул ВМ должен быть основан не на пустом шаблоне."

msgctxt "VM_POOL_CANNOT_UPDATE_POOL_NOT_FOUND"
msgid "Cannot edit VM-Pool properties - VM-Pool not found."
msgstr "Невозможно изменить свойства пула ВМ — пул не найден."

msgctxt "VM_POOL_CANNOT_ATTACH_TO_MORE_VMS_FROM_POOL"
msgid "User is already attached to maximum number of VMs from this VM-Pool."
msgstr "Пользователь уже присоединён к максимальному числу ВМ из этого пула."

msgctxt "VM_CANNOT_SUSPENDE_HAS_RUNNING_TASKS"
msgid ""
"Cannot suspend VM. VM has asynchronous running tasks, please retry later."
msgstr ""
"Невозможно заморозить ВМ. На ВМ запущены асинхронно выполняющиеся задачи, "
"повторите попытку позже."

msgctxt "VM_CANNOT_SUSPEND_STATELESS_VM"
msgid "Cannot suspend VM, VM is stateless."
msgstr "Невозможно заморозить ВМ, ВМ выполняется без сохранения состояния."

msgctxt "VM_CANNOT_SUSPEND_VM_FROM_POOL"
msgid "Cannot suspend VM that belongs to a  VM-Pool."
msgstr "Невозможно заморозить машину, которая принадлежит пулу ВМ."

msgctxt "ACTION_TYPE_FAILED_NO_SUITABLE_DOMAIN_FOUND"
msgid ""
"Cannot ${action} ${type}. No active data Storage Domain with enough storage "
"was found in the Data Center."
msgstr ""
"Невозможно ${action} ${type}. В дата-центре не найдено доменов хранилищ "
"данных с нужным объёмом хранилища."

msgctxt "USER_NOT_AUTHORIZED_TO_PERFORM_ACTION"
msgid "User is not authorized to perform this action."
msgstr "Пользователю запрещено выполнение этого действия."

msgctxt "ERROR_CANNOT_REMOVE_ROLE_ATTACHED_TO_PERMISSION"
msgid ""
"One or more Permissions is still associated with Role.\n"
"- Please remove all Permissions first."
msgstr ""
"Одна или более настроек прав доступа всё ещё привязаны к роли.\n"
"— Удалите сначала все права доступа."

msgctxt "ERROR_CANNOT_REMOVE_ROLE_INVALID_ROLE_ID"
msgid "Cannot remove Role, invalid Role id."
msgstr "Невозможно удалить роль, недопустимый ID роли."

msgctxt "ERROR_CANNOT_UPDATE_ROLE_ID"
msgid "Cannot ${action} Role. Changing Role ID is not allowed."
msgstr "Невозможно ${action} роль. Менять ID роли запрещено."

msgctxt "ERROR_CANNOT_UPDATE_ROLE_TYPE"
msgid "Cannot ${action} Role. Changing Role type is not allowed."
msgstr "Невозможно ${action} роли. Менять тип роли запрещено."

msgctxt "VDS_APPROVE_VDS_IN_WRONG_STATUS"
msgid ""
"Cannot approve RHEV Hypervisor Host.\n"
"-Host must be in \"Pending Approval\" or \"Install Failed\" status in order "
"to be approved."
msgstr ""
"Невозможно утвердить хост гипервизора RHEV.\n"
"— Хост можно утвердить, если он имеет статус «Ожидает подтверждения» или "
"«Сбой "
"установки»."

msgctxt "VDS_CLUSTER_IS_NOT_VALID"
msgid "Cannot ${action} ${type}. Cluster ID is not valid."
msgstr "Невозможно ${action} ${type}. Недопустимый ID кластера."

msgctxt "VDS_CLUSTER_ON_DIFFERENT_STORAGE_POOL"
msgid ""
"Cannot ${action} ${type}. Target cluster belongs to different Data Center."
msgstr ""
"Невозможно ${action} ${type}. Целевой кластер принадлежит другому "
"дата-центру."

msgctxt "VDS_GROUP_CANNOT_UPDATE_CPU_ILLEGAL"
msgid ""
"Cannot change Cluster CPU type when all Hosts attached to this Cluster are "
"not in Maintenance."
msgstr ""
"Невозможно изменить тип ЦПУ кластера, если не все хосты, подключенные к этому "
"кластеру, находятся в режиме обслуживания."

msgctxt "VDS_GROUP_CANNOT_UPDATE_CPU_ARCHITECTURE_ILLEGAL"
msgid ""
"Cannot change Cluster CPU architecture when there are Hosts or VMs attached "
"to this Cluster."
msgstr ""
"Невозможно изменить архитектуру ЦПУ кластера, если присутствуют хосты/ВМ, "
"присоединённые "
"к этому кластеру."

msgctxt "VDS_GROUP_CANNOT_UPDATE_CPU_WITH_LOWER_HOSTS"
msgid ""
"Cannot change Cluster CPU to higher CPU type when there are active Hosts "
"with lower CPU type.\n"
"-Please move Hosts with lower CPU to maintenance first."
msgstr ""
"Невозможно изменить тип ЦПУ кластера на более высокий, если присутствуют "
"активные хосты с меньшим типом ЦПУ.\n"
"— Сначала переведите хосты с меньшим типом ЦПУ в режим обслуживания."

msgctxt "VDS_GROUP_CANNOT_UPDATE_COMPATIBILITY_VERSION_WITH_LOWER_HOSTS"
msgid ""
"Cannot change Cluster Compatibility Version to higher version when there are "
"active Hosts with lower version.\n"
"-Please move Hosts with lower version to maintenance first."
msgstr ""
"Невозможно изменить версию совместимости кластера на более высокую, если "
"присутствуют "
"активные хосты с меньшей версией.\n"
"— Сначала переведите хосты с меньшей версией в режим обслуживания."

msgctxt "VDS_GROUP_CANNOT_UPDATE_SUPPORTED_FEATURES_WITH_LOWER_HOSTS"
msgid ""
"Cannot enable new Cluster feature when there are active hosts which don't "
"support the selected feature.\n"
"-Please move hosts with lower version to maintenance first."
msgstr ""
"Невозможно включить новую возможность кластера, если есть активные хосты, "
"не поддерживающие эту возможность.\n"
"— Сначала переведите хосты с низшей версией в режим обслуживания."

msgctxt "VDS_GROUP_CANNOT_UPDATE_VDS_UP"
msgid ""
"Cannot change Cluster.Trying to connect Cluster to Data Center with Hosts "
"that are up."
msgstr ""
"Невозможно сменить кластер. Попробуйте подключить кластер к дата-центру "
"с хостами в работоспособном состоянии."

msgctxt "VDS_GROUP_CANNOT_ADD_COMPATIBILITY_VERSION_WITH_LOWER_STORAGE_POOL"
msgid ""
"Cannot add Cluster with Compatibility Version that is lower than the Data "
"Center Compatibility Version.\n"
"-Please upgrade your Cluster to a later Compatibility version first."
msgstr ""
"Невозможно добавить кластер с версией совместимости ниже, чем версия "
"совместимости дата-центра.\n"
"— Сначала обновите кластер до последней версии совместимости."

msgctxt "VDS_GROUP_CPU_TYPE_CANNOT_BE_NULL"
msgid "Cannot add Cluster. CPU type must be specified."
msgstr "Невозможно добавить кластер. Должен быть указан тип ЦПУ."

msgctxt "VDS_GROUP_CANNOT_DO_ACTION_NAME_IN_USE"
msgid "Cannot ${action} Cluster. Cluster name is already in use."
msgstr "Невозможно ${action} кластер. Имя кластера уже используется."

msgctxt "VDS_GROUP_HOSTS_MUST_BE_DOWN"
msgid ""
"Cannot update Cluster. Clusters hosts must be down in order to perform this "
"action."
msgstr ""
"Невозможно обновить кластер. Для выполнения этой операции хосты кластера "
"должны быть отключены."

msgctxt "ACTION_TYPE_FAILED_VNIC_PROFILE_NOT_EXISTS"
msgid ""
"Cannot ${action} ${type}. The specified VM network interface profile doesn't "
"exist."
msgstr ""
"Невозможно ${action} ${type}. Указанный профиль сетевого интерфейса ВМ не "
"существует."

msgctxt "ACTION_TYPE_FAILED_VNIC_PROFILE_NAME_IN_USE"
msgid ""
"Cannot ${action} ${type}. The VM network interface profile's name is already "
"used by an existing profile for the same network.\n"
"-Please choose a different name."
msgstr ""
"Невозможно ${action} ${type}. Имя профиля сетевого интерфейса ВМ уже "
"используется существующим профилем в той же сети.\n"
"— Выберите другое имя."

msgctxt "ACTION_TYPE_FAILED_VNIC_PROFILE_IN_ONE_USE"
msgid ""
"Cannot ${action} ${type}. This VM network interface profile is used by "
"${entities}:  ${ENTITIES_USING_VNIC_PROFILE}.\n"
" - Please remove it from this ${entities} and try again."
msgstr ""
"Невозможно ${action} ${type}. Этот сетевой профиль ВМ используют "
"${entities}:  ${ENTITIES_USING_VNIC_PROFILE}.\n"
" — Удалите его из ${entities} и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_VNIC_PROFILE_IN_MANY_USES"
msgid ""
"Cannot ${action} ${type}. This VM network interface profile is used by "
"${entities} (${ENTITIES_USING_VNIC_PROFILE_COUNTER}): "
"${ENTITIES_USING_VNIC_PROFILE}\n"
" - Please detach ${entities} using this profile and try again."
msgstr ""
"Невозможно ${action} ${type}. Этот сетевой профиль ВМ используют "
"${entities} (${ENTITIES_USING_VNIC_PROFILE_COUNTER}): "
"${ENTITIES_USING_VNIC_PROFILE}\n"
" — Отсоедините ${entities}, использующие этот профиль, и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_CANNOT_CHANGE_VNIC_PROFILE_NETWORK"
msgid ""
"Cannot ${action} ${type}. VM network interface profile's network cannot be "
"changed."
msgstr ""
"Невозможно ${action} ${type}. Профиль сетевого интерфейса ВМ не может быть "
"изменен."

msgctxt "ACTION_TYPE_FAILED_CANNOT_ADD_VNIC_PROFILE_TO_NON_VM_NETWORK"
msgid ""
"Cannot ${action} ${type}. VM network interface profiles cannot be added to a "
"non-VM network. Please make sure the network is a VM network."
msgstr ""
"Невозможно ${action} ${type}. Профили сетевого интерфейса ВМ "
"нельзя добавить не к ВМ сети. Убедитесь, что сеть является ВМ сетью."

msgctxt "ACTION_TYPE_FAILED_CANNOT_FIND_VNIC_PROFILE_FOR_NETWORK"
msgid ""
"Cannot ${action} ${type}. There is no VM network interface profile for the "
"network the user can use.\n"
"- Please use a VM network interface profile instead of a network name."
msgstr ""
"Невозможно ${action} ${type}. Не существует профиля сетевого интерфейса сети "
"ВМ "
"для той сети, которую может использовать пользователь.\n"
"— Используйте профиль сетевого интерфейса ВМ вместо имени сети."

msgctxt "ACTION_TYPE_FAILED_NAME_ALREADY_USED"
msgid ""
"Cannot ${action} ${type}. The ${type} name is already in use, please choose "
"a unique name and try again."
msgstr ""
"Невозможно ${action} ${type}.  Имя ${type} уже используется, выберите "
"уникальное "
"имя и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_NOT_EXIST"
msgid "Cannot ${action} ${type}. Storage connection doesn't exist."
msgstr "Невозможно ${action} ${type}. Подключение к хранилищу не существует."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_FOR_DOMAIN_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. Storage connection is not attached to the "
"specified domain."
msgstr ""
"Невозможно ${action} ${type}. Подключение к хранилищу не присоединено к "
"указанному домену."

msgctxt "ACTION_TYPE_FAILED_ACTION_IS_SUPPORTED_ONLY_FOR_ISCSI_DOMAINS"
msgid ""
"Cannot ${action} ${type}. Action is supported only for iSCSI storage domains."
msgstr ""
"Невозможно ${action} ${type}. Действие поддерживается только для доменов "
"хранилища iSCSI."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_ID_EMPTY"
msgid "Cannot ${action} ${type}. Storage connection id is empty."
msgstr "Невозможно ${action} ${type}. Пустой ID подключения к хранилищу."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_ID_NOT_EMPTY"
msgid "Cannot ${action} ${type}. Storage connection id is not empty."
msgstr "Невозможно ${action} ${type}. ID подключения к хранилищу не пустой."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_ALREADY_EXISTS"
msgid "Cannot ${action} ${type}. Storage connection already exists."
msgstr "Невозможно ${action} ${type}. Подключение к хранилищу уже существует."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_FOR_DOMAIN_ALREADY_EXISTS"
msgid ""
"Cannot ${action} ${type}. Storage connection is already attached to the "
"specified domain."
msgstr ""
"Невозможно ${action} ${type}. Подключение к хранилищу уже присоединено к "
"указанному домену."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_CONNECTION_UNSUPPORTED_ACTION_FOR_STORAGE_TYPE"
msgid ""
"Cannot ${action} ${type}. Storage connection parameters can be edited only "
"for NFS, Posix, local or iSCSI data domains."
msgstr ""
"Невозможно ${action} ${type}. Параметры подключения к хранилищу могут быть "
"изменены только для доменов данных NFS, Posix, iSCSI или локальных."

msgctxt ""
"ACTION_TYPE_FAILED_UNSUPPORTED_ACTION_DOMAIN_MUST_BE_IN_MAINTENANCE_OR_UNATTAC"
"HED"
msgid ""
"Cannot ${action} ${type}. The data domains ${domainNames} should be in "
"maintenance or unattached."
msgstr ""
"Невозможно ${action} ${type}. Домены данных ${domainNames} должны находиться "
"в "
"режиме обслуживания или быть отсоединёнными."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_CONNECTION_UNSUPPORTED_ACTION_NOT_SAME_STORAGE_TYPE"
msgid ""
"Cannot ${action} ${type}. Storage connection and domain type don't match."
msgstr ""
"Невозможно ${action} ${type}. Тип подключения к хранилищу и тип домена "
"не совпадают."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_CONNECTION_UNSUPPORTED_ACTION_FOR_RUNNING_VMS"
msgid "Cannot ${action} ${type}. VMs ${vmNames} should be down."
msgstr "Невозможно ${action} ${type}. ВМ ${vmNames} должны быть отключены."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_CONNECTION_UNSUPPORTED_ACTION_FOR_RUNNING_VMS_AND_D"
"OMAINS_STATUS"
msgid ""
"Cannot ${action} ${type}. The data domains ${domainNames} should be in "
"maintenance or unattached, and VMs ${vmNames} should be down."
msgstr ""
"Невозможно ${action} ${type}. Домены данных ${domainNames} должны находиться "
"в режиме обслуживания или быть отсоединёнными, а машины ${vmNames} должны "
"быть отключены."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_CONNECTION_BELONGS_TO_SEVERAL_STORAGE_DOMAINS"
msgid ""
"Cannot ${action} ${type}. Storage connection parameters are used by the "
"following storage domains : ${domainNames}."
msgstr ""
"Невозможно ${action} ${type}. Параметры подключения к хранилищу используются "
"следующими доменами хранилищ: ${domainNames}."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_CONNECTION_BELONGS_TO_SEVERAL_STORAGE_DOMAINS_AND_D"
"ISKS"
msgid ""
"Cannot ${action} ${type}. Storage connection parameters are used by the "
"following storage domains : ${domainNames} and disks: ${diskNames}."
msgstr ""
"Невозможно ${action} ${type}. Параметры подключения к хранилищу используются "
"следующими доменами хранилищ: ${domainNames} и дисками: ${diskNames}."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_BELONGS_TO_SEVERAL_DISKS"
msgid ""
"Cannot ${action} ${type}. Storage connection parameters are used by the "
"following disks : ${diskNames}."
msgstr ""
"Невозможно ${action} ${type}. Параметры подключения к хранилищу используются "
"следующими дисками: ${diskNames}."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_UNSUPPORTED_CHANGE_STORAGE_TYPE"
msgid "Cannot ${action} ${type}. Storage type cannot be edited."
msgstr "Невозможно ${action} ${type}. Тип хранилища нельзя изменить."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_EXTENSION_ALREADY_EXISTS"
msgid ""
"Cannot ${action} ${type}. Connection extension already exists for the host "
"${vdsName} and the target ${target}."
msgstr ""
"Невозможно ${action} ${type}. Расширение соединения уже существует для хоста "
"${vdsName} и цели ${target}."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTION_EXTENSION_DOES_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. Connection extension with the id ${connExtId} does "
"not exist."
msgstr ""
"Невозможно ${action} ${type}. Расширение подключения с ID ${connExtId} не "
"существует."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_NOT_EXIST"
msgid "Cannot ${action} ${type}. Storage Domain doesn't exist."
msgstr "Невозможно ${action} ${type}. Домен хранилища не существует."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_AND_CLUSTER_IN_DIFFERENT_POOL"
msgid ""
"Cannot ${action} ${type}. Storage Domain and Cluster must be in the same "
"Storage Pool."
msgstr ""
"Невозможно ${action} ${type}. Домен хранилища и кластер должны принадлежать "
"одному пулу хранилища."

msgctxt "ACTION_TYPE_FAILED_VDS_NOT_IN_DEST_STORAGE_POOL"
msgid ""
"Cannot ${action} ${type}. The selected proxy Host must be in the destination "
"Storage Pool."
msgstr ""
"Невозможно ${action} ${type}. Выбранный хост прокси должен находиться в "
"целевом "
"пуле хранилища."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_STATUS_ILLEGAL"
msgid ""
"Cannot ${action} ${type}. The relevant Storage Domain is inaccessible.\n"
"-Please handle Storage Domain issues and retry the operation."
msgstr ""
"Невозможно ${action} ${type}. Релевантный домен хранилища недоступен.\n"
"— Решите проблему с доменом хранилища и повторите операцию."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_STATUS_ILLEGAL2"
msgid ""
"Cannot ${action} ${type}. The relevant Storage Domain's status is ${status}."
msgstr ""
"Невозможно ${action} ${type}. Релевантный домен имеет статус ${status}."

msgctxt "STORAGE_POOL_REINITIALIZE_WITH_MORE_THAN_ONE_DATA_DOMAIN"
msgid ""
"Cannot ${action} ${type}.\n"
"The ${action} action can be performed on a Data Center that has only one "
"Storage Domain in Active/Unknown state."
msgstr ""
"Невозможно ${action} ${type}.\n"
"Действие ${action} может быть выполнено в дата-центре только с "
"одним доменом хранилища со статусом «активен»/«неизвестно»."

msgctxt "ACTION_TYPE_FAILED_STORAGE_POOL_NOT_EXIST"
msgid "Data Center doesn't exist."
msgstr "Дата-центр не существует."

msgctxt "ACTION_TYPE_FAILED_FORMAT_STORAGE_DOMAIN_WITH_ATTACHED_DATA_DOMAIN"
msgid ""
"Cannot ${action} ${type}.\n"
" The Storage Domain metadata indicates it is already attached to a Data "
"Center hence cannot be formatted.\n"
" To remove the Storage Domain one should either remove it without the format "
"option or attach it to an existing Data Center, detach it, and try again."
msgstr ""
"Невозможно ${action} ${type}.\n"
" Метаданные домена хранилища показывают, что он уже присоединён к дата-центру "
"и поэтому не может быть отформатирован.\n"
" Чтобы удалить домен хранилища: либо удалите его без параметра "
"форматирования, "
"либо присоедините его к существующему дата-центру, затем отсоедините и "
"повторите попытку"

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_NOT_IN_STORAGE_POOL"
msgid ""
"Cannot ${action} ${type}. The selected Storage Domain is not part of the "
"Data Center."
msgstr ""
"Невозможно ${action} ${type}. Выбранный домен хранилища не является частью "
"дата-центра."

msgctxt "ACTION_TYPE_FAILED_LUNS_ALREADY_PART_OF_STORAGE_DOMAINS"
msgid ""
"Cannot ${action} ${type}. The following LUNs are already part of existing "
"storage domains: ${lunIds}."
msgstr ""
"Невозможно ${action} ${type}. Следующие LUN уже являются частью "
"существующих доменов хранилища: ${lunIds}."

msgctxt "ACTION_TYPE_FAILED_LUNS_ALREADY_USED_BY_DISKS"
msgid ""
"Cannot ${action} ${type}. The following LUNs are already used by DirectLUN "
"disks: ${lunIds}."
msgstr ""
"Невозможно ${action} ${type}. Следующие LUN уже используются дисками "
"DirectLUN: ${lunIds}."

msgctxt "ERROR_CANNOT_REMOVE_STORAGE_POOL_WITH_NONMASTER_DOMAINS"
msgid ""
"Cannot remove Data Center when there are more than one Storage Domain "
"attached."
msgstr ""
"Невозможно удалить дата-центр, если к нему присоединено больше одного "
"домена хранилища."

msgctxt "ERROR_CANNOT_MANAGE_STORAGE_DOMAIN"
msgid ""
"Cannot manage Storage Domain. The domain is defined externally (such as "
"through a provider)."
msgstr ""
"Домен хранилища недоступен для управления. Домен определён извне "
"(например, через поставщика)."

msgctxt "ERROR_CANNOT_FORCE_REMOVE_STORAGE_POOL_WITH_VDS_NOT_IN_MAINTENANCE"
msgid ""
"Cannot ${action} ${type} while there are Hosts that are not in Maintenance "
"mode."
msgstr ""
"Невозможно ${action} ${type} пока есть хосты, находящиеся не "
"в режиме обслуживания."

msgctxt "ACTION_TYPE_FAILED_STORAGE_POOL_STATUS_ILLEGAL"
msgid "Cannot ${action} ${type}. Unknown Data Center status."
msgstr "Невозможно ${action} ${type}. Статус дата-центра неизвестен."

msgctxt "ACTION_TYPE_FAILED_MIXED_STORAGE_TYPES_NOT_ALLOWED"
msgid ""
"Cannot ${action} ${type}. Data Center version does not support mixed storage "
"types."
msgstr ""
"Невозможно ${action} ${type}. Дата-центр не поддерживает смешанные "
"типы хранилищ."

msgctxt "NETWORK_BOND_NAME_EXISTS"
msgid "Bond name already exists."
msgstr "Имя связи уже существует."

msgctxt "NETWORK_BOND_NOT_EXISTS"
msgid "Bond name doesn't exist."
msgstr "Имя связи не существует."

msgctxt "NETWORK_CHECK_CONNECTIVITY"
msgid "Cannot check connectivity on non management network."
msgstr ""
"Невозможно проверить возможность подключения в не-административной сети."

msgctxt "NETWORK_BOND_PARAMETERS_INVALID"
msgid ""
"Invalid parameters.\n"
"-Please check that bond name is formatted as <bondYYY>.\n"
"-Bond consist of at least two network interfaces(NICs)."
msgstr ""
"Неверные параметры.\n"
"— Имя связи должно быть в формате <bondYYY>.\n"
"— Связь должна состоять минимум из двух сетевых интерфейсов (NIC)."

msgctxt "NETWORK_DEFAULT_UPDATE_NAME_INVALID"
msgid "Network '${NetworkName}' is mandatory, it cannot be modified."
msgstr "Сеть «${NetworkName}» является обязательной и не может быть изменена."

msgctxt "NETWORK_INTERFACE_ALREADY_IN_BOND"
msgid "Network Interface already belongs to the bond."
msgstr "Сетевой интерфейс уже включен в связь."

msgctxt ""
"ACTION_TYPE_FAILED_NETWORK_ATTACHMENTS_REFERENCES_SAME_NETWORK_DUPLICATELY"
msgid ""
"Cannot ${action} ${type}. Multiple network attachments "
"(${ACTION_TYPE_FAILED_NETWORK_ATTACHMENTS_REFERENCES_SAME_NETWORK_DUPLICATELY_"
"LIST}) "
"references same network "
"(${ACTION_TYPE_FAILED_NETWORK_ATTACHMENTS_REFERENCES_SAME_NETWORK_DUPLICATELY_"
"ENTITY})."
msgstr ""
"Невозможно ${action} ${type}. Несколько подключений к сети "
"(${ACTION_TYPE_FAILED_NETWORK_ATTACHMENTS_REFERENCES_SAME_NETWORK_DUPLICATELY_"
"LIST}) "
"ссылаются одну и ту же сеть "
"(${ACTION_TYPE_FAILED_NETWORK_ATTACHMENTS_REFERENCES_SAME_NETWORK_DUPLICATELY_"
"ENTITY})."

msgctxt "NETWORK_ATTACHMENT_IN_BOTH_LISTS"
msgid ""
"Cannot ${action} ${type}. Cannot remove and update a Network Attachment "
"simultaneously. The same network attachment "
"('${NETWORK_ATTACHMENT_IN_BOTH_LISTS_ENTITY}') appears in both 'network "
"attachments' and 'removed network attachments' lists."
msgstr ""
"Невозможно ${action} ${type}. Нельзя одновременно удалять и обновлять "
"подключение к сети. "
"Одно и то же подключение к сети («${NETWORK_ATTACHMENT_IN_BOTH_LISTS_ENTITY}»)"
" присутствует "
"как в списке «подключения к сети», так и в списке «удалённые подключения к "
"сети»."

msgctxt "NETWORK_INTERFACE_REFERENCED_AS_A_SLAVE_MULTIPLE_TIMES"
msgid ""
"Cannot ${action} ${type}. Network interface "
"'${NETWORK_INTERFACE_REFERENCED_AS_A_SLAVE_MULTIPLE_TIMES_ENTITY}' is used "
"multiple times in new or modified bonds in this request. Slave can be "
"neither shared by multiple bonds nor used multiple times in one bond."
msgstr ""
"Невозможно ${action} ${type}. В этом запросе сетевой интерфейс "
"«${NETWORK_INTERFACE_REFERENCED_AS_A_SLAVE_MULTIPLE_TIMES_ENTITY}» "
"неоднократно "
"используется в новых или изменённых связях. Подчинённый интерфейс не может "
"быть ни "
"общим для нескольких связей, ни использоваться несколько раз в одной связи."

msgctxt "NIC_NOT_EXISTS_ON_HOST"
msgid ""
"Cannot ${action} ${type}. The given network interface does not exist on "
"specified host '${NIC_NOT_EXISTS_ON_HOST_ENTITY}'."
msgstr ""
"Невозможно ${action} ${type}. Указанный сетевой интерфейс не существует на "
"указанном хосте «${NIC_NOT_EXISTS_ON_HOST_ENTITY}»."

msgctxt "NETWORK_INTERFACE_BOND_OR_VLAN_CANNOT_BE_SLAVE"
msgid ""
"Cannot ${action} ${type}. Network Interface '${interfaceName}' cannot be "
"slave: Neither bond nor vlan can be slave."
msgstr ""
"Невозможно ${action} ${type}. Сетевой интерфейс «${interfaceName}» не может "
"быть подчиненным: ни BOND, ни VLAN не могут быть подчинёнными."

msgctxt "CANNOT_ADD_NETWORK_ATTACHMENT_ON_SLAVE_OR_VLAN"
msgid ""
"Cannot ${action} ${type}. Cannot create Network Attachment directly on slave "
"or vlan interface. Network name = '${networkName}', Network interface name = "
"'${nicName}'."
msgstr ""
"Невозможно ${action} ${type}. Нельзя создать подключение к сети напрямую на "
"подчинённом "
"или на vlan интерфейсе. Имя сети = «${networkName}», имя сетевого интерфейса "
"= «${nicName}»."

msgctxt "NETWORK_ATTACHMENT_NOT_SPECIFIED"
msgid ""
"Cannot ${action} ${type}. Network attachment must be specified for this "
"request."
msgstr ""
"Невозможно ${action} ${type}. В этом запросе должно быть указано "
"подключение к сети."

msgctxt "NIC_LABEL_REFERENCES_NICS_INCOHERENTLY"
msgid ""
"Cannot ${action} ${type}. NicLabel (${referrerId}) does not reference nic "
"coherently. Nic name and id reference different nics: nicId ${referringId} "
"nicName ${referringName}."
msgstr ""
"Невозможно ${action} ${type}. NicLabel (${referrerId}) не ссылается на nic "
"когерентно. Имя nic и ID ссылаются на разные nic: nicId ${referringId} "
"nicName ${referringName}."

msgctxt "NETWORK_ATTACHMENT_REFERENCES_NETWORK_INCOHERENTLY"
msgid ""
"Cannot ${action} ${type}. NetworkAttachment (id ${referrerId}) does not "
"reference network coherently. Network name and id reference different "
"networks: networkId ${referringId} networkName ${referringName}."
msgstr ""
"Невозможно ${action} ${type}. NetworkAttachment (id ${referrerId}) не "
"ссылается "
"на сеть когерентно. Имя сети и ID ссылаются на разные сети: "
"networkId ${referringId} networkName ${referringName}."

msgctxt "BOND_REFERENCES_NICS_INCOHERENTLY"
msgid ""
"Cannot ${action} ${type}. Bond (${referrerId}) does not reference nic "
"coherently. Nic name and id reference different nics: nicId ${referringId} "
"nicName ${referringName}."
msgstr ""
"Невозможно ${action} ${type}. Связь (${referrerId}) не ссылается на nic "
"когерентно. Имя nic и ID ссылаются на разные nic: nicId ${referringId} "
"nicName ${referringName}."

msgctxt ""
"NETWORK_INTERFACE_ADDED_TO_BOND_AND_NETWORK_IS_ATTACHED_TO_IT_AT_THE_SAME_TIME"
msgid ""
"Cannot ${action} ${type}. Network Interface "
"'$NETWORK_INTERFACE_ADDED_TO_BOND_AND_NETWORK_IS_ATTACHED_TO_IT_AT_THE_SAME_TI"
"ME_ENTITY' "
"cannot become slave, because you're also attaching network '${networkName}' "
"to it."
msgstr ""
"Невозможно ${action} ${type}. Сетевой интерфейс "
"«$NETWORK_INTERFACE_ADDED_TO_BOND_AND_NETWORK_IS_ATTACHED_TO_IT_AT_THE_SAME_TI"
"ME_ENTITY» "
"не может стать подчинённым, поскольку к нему также присоединяется сеть "
"«'${networkName}»."

msgctxt "NETWORK_ATTACHMENT_WHEN_UPDATING_YOU_HAVE_TO_PROVIDE_ID"
msgid ""
"Cannot ${action} ${type}. When updating Network attachment non null ID has "
"to be provided."
msgstr ""
"Невозможно ${action} ${type}. При обновлении подключения к сети нужно указать "
"не нулевой ID."

msgctxt "NETWORK_ATTACHMENT_CANNOT_BE_CREATED_WITH_SPECIFIC_ID"
msgid ""
"Cannot ${action} ${type}. New Network attachment cannot be created with "
"given ID. Please remove Network Attachment ID (id "
"'${NETWORK_ATTACHMENT_CANNOT_BE_CREATED_WITH_SPECIFIC_ID_ENTITY}') from "
"request."
msgstr ""
"Невозможно ${action} ${type}. С указанными ID нельзя создать новое "
"подключение к сети. "
"Удалите из запроса ID подключения к сети "
"(«${NETWORK_ATTACHMENT_CANNOT_BE_CREATED_WITH_SPECIFIC_ID_ENTITY}»)."

msgctxt "NETWORK_INTERFACE_ATTACHED_TO_NETWORK_CANNOT_BE_SLAVE"
msgid ""
"Cannot ${action} ${type}. Network Interface "
"'${NETWORK_INTERFACE_ATTACHED_TO_NETWORK_CANNOT_BE_SLAVE_ENTITY}' cannot "
"become slave, there's network '${networkName}' attached to it."
msgstr ""
"Невозможно ${action} ${type}. Сетевой интерфейс "
"«${NETWORK_INTERFACE_ATTACHED_TO_NETWORK_CANNOT_BE_SLAVE_ENTITY}» не может "
"стать подчинённым, к нему присоединена сеть «${networkName}»."

msgctxt "BOND_USED_BY_NETWORK_ATTACHMENTS"
msgid ""
"Cannot remove bond ${BondName}, because it's used by Network Attachments: "
"${attachmentIds}."
msgstr ""
"Невозможно удалить связь ${BondName}, т.к. она используется подключениями к "
"сети ${attachmentIds}."

msgctxt "NETWORK_ATTACHMENT_NOT_EXISTS"
msgid ""
"Cannot ${action} ${type}. Given Network Attachment (id "
"'${NETWORK_ATTACHMENT_NOT_EXISTS_ENTITY}') does not exist."
msgstr ""
"Невозможно ${action} ${type}. Данное подключение к сети "
"(id «${NETWORK_ATTACHMENT_NOT_EXISTS_ENTITY}») не существует."

msgctxt "NETWORK_ATTACHMENT_NETWORK_ID_IS_NOT_SET"
msgid ""
"Cannot ${action} ${type}. There's unset networkId among given network "
"attachments."
msgstr ""
"Невозможно ${action} ${type}. Среди указанных подключений к сети есть "
"ненастроенный "
"networkId."

msgctxt "NETWORK_ATTACHMENTS_NOT_EXISTS"
msgid ""
"Cannot ${action} ${type}. Following Network Attachments do not exist: "
"${NETWORK_ATTACHMENTS_NOT_EXISTS_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Следующие подключения к сети не существуют: "
"${NETWORK_ATTACHMENTS_NOT_EXISTS_LIST}."

msgctxt "NETWORK_ALREADY_ATTACHED_TO_HOST"
msgid ""
"Cannot ${action} ${type}. Given network '${networkName}' is already attached "
"to given host '${hostName}'."
msgstr ""
"Невозможно ${action} ${type}. Указанная сеть «${networkName}» уже закреплена "
"за хостом «${hostName}»."

msgctxt "CANNOT_CHANGE_ATTACHED_NETWORK"
msgid ""
"Cannot ${action} ${type}. Network of given Network Attachment (id "
"${networkAttachmentID}) cannot be updated."
msgstr ""
"Невозможно ${action} ${type}. Сеть указанного подключения к сети "
"(id ${networkAttachmentID}) нельзя обновить."

msgctxt "ACTION_TYPE_FAILED_ROLE_NETWORK_HAS_NO_BOOT_PROTOCOL"
msgid ""
"Cannot ${action} ${type}. Role (migration/display/gluster) network "
"'${ACTION_TYPE_FAILED_ROLE_NETWORK_HAS_NO_BOOT_PROTOCOL_ENTITY}' hasn't boot "
"protocol assigned."
msgstr ""
"Невозможно ${action} ${type}. Сети роли (миграция/монитор/gluster) "
"«${ACTION_TYPE_FAILED_ROLE_NETWORK_HAS_NO_BOOT_PROTOCOL_ENTITY}» "
"не назначен протокол."

msgctxt "INTERFACE_ON_NIC_LABEL_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. Label '${INTERFACE_ON_NIC_LABEL_NOT_EXIST_ENTITY}' "
"cannot be attached to non-existing/removed '${interfaceName}' network "
"interface."
msgstr ""
"Невозможно ${action} ${type}. Метка «${INTERFACE_ON_NIC_LABEL_NOT_EXIST_ENTITY"
"}» "
"не может быть назначена несуществующему/удалённому сетевому интерфейсу "
"«${interfaceName}»."

msgctxt "LABEL_NOT_EXIST_IN_HOST"
msgid ""
"Cannot ${action} ${type}. Label '${LABEL_NOT_EXIST_IN_HOST_ENTITY}' cannot "
"be removed, since it isn't attached to any network interface."
msgstr ""
"Невозможно ${action} ${type}. Метка «${LABEL_NOT_EXIST_IN_HOST_ENTITY}» не "
"может быть удалена, т.к. не назначена никакому сетевому интерфейсу."

msgctxt "PARAMS_CONTAIN_DUPLICATE_LABELS"
msgid ""
"Cannot ${action} ${type}. Labels '${PARAMS_CONTAIN_DUPLICATE_LABELS_LIST}' "
"appear in the 'labels/removed labels' lists more than once."
msgstr ""
"Невозможно ${action} ${type}. Метки «${PARAMS_CONTAIN_DUPLICATE_LABELS_LIST}» "
"присутствуют в списках «метки/удалённые метки» более одного раза."

msgctxt "LABEL_ATTACH_TO_IMPROPER_INTERFACE"
msgid ""
"Cannot ${action} ${type}. Network interface "
"'${LABEL_ATTACH_TO_IMPROPER_INTERFACE_ENTITY}' cannot be labeled. Label can "
"be provided only to interfaces or to bonds (not to slaves nor vlans)."
msgstr ""
"Невозможно ${action} ${type}. Сетевому интерфейсу "
"«${LABEL_ATTACH_TO_IMPROPER_INTERFACE_ENTITY}» нельзя назначить метку. "
"Метку можно назначать только интерфейсам и связям (но не подчинённым "
"интерфейсам или vlan)."

msgctxt "NETWORK_ATTACHMENT_REFERENCES_NICS_INCOHERENTLY"
msgid ""
"Cannot ${action} ${type}. NetworkAttachment (id ${ENTITY_ID}) does not "
"reference nic coherently. Nic name and id references different nic: nicId "
"${referringId} nicName ${referringName}."
msgstr ""
"Невозможно ${action} ${type}. NetworkAttachment (id ${ENTITY_ID}) не "
"ссылается на nic "
"когерентно. Имя nic и ID ссылаются на разные nic: nicId ${referringId} "
"nicName ${referringName}."

msgctxt "NETWORK_INTERFACE_NOT_EXISTS"
msgid "Network name doesn't exist."
msgstr "Имя сети не существует."

msgctxt "ERROR_CANNOT_ATTACH_MORE_THAN_ONE_ISO_DOMAIN"
msgid ""
"Cannot attach more than one ISO Storage Domain to the same Data Center. If "
"you want to use a newly created Domain, detach the existing attached Domain "
"and attach the new one."
msgstr ""
"Невозможно присоединить больше одного домена хранилища ISO к одному и тому же "
"дата-центру. "
"При необходимости использовать только что созданный домен, отсоедините "
"существующий присоединённый "
"домен и присоедините новый."

msgctxt "ERROR_CANNOT_ATTACH_MORE_THAN_ONE_EXPORT_DOMAIN"
msgid ""
"Cannot attach more than one Import/Export Storage Domain to the same Data "
"Center. If you want to use a newly created Domain, detach the existing "
"attached Domain and attach the new one."
msgstr ""
"Невозможно присоединить больше одного импорт/экспорт домена хранилища "
"к одному и тому же дата-центру. При необходимости использовать только что "
"созданный "
"домен, отсоедините существующий присоединённый домен и присоедините новый."

msgctxt "ERROR_CANNOT_ATTACH_STORAGE_DOMAIN_STORAGE_TYPE_NOT_MATCH"
msgid ""
"Cannot attach storage to Data Center. Storage type doesn't match the Data "
"Center type."
msgstr ""
"Невозможно присоединить хранилище к дата-центру. Тип хранилища не совпадает "
"с типом дата-центра."

msgctxt "ERROR_CANNOT_CHANGE_STORAGE_POOL_TYPE_WITH_DOMAINS"
msgid ""
"Cannot change Data Center type when Storage Domains are still attached to it."
"\n"
"-Please detach all attached Storage Domains and retry."
msgstr ""
"Невозможно сменить тип дата-центра, если к нему присоединены домены "
"хранилища.\n"
"— Отсоедините все домены хранилищ и повторите попытку."

msgctxt "NETWORK_INTERFACE_ALREADY_HAVE_NETWORK"
msgid "The Network Interface is already attached to a Logical Network."
msgstr "Сетевой интерфейс уже присоединён к логической сети."

msgctxt "NETWORK_NOT_EXISTS"
msgid "The specified Logical Network doesn't exist."
msgstr "Указанная логическая сеть не существует."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_NOT_EXISTS"
msgid "The specified Network QoS doesn't exist."
msgstr "Указанного QoS сети не существует"

msgctxt "ACTION_TYPE_FAILED_NETWORK_ATTACHMENT_CONTAINS_DUPLICATES"
msgid "Provided NetworkAttachments contains duplicates."
msgstr "Предоставленные NetworkAttachments содержат дубликаты"

msgctxt "EXTERNAL_NETWORK_CANNOT_BE_PROVISIONED"
msgid ""
"The specified external network cannot be configured on the host's interface."
msgstr "Указанная внешняя сеть не может быть настроена как интерфейс хоста."

msgctxt "NETWORK_LABEL_FORMAT_INVALID"
msgid ""
"Network label must be formed only from: English letters, numbers, hyphen or "
"underscore."
msgstr ""
"Метка сети может состоять только из букв латинского алфавита, чисел, "
"дефисов или знаков нижнего подчёркивания."

msgctxt "ACTION_TYPE_FAILED_NETWORK_ALREADY_LABELED"
msgid "Cannot ${action} ${type}. The specified network is already labeled."
msgstr "Невозможно ${action} ${type}. Указанная сеть уже имеет метку."

msgctxt "ACTION_TYPE_FAILED_CANNOT_REMOVE_LABELED_NETWORK_FROM_NIC"
msgid ""
"Cannot ${action} ${type}. The following networks cannot be removed from the "
"network interface since they are managed by the label: "
"${ACTION_TYPE_FAILED_CANNOT_REMOVE_LABELED_NETWORK_FROM_NIC_ENTITY}. Please "
"remove the label from the network interface in order to remove the network."
msgstr ""
"Невозможно ${action} ${type}. Следующие сети нельзя удалить из сетевого "
"интерфейса, "
"потому что они управляются меткой: "
"${ACTION_TYPE_FAILED_CANNOT_REMOVE_LABELED_NETWORK_FROM_NIC_ENTITY}. Чтобы "
"удалить сеть, удалите метку с сетевого интерфейса."

msgctxt "ACTION_TYPE_FAILED_CANNOT_MOVE_LABELED_NETWORK_TO_ANOTHER_NIC"
msgid ""
"Cannot ${action} ${type}. Network '${networkName}' cannot be moved to "
"another network interface since it's managed by label "
"'${ACTION_TYPE_FAILED_CANNOT_MOVE_LABELED_NETWORK_TO_ANOTHER_NIC_ENTITY}'. "
"Please remove the label from the network interface in order to remove the "
"network."
msgstr ""
"Невозможно ${action} ${type}. Сеть «${networkName}» нельзя перенести на "
"другой "
"сетевой интерфейс, поскольку она управляется меткой "
"«${ACTION_TYPE_FAILED_CANNOT_MOVE_LABELED_NETWORK_TO_ANOTHER_NIC_ENTITY}». "
"Чтобы перенести сеть, удалите метку с сетевого интерфейса."

msgctxt "NETWORK_SHOULD_BE_ATTACHED_VIA_LABEL_TO_ANOTHER_NIC"
msgid ""
"Cannot ${action} ${type}. Cannot attach network "
"'${NETWORK_SHOULD_BE_ATTACHED_VIA_LABEL_TO_ANOTHER_NIC_ENTITY}' to network "
"interface '${interfaceName}', it should be attached via label to network "
"interface '${labeledInterfaceName}'."
msgstr ""
"Невозможно ${action} ${type}. Невозможно присоединить сеть "
"«${NETWORK_SHOULD_BE_ATTACHED_VIA_LABEL_TO_ANOTHER_NIC_ENTITY}» к сетевому "
"интерфейсу «${interfaceName}». Сеть необходимо присоединить с помощью "
"метки на сетевом интерфейсе «${labeledInterfaceName}»."

msgctxt "ACTION_TYPE_FAILED_CANNOT_REMOVE_NETWORK_FROM_BRICK"
msgid ""
"Cannot ${action} ${type}. The following networks cannot be removed from the "
"network interface since they are used by gluster volume bricks: "
"${ACTION_TYPE_FAILED_CANNOT_REMOVE_NETWORK_FROM_BRICK_LIST}. Please remove "
"or replace the bricks in order to remove the network."
msgstr ""
"Невозможно ${action} ${type}. Следующие сети нельзя удалить с сетевого "
"интерфейса, "
"поскольку они используются элементами тома gluster: "
"${ACTION_TYPE_FAILED_CANNOT_REMOVE_NETWORK_FROM_BRICK_LIST}. Чтобы удалить "
"сеть, "
"удалите или замените элементы."

msgctxt "LABELED_NETWORK_ATTACHED_TO_WRONG_INTERFACE"
msgid ""
"Cannot ${action} ${type}. The following networks are already attached to a "
"different interface: ${AssignedNetworks}. Please remove the networks in "
"order to label the interface."
msgstr ""
"Невозможно ${action} ${type}. Следующие сети уже присоединены к другому "
"интерфейсу: ${AssignedNetworks}. Чтобы назначить метку интерфейсу, эти "
"сети нужно удалить."

msgctxt "OTHER_INTERFACE_ALREADY_LABELED"
msgid ""
"Cannot ${action} ${type}. The label '${NicLabel}' is already defined on "
"other interface ${LabeledNic} on the host."
msgstr ""
"Невозможно ${action} ${type}. Метка «${NicLabel}» уже определена на другом "
"интерфейсе хоста ${LabeledNic}."

msgctxt "ACTION_TYPE_FAILED_SRIOV_FEATURE_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. SR-IOV is not supported on the selected cluster "
"version."
msgstr ""
"Невозможно ${action} ${type}. SR-IOV не поддерживается выбранной версией "
"кластера."

msgctxt "ACTION_TYPE_FAILED_NUM_OF_VFS_CANNOT_BE_CHANGED"
msgid ""
"Cannot ${action} ${type}. The selected network interface ${nicName} has VFs "
"that are in use."
msgstr ""
"Невозможно ${action} ${type}. Выбранный сетевый интерфейс ${nicName} имеет "
"использующиеся виртуальные функции."

msgctxt "ACTION_TYPE_FAILED_NIC_IS_NOT_SRIOV_ENABLED"
msgid ""
"Cannot ${action} ${type}. The selected network interface ${nicName} is not "
"SR-IOV enabled."
msgstr ""
"Невозможно ${action} ${type}. На выбранном сетевом интерфейсе ${nicName} не "
"включена технология SR-IOV."

msgctxt "ACTION_TYPE_FAILED_NUM_OF_VFS_NOT_IN_VALID_RANGE"
msgid ""
"Cannot ${action} ${type}. The specified number of VFs on network interface "
"${nicName} is ${numOfVfs}, the valid range is 0 - ${maxNumOfVfs}."
msgstr ""
"Невозможно ${action} ${type}. Указанное число виртуальных функций на сетевом "
"интерфейсе "
"${nicName} — ${numOfVfs}. Допустимый диапазон: 0 - ${maxNumOfVfs}."

msgctxt "ACTION_TYPE_FAILED_CANNOT_SET_SPECIFIC_NETWORKS"
msgid ""
"Cannot ${action} ${type}. All networks are allowed, cannot set specific "
"network/label."
msgstr ""
"Невозможно ${action} ${type}. Разрешены все типы сетей, нельзя указать "
"конкретную сеть/метку."

msgctxt "ACTION_TYPE_FAILED_NETWORK_ALREADY_IN_VFS_CONFIG"
msgid ""
"Cannot ${action} ${type}. Network ${networkName} already exists in network "
"interface ${nicName} VFs configuration."
msgstr ""
"Невозможно ${action} ${type}. Сеть ${networkName} уже существует в "
"конфигурации "
"сетевого интерфейса ${nicName} виртуальных функций."

msgctxt "ACTION_TYPE_FAILED_NETWORK_NOT_IN_VFS_CONFIG"
msgid ""
"Cannot ${action} ${type}. Network ${networkName} doesn't exist in network "
"interface ${nicName} VFs configuration."
msgstr ""
"Невозможно ${action} ${type}. Сеть ${networkName} не существует в "
"конфигурации "
"сетевого интерфейса ${nicName} виртуальных функций."

msgctxt "ACTION_TYPE_FAILED_NETWORK_NOT_EXIST"
msgid "Cannot ${action} ${type}. Network with id ${networkId} doesn't exist."
msgstr "Невозможно ${action} ${type}. Сеть с ID ${networkId} не существует."

msgctxt "ACTION_TYPE_FAILED_LABEL_ALREADY_IN_VFS_CONFIG"
msgid ""
"Cannot ${action} ${type}. Label ${label} already exists in network interface "
"${nicName} VFs configuration."
msgstr ""
"Невозможно ${action} ${type}. Метка ${label} уже существует в конфигурации "
"сетевого интерфейса ${nicName} виртуальных функций."

msgctxt "ACTION_TYPE_FAILED_LABEL_NOT_IN_VFS_CONFIG"
msgid ""
"Cannot ${action} ${type}. Label ${label} doesn't exist in network interface "
"${nicName} VFs configuration."
msgstr ""
"Невозможно ${action} ${type}. Метка ${label} не существует в конфигурации "
"сетевого интерфейса ${nicName} виртуальных функций."

msgctxt "ERROR_CANNOT_DETACH_LAST_STORAGE_DOMAIN"
msgid ""
"Cannot remove the master Storage Domain from the Data Center without another "
"active Storage Domain to take its place.\n"
"-Either activate another Storage Domain in the Data Center, or remove the "
"Data Center."
msgstr ""
"Невозможно удалить главный домен хранилища из дата-центра, не заменив его "
"другим активным доменом хранилища.\n"
"— Активируйте другой домен хранилища в дата-центре, либо удалите "
"дата-центр."

msgctxt "ERROR_CANNOT_DESTROY_LAST_STORAGE_DOMAIN"
msgid ""
"Cannot destroy the master Storage Domain from the Data Center without "
"another active Storage Domain to take its place.\n"
"-Either activate another Storage Domain in the Data Center, or remove the "
"Data Center.\n"
"-If you have problems with the master Data Domain, consider following the "
"recovery process described in the documentation, or contact your system "
"administrator."
msgstr ""
"Невозможно разрушить главный домен хранилища в дата-центре, не заменив его "
"другим активным доменом хранилища.\n"
"— Активируйте другой домен хранилища в дата-центре, либо удалите "
"дата-центр.\n"
"— При наличии проблем с главным доменом данных попробуйте выполнить "
"процедуру восстановления, описанную в документации, или свяжитесь с "
"системным администратором."

msgctxt "ERROR_CANNOT_DESTROY_LAST_STORAGE_DOMAIN_HOST_NOT_ACTIVE"
msgid ""
"In order to complete the operation a new master Storage Domain needs to be "
"elected, which requires at least one active Host in the Data Center.\n"
"-Please make sure one of the Hosts is active in the Data Center first."
msgstr ""
"Для завершения операции нужно выбрать новый главный домен хранилища, "
"что  требует наличия минимум одного активного хоста в дата-центре.\n"
"— Сначала убедитесь, что один из хостов активен в дата-центре."

msgctxt "NETWORK_VLAN_OUT_OF_RANGE"
msgid "VLAN ID must be a number between 0 and 4094."
msgstr "VLAN ID должен иметь номер в диапазоне от 0 до 4094."

msgctxt "ERROR_CANNOT_ADD_STORAGE_POOL_WITHOUT_DATA_DOMAIN"
msgid ""
"Cannot attach Storage Domain.\n"
"-Please attach Data Domain to the Data Center first."
msgstr ""
"Невозможно подключить домен хранилища.\n"
"— Сначала присоедините домен данных к дата-центру."

msgctxt "ERROR_CANNOT_ADD_STORAGE_DOMAIN_WITH_ATTACHED_DATA_DOMAIN"
msgid ""
"Cannot ${action} ${type}.\n"
"The Storage Domain is already attached to a different Data Center and cannot "
"be attached to an uninitialized Data Center.\n"
"-Please attach a different Data Domain to the Data Center first."
msgstr ""
"Невозможно ${action} ${type}.\n"
"Этот домен хранилища уже присоединён к другому дата-центру, и не может быть "
"присоединён к не инициализированному дата-центру.\n"
"— Сначала присоедините другой домен данных к дата-центру."

msgctxt "ERROR_CANNOT_REMOVE_POOL_WITH_ACTIVE_DOMAINS"
msgid ""
"Cannot remove Data Center which contains active/locked Storage Domains.\n"
"-Please deactivate all domains and wait for tasks to finish before removing "
"the Data Center."
msgstr ""
"Невозможно удалить дата-центр, содержащий активные/заблокированные домены "
"хранилищ.\n"
"— Деактивируйте все домены и подождите завершения выполнения задач перед тем, "
"как удалять "
"дата-центр."

msgctxt "VDS_GROUP_CANNOT_CHANGE_STORAGE_POOL"
msgid "Cannot change Data Center association when editing a Cluster."
msgstr ""
"Во время изменения свойств кластера невозможно изменить привязку дата-центра."

msgctxt "ACTION_TYPE_FAILED_SETUP_NETWORKS_IN_PROGRESS"
msgid ""
"Cannot ${action} ${type}. Another Setup Networks process in progress on the "
"host. Please try later after refreshing the host network capabilities."
msgstr ""
"Невозможно ${action} ${type}. На хосте выполняется другой процесс настройки "
"сети. "
"Повторите попытку позже, после обновления сетевых характеристик хоста."

msgctxt "ACTION_TYPE_FAILED_NETWORK_NAME_IN_USE"
msgid ""
"Cannot ${action} ${type}. The name of the logical network '${NetworkName}' "
"is already used by an existing logical network in the same data-center.\n"
"-Please choose a different name."
msgstr ""
"Невозможно ${action} ${type}. Имя логической сети «${NetworkName}» "
"уже используется существующей логической сетью  того же дата-центра.\n"
"— Выберите другое имя."

msgctxt "ACTION_TYPE_FAILED_NETWORK_IN_MANY_USES"
msgid ""
"Cannot ${action} ${type}. This logical network is used by ${entities}: "
"(${ENTITIES_USING_NETWORK_COUNTER}): ${ENTITIES_USING_NETWORK}\n"
" - Please detach ${entities} using this logical network and try again."
msgstr ""
"Невозможно ${action} ${type}. Эту логическую сеть используют ${entities}: "
"(${ENTITIES_USING_NETWORK_COUNTER}): ${ENTITIES_USING_NETWORK}\n"
" — Отсоедините ${entities}, использующие эту логическую сеть, и "
"повторите попытку."

msgctxt "ACTION_TYPE_FAILED_NETWORK_IN_ONE_USE"
msgid ""
"Cannot ${action} ${type}. This logical network is used by ${entities}: "
"${ENTITIES_USING_NETWORK}\n"
" - Please remove it from this ${entities} and try again."
msgstr ""
"Невозможно ${action} ${type}. Эту логическую сеть используют ${entities}: "
"${ENTITIES_USING_NETWORK}\n"
" — Удалите её из ${entities} и повторите попытку."

msgctxt "ERROR_CANNOT_CREATE_STORAGE_DOMAIN_WITHOUT_VG_LV"
msgid "Volume Group (VGs) and Logical Volumes (LVs) are not specified."
msgstr "Не указаны логические тома и группы тома."

msgctxt "NETWORK_INTERFACE_TEMPLATE_CANNOT_BE_SET"
msgid "Cannot ${action} ${type}. VM Template ID must be empty."
msgstr ""
"Невозможно ${action} ${type}. ID шаблона виртуальной машины должен быть "
"пустым."

msgctxt "NETWORK_INTERFACE_VM_CANNOT_BE_SET"
msgid "Cannot ${action} ${type}. VM ID must be empty."
msgstr "Невозможно ${action} ${type}. ID машины должен быть пустым."

msgctxt "EN_UNKNOWN_NOTIFICATION_METHOD"
msgid "The Notification method is unsupported."
msgstr "Этот метод уведомления о событии не поддерживается."

msgctxt "EN_UNSUPPORTED_NOTIFICATION_EVENT"
msgid "The notification event ${eventName} is unsupported."
msgstr "Этот метод уведомления о событии ${eventName} не поддерживается."

msgctxt "EN_ALREADY_SUBSCRIBED"
msgid ""
"User is already subscribed to this event with the same Notification method."
msgstr ""
"Пользователь уже подписан на уведомление об этом событии, с этим же "
"методом уведомления."

msgctxt "EN_NOT_SUBSCRIBED"
msgid ""
"Cannot ${action} ${type}.User is not subscribed to this event with the given "
"Notification method."
msgstr ""
"Невозможно ${action} ${type}. Пользователь не подписан на это событие с "
"указанным методом уведомления."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_TYPE_ILLEGAL"
msgid "Cannot ${action} ${type}. Storage Domain type is illegal."
msgstr "Невозможно ${action} ${type}. Недопустимый тип домена хранилища."

msgctxt ""
"ACTION_TYPE_FAILED_DOMAIN_TYPE_CAN_BE_CREATED_ONLY_ON_SPECIFIC_STORAGE_DOMAINS"
msgid ""
"Cannot ${action} ${type}. ${domainType} Storage Domain can only be created "
"on the following storage types: ${storageTypes}."
msgstr ""
"Невозможно ${action} ${type}. Домен хранилища с типом ${domainType} может "
"быть создан "
"только со следующими типами хранилища: ${storageTypes}."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_FORMAT_ILLEGAL"
msgid ""
"Cannot ${action} ${type}. Storage Domain format ${storageFormat} is illegal."
msgstr ""
"Невозможно ${action} ${type}. Недопустимый формат домена хранилища: "
"${storageFormat}."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DOMAIN_FORMAT_ILLEGAL_HOST"
msgid ""
"Cannot ${action} ${type}. Storage format ${storageFormat} is not supported "
"on the selected host version."
msgstr ""
"Невозможно ${action} ${type}. Формат хранилища {storageFormat} не "
"поддерживается "
"версией выбранного хоста."

msgctxt "ERROR_CANNOT_EXTEND_CONNECTION_FAILED"
msgid ""
"Cannot extend Storage Domain. Storage device ${lun} is unreachable from "
"${hostName}."
msgstr ""
"Невозможно расширить домен хранилища. Устройство хранения ${lun} "
"недоступно для ${hostName}."

msgctxt "ERROR_CANNOT_CHANGE_STORAGE_DOMAIN_FIELDS"
msgid ""
"Cannot ${action} ${type}. Can only update the following fields: name, "
"description, comment, wipe after delete, low space threshold, and critical "
"space threshold."
msgstr ""
"Невозможно ${action} ${type}. Можно обновить содержание только следующих "
"полей: имя, описание, комментарий, очистка после удаления, порог недостатка "
"места, "
"критический порог недостатка места."

msgctxt ""
"ERROR_CANNOT_UPDATE_STORAGE_POOL_COMPATIBILITY_VERSION_BIGGER_THAN_CLUSTERS"
msgid ""
"Cannot update Data Center compatibility version to a value that is greater "
"than its Cluster's version. The following clusters should be upgraded "
"${ClustersList}."
msgstr ""
"Невозможно обновить версию совместимости дата-центра до значения большего, "
"чем версия его кластера. Необходимо обновить следующие кластеры: "
"${ClustersList}."

msgctxt "ERROR_CANNOT_ADD_EXISTING_STORAGE_DOMAIN_CONNECTION_DATA_ILLEGAL"
msgid ""
"Cannot import Storage Domain. Internal Error: The connection data is illegal."
msgstr ""
"Невозможно импортировать домен хранилища. Внутренняя ошибка: недопустимые "
"данные "
"о соединении."

msgctxt "NETWORK_MAC_ADDRESS_IN_USE"
msgid "MAC Address is already in use."
msgstr "Этот MAC адрес уже используется"

msgctxt "NETWORK_INTERFACE_IN_USE_BY_VM"
msgid ""
"Cannot ${action} ${type}. There is at least one running VM that uses this "
"Network."
msgstr ""
"Невозможно ${action} ${type}. Как минимум одна выполняющаяся ВМ использует "
"эту сеть."

msgctxt "NETWORK_CLUSTER_NETWORK_IN_USE"
msgid ""
"Cannot ${action} ${type}. Network is being used by at least one Cluster."
msgstr ""
"Невозможно ${action} ${type}. Сеть используется по крайней мере одним "
"кластером."

msgctxt "ERROR_CANNOT_DEACTIVATE_MASTER_WITH_NON_DATA_DOMAINS"
msgid ""
"Cannot deactivate a Master Storage Domain while there are ISO/Export active "
"domains in the Data Center.\n"
"-Please deactivate all ISO/Export domains first."
msgstr ""
"Невозможно деактивировать главный домен хранилища, пока в дата-центре "
"есть активные домены ISO/экспорта.\n"
"— Сначала деактивируйте все домены ISO/экспорта."

msgctxt "ERROR_CANNOT_DEACTIVATE_DOMAIN_WITH_TASKS"
msgid ""
"Cannot ${action} ${type} while there are running tasks on this ${type}.\n"
"-Please wait until tasks will finish and try again."
msgstr ""
"Невозможно ${action} ${type} пока есть запущенные задачи на ${type}.\n"
"— Подождите окончания выполнения задач и повторите попытку."

msgctxt "ERROR_CANNOT_DEACTIVATE_MASTER_DOMAIN_WITH_TASKS_ON_POOL"
msgid ""
"Cannot deactivate Master Data Domain while there are running tasks on its "
"Data Center.\n"
"-Please wait until tasks will finish and try again."
msgstr ""
"Невозможно деактивировать главный домен данных, пока пока есть запущенные "
"задачи "
"в его дата-центре.\n"
"— Подождите окончания выполнения задач и повторите попытку."

msgctxt "ERROR_CANNOT_REMOVE_LAST_SUPER_USER_ROLE"
msgid ""
"Cannot ${action} ${type}. The system must have at least one Super User Role "
"defined."
msgstr ""
"Невозможно ${action} ${type}.  В системе должен быть как минимум один "
"пользователь "
"с назначенной ролью супер-пользователя."

msgctxt "VM_CANNOT_REMOVE_VM_WHEN_STATUS_IS_NOT_DOWN"
msgid ""
"Cannot remove VM that its state is not down.\n"
"-Please stop the VM first."
msgstr ""
"Невозможно удалить ВМ,  машина должна иметь статус «Отключена».\n"
"— Сначала остановите машину."

msgctxt "VM_CANNOT_REMOVE_WITH_DETACH_DISKS_SNAPSHOTS_EXIST"
msgid "Cannot ${action} ${type} with detaching disks, snapshots exist."
msgstr ""
"Невозможно ${action} ${type} с отсоединённым диском, снимки существуют."

msgctxt "VM_CANNOT_REMOVE_WITH_DETACH_DISKS_BASED_ON_TEMPLATE"
msgid ""
"Cannot ${action} ${type} with detaching disks, VM is based from template."
msgstr ""
"Невозможно ${action} ${type} с отсоединённым диском, ВМ основана на шаблоне."

msgctxt "ACTION_TYPE_FAILED_MASTER_STORAGE_DOMAIN_NOT_ACTIVE"
msgid ""
"Failed to ${action} ${type} due to an error on the Data Center master "
"Storage Domain.\n"
"-Please activate the master Storage Domain first."
msgstr ""
"Сбой ${action} ${type} из-за ошибки на главном домене хранилища дата-центра.\n"
"— Сначала активируйте главный домен хранилища."

msgctxt "TAGS_SPECIFY_TAG_IS_NOT_EXISTS"
msgid "The specified Tag does not exist."
msgstr "Указанной метки не существует."

msgctxt "NETWORK_CANNOT_ADD_INTERFACE_WHEN_VM_STATUS_NOT_UP_DOWN_LOCKED"
msgid ""
"Cannot ${action} a Network Interface when VM is not Down, Up or Image-Locked."
msgstr ""
"Невозможно ${action} сетевой интерфейс. Статус ВМ должен быть"
"«Отключена», «Запущена» или «Образ заблокирован»."

msgctxt "NETWORK_VLAN_IN_USE"
msgid "The specified VLAN ID (${vlanId}) is already in use."
msgstr "Указанный VLAN ID (${vlanId}) уже используется."

msgctxt "NETWORK_CLUSTER_HAVE_NOT_EXISTING_DATA_CENTER_NETWORK"
msgid ""
"Cluster has Networks that doesn't exist in the Data Center.\n"
"-Please remove those Networks first."
msgstr ""
"На кластере есть сети, не существующие в дата-центре.\n"
"— Удалите сначала эти сети."

msgctxt "NETWORK_NOT_EXISTS_IN_CURRENT_CLUSTER"
msgid "The specified Logical Network doesn't exist in the current Cluster."
msgstr "Указанная логическая сеть не существует на текущем кластере."

msgctxt "NETWORKS_NOT_IN_SYNC"
msgid ""
"Cannot ${action} ${type}. The following Networks' definitions on the Network "
"Interfaces are different than those on the Logical Networks. Please "
"synchronize the Network Interfaces before editing the networks: "
"${NETWORK_NOT_IN_SYNC}."
msgstr ""
"Невозможно ${action} ${type}. Следующие определения сетей на сетевых "
"интерфейсах отличаются от их описания в Логических сетях. Пожалуйста, "
"синхронизируйте сетевые интерфейсы перед редактированием сетей: "
"${NETWORK_NOT_IN_SYNC}."

msgctxt "NETWORK_INTERFACES_ALREADY_SPECIFIED"
msgid ""
"Cannot ${action} ${type}. The following Network Interfaces were specified "
"more than once: ${NETWORK_INTERFACES_ALREADY_SPECIFIED_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Следующие сетевые интерфейсы были определены "
"более одного раза:  ${NETWORK_INTERFACES_ALREADY_SPECIFIED_LIST}."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORKS_CANNOT_BE_PROVISIONED"
msgid ""
"Cannot ${action} ${type}. The following external networks cannot be "
"configured on host via 'Setup Networks': "
"${ACTION_TYPE_FAILED_EXTERNAL_NETWORKS_CANNOT_BE_PROVISIONED_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Следующие внешние сети не могут быть настроены "
"на хосте через меню «Настроить сети»: "
"${ACTION_TYPE_FAILED_EXTERNAL_NETWORKS_CANNOT_BE_PROVISIONED_LIST}."

msgctxt "NETWORKS_ALREADY_ATTACHED_TO_IFACES"
msgid ""
"Cannot ${action} ${type}. The following Logical Networks are attached to "
"more than one Network Interface: "
"${NETWORKS_ALREADY_ATTACHED_TO_IFACES_ENTITY}."
msgstr ""
"Невозможно ${action} ${type}. Следующие логические сети присоединены более "
"чем "
"к одному сетевому интерфейсу: "
"${NETWORKS_ALREADY_ATTACHED_TO_IFACES_ENTITY}."

msgctxt "NETWORK_INTERFACES_DONT_EXIST"
msgid ""
"Cannot ${action} ${type}. The following Network Interfaces do not exist on "
"the Host: ${NETWORK_INTERFACES_DONT_EXIST_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Следующие сетевые интерфейсы не "
"существуют на хосте: ${NETWORK_INTERFACES_DONT_EXIST_LIST}."

msgctxt "NETWORKS_DONT_EXIST_IN_CLUSTER"
msgid ""
"Cannot ${action} ${type}. The following Logical Networks do not exist in the "
"Host's Cluster: ${NETWORKS_DONT_EXIST_IN_CLUSTER_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Следующие логические сети не существуют "
"на кластере хоста: ${NETWORKS_DONT_EXIST_IN_CLUSTER_LIST}."

msgctxt "NETWORKS_DONT_EXIST_IN_DATA_CENTER"
msgid ""
"Cannot ${action} ${type}. The following Logical Networks: ${networkIds} do "
"not exist in the Data Center: ${dataCenterId}."
msgstr ""
"Невозможно ${action} ${type}. Следующие логические сети: ${networkIds} не "
"существуют в дата-центре: ${dataCenterId}."

msgctxt "NETWORK_BONDS_INVALID_SLAVE_COUNT"
msgid ""
"Cannot ${action} ${type}. The following bonds consist of less than two "
"Network Interfaces: ${NETWORK_BONDS_INVALID_SLAVE_COUNT_ENTITY}."
msgstr ""
"Невозможно ${action} ${type}. Следующие связи состоят менее чем из двух "
"сетевых интерфейсов: ${NETWORK_BONDS_INVALID_SLAVE_COUNT_ENTITY}."

msgctxt "NETWORK_INTERFACES_NOT_EXCLUSIVELY_USED_BY_NETWORK"
msgid ""
"Cannot ${action} ${type}. The following Network Interfaces can have only a "
"single VM Logical Network, or at most one non-VM Logical Network and/or "
"several VLAN Logical Networks: "
"${NETWORK_INTERFACES_NOT_EXCLUSIVELY_USED_BY_NETWORK_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Следующие сетевые интерфейсы могут иметь "
"только одну логическую сеть, или хотя бы одну не-ВМ логическую сеть и/или "
"несколько логических сетей VLAN: "
"${NETWORK_INTERFACES_NOT_EXCLUSIVELY_USED_BY_NETWORK_LIST}."

msgctxt "NETWORK_INTERFACES_NOT_EXCLUSIVELY_USED_BY_UNTAGGED_NETWORK"
msgid ""
"Cannot ${action} ${type}. At most one VLAN-untagged Logical Network is "
"allowed on a NIC (optionally in conjunction with several VLAN Logical "
"Networks). The following Network Interfaces violate that : "
"${NETWORK_INTERFACES_NOT_EXCLUSIVELY_USED_BY_UNTAGGED_NETWORK_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Не более одной логической сети без метки "
"VLAN допускается на NIC (как вариант, одновременно с несколькими логическими "
"сетями "
"VLAN). Следующие сетевые интерфейсы нарушают это правило: "
"${NETWORK_INTERFACES_NOT_EXCLUSIVELY_USED_BY_UNTAGGED_NETWORK_LIST}."

msgctxt "NETWORK_CANNOT_DETACH_NETWORK_USED_BY_VMS"
msgid ""
"Cannot ${action} ${type}. The VM(s) "
"'${NETWORK_CANNOT_DETACH_NETWORK_USED_BY_VMS_LIST}' are actively using the "
"Logical Network(s) '${networkNames}'. Please stop the VMs and try again."
msgstr ""
"Невозможно ${action} ${type}. Машины"
"«${NETWORK_CANNOT_DETACH_NETWORK_USED_BY_VMS_LIST}» активно используют "
"логическую сеть(сети) «${networkNames}». Остановите ВМ и повторите попытку."

msgctxt "NON_VM_NETWORK_CANNOT_SUPPORT_STP"
msgid "Cannot ${action} ${type}. STP can only be enabled on VM Networks."
msgstr "Невозможно ${action} ${type}. STP можно включить только в сетях ВМ."

msgctxt "NETWORK_MTU_DIFFERENCES"
msgid ""
"Cannot ${action} ${type}. The following Logical Networks do not have the "
"same MTU value: ${NETWORK_MTU_DIFFERENCES_LIST}."
msgstr ""
"Невозможно ${action} ${type}. У следующих логических сетей не совпадают "
"значения MTU: ${NETWORK_MTU_DIFFERENCES_LIST}."

msgctxt "NETWORK_MTU_OVERRIDE_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Overriding MTU is not supported for this Data "
"Center's compatibility version."
msgstr ""
"Невозможно ${action} ${type}. Переопределение MTU не поддерживается версией "
"совместимости "
"данного дата-центра."

msgctxt "ACTION_TYPE_FAILED_NETWORK_FROM_DIFFERENT_DC"
msgid "Cannot ${action} ${type}. Network belongs to a different data-center."
msgstr "Невозможно ${action} ${type}. Сеть принадлежит другому дата-центру."

msgctxt "ACTION_TYPE_FAILED_DEFAULT_MANAGEMENT_NETWORK_NOT_FOUND"
msgid ""
"Cannot ${action} ${type}. No single suitable network was found to serve as "
"the management network."
msgstr ""
"Невозможно ${action} ${type}. Не найдено ни одной единой сети, "
"подходящей для сети управления."

msgctxt "ACTION_TYPE_FAILED_MANAGEMENT_NETWORK_CANNOT_BE_CHANGED"
msgid ""
"Cannot ${action} ${type}. Changing management network in a non-empty cluster "
"is not allowed."
msgstr ""
"Невозможно ${action} ${type}. Изменение сети управления в не пустом кластере "
"не разрешается."

msgctxt "ACTION_TYPE_FAILED_TARGET_CLUSTER_WITH_DIFF_MANAGEMENT_NETWORK"
msgid ""
"Cannot ${action} ${type}. Moving a host to cluster with different management "
"network is not allowed."
msgstr ""
"Невозможно ${action} ${type}. Перемещения хоста на кластер с другой сетью "
"управления "
"не разрешается."

msgctxt "ACTION_TYPE_FAILED_MANAGEMENT_NETWORK_CANNOT_BE_EXTERNAL"
msgid ""
"Cannot ${action} ${type}. Management network has to be not external one."
msgstr ""
"Невозможно ${action} ${type}. Сеть управления не должна быть внешней сетью."

msgctxt "ACTION_TYPE_FAILED_DUPLICATE_NETWORK_CLUSTER_INPUT"
msgid ""
"Cannot ${action} ${type}. Network cluster ${NetworkCluster} appears more "
"than once."
msgstr ""
"Невозможно ${action} ${type}. Сетевой кластер ${NetworkCluster}присутствует "
"больше "
"одного раза."

msgctxt "ACTION_TYPE_FAILED_MIGRATION_NETWORK_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Migration network is not supported for this "
"cluster version."
msgstr ""
"Невозможно ${action} ${type}. Миграция сетей не поддерживается версией "
"данного кластера."

msgctxt "ACTION_TYPE_FAILED_PROVIDER_DOESNT_EXIST"
msgid "Cannot ${action} ${type}. The provider does not exist in the system."
msgstr "Невозможно ${action} ${type}. Поставщик не существует в системе."

msgctxt "ACTION_TYPE_FAILED_CANNOT_CHANGE_PROVIDER_TYPE"
msgid "Cannot ${action} ${type}. The provider type cannot be changed."
msgstr "Невозможно ${action} ${type}. Тип поставщика нельзя изменять."

msgctxt "ACTION_TYPE_FAILED_PROVIDER_TYPE_MISMATCH"
msgid ""
"Cannot ${action} ${type}. The provider type should be 'OpenStack Networking'."
msgstr ""
"Невозможно ${action} ${type}. Поставщик должен иметь тип «Сеть OpenStack»."

msgctxt "ACTION_TYPE_FAILED_HOST_PROVIDER_TYPE_MISMATCH"
msgid ""
"Cannot ${action} ${type}. The provider type should be 'External Host "
"Provider (Foreman/Satellite)'."
msgstr ""
"Невозможно ${action} ${type}. Поставщик должен иметь тип «Внешний "
"поставщик хоста (Foreman/Satellite)»."

msgctxt "ACTION_TYPE_FAILED_MISSING_NETWORK_MAPPINGS"
msgid ""
"Cannot ${action} ${type}. The network mappings should be provided as a "
"parameter or configured on the network provider."
msgstr ""
"Невозможно ${action} ${type}. Сетевые сопоставления должны указываться в "
"качестве параметра или должны быть настроены поставщиком сети."

msgctxt "ACTION_TYPE_FAILED_MISSING_MESSAGING_BROKER_PROPERTIES"
msgid ""
"Cannot ${action} ${type}. The network provider's broker host address must be "
"configured on the provider."
msgstr ""
"Невозможно ${action} ${type}. Адрес хоста брокера поставщика сети "
"должен быть настроен на стороне поставщика."

msgctxt "ACTION_TYPE_FAILED_PROVIDER_NETWORKS_USED_MULTIPLE_TIMES"
msgid ""
"Cannot ${action} ${type}. Several external networks "
"(${NETWORK_NAMES_COUNTER}) are being used by virtual machines and/or "
"templates:\n"
"${NETWORK_NAMES}\n"
" - Please resolve the external networks usage first and try again."
msgstr ""
"Невозможно ${action} ${type}. Несколько внешних сетей "
"(${NETWORK_NAMES_COUNTER}) используются виртуальными машинами и/или "
"шаблонами:\n"
"${NETWORK_NAMES}\n"
" — Решите вопрос использования внешних сетей и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_PROVIDER_NETWORKS_USED_ONCE"
msgid ""
"Cannot ${action} ${type}. External networks is used by virtual machines and/"
"or templates:\n"
"${NETWORK_NAMES}\n"
" - Please resolve the external networks usage first and try again."
msgstr ""
"Невозможно ${action} ${type}. Внешние сети используются виртуальными машинами "
"и/или шаблонами:\n"
"${NETWORK_NAMES}\n"
" — Решите вопрос использования внешних сетей и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_ALREADY_EXISTS"
msgid ""
"Cannot ${action} ${type}. The external network already exists as "
"'${NetworkName}' in the data center."
msgstr ""
"Невозможно ${action} ${type}. Внешняя сеть уже существует в дата-центре "
"как «${NetworkName}»."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_MUST_BE_VM_NETWORK"
msgid ""
"Cannot ${action} ${type}. An external network cannot be a non-VM network."
msgstr "Невозможно ${action} ${type}. Внешняя сеть может быть только сетью ВМ."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_DETAILS_CANNOT_BE_EDITED"
msgid ""
"Cannot ${action} ${type}. External network details (except name and "
"description) cannot be changed."
msgstr ""
"Невозможно ${action} ${type}. Параметры внешней сети нельзя изменить "
"(кроме имени и описания)."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. External networks are not supported for this "
"cluster's compatibility version."
msgstr ""
"Невозможно ${action} ${type}. Внешние сети не поддерживаются версией "
"совместимости "
"данного кластера."

msgctxt "ACTION_TYPE_FAILED_NOT_SUPPORTED_FOR_EXTERNAL_NETWORK"
msgid ""
"Cannot ${action} ${type}. This operation is not supported for external "
"networks."
msgstr ""
"Невозможно ${action} ${type}. Для внешних сетей операция не поддерживается."

msgctxt "ACTION_TYPE_FAILED_DATA_CENTER_ID_CANNOT_BE_CHANGED"
msgid "Cannot ${action} ${type}. Change of Data Center ID is not allowed."
msgstr "Невозможно ${action} ${type}. Изменение ID дата-центра запрещено."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_CANNOT_BE_DISPLAY"
msgid ""
"Cannot ${action} ${type}. External network cannot be used as a display "
"network."
msgstr ""
"Невозможно ${action} ${type}. Внешняя сеть не может использоваться для "
"передачи экрана."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_CANNOT_BE_REQUIRED"
msgid ""
"Cannot ${action} ${type}. External network cannot be set as required in the "
"cluster."
msgstr ""
"Невозможно ${action} ${type}. Внешняя сеть на кластере не может быть "
"настроена "
"как обязательная."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_WITH_VLAN_MUST_BE_LABELED"
msgid "Cannot ${action} ${type}. External network with vlan must be labeled."
msgstr "Невозможно ${action} ${type}. Внешняя сеть с vlan должна иметь метку."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_CANNOT_BE_PORT_MIRRORED"
msgid ""
"Cannot ${action} ${type}. External network cannot be used when port "
"mirroring is set."
msgstr ""
"Невозможно ${action} ${type}. Внешнюю сеть нельзя использовать "
"при настроенном зеркалировании портов."

msgctxt ""
"ACTION_TYPE_FAILED_PASSTHROUGH_PROFILE_CONTAINS_NOT_SUPPORTED_PROPERTIES"
msgid ""
"Cannot ${action} ${type}. 'Port Mirroring' and 'Qos' are not supported on "
"passthrough profiles."
msgstr ""
"Невозможно ${action} ${type}. «Зеркалирование портов» и «Qos» не "
"поддерживаются "
"в транзитных профилях."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_CANNOT_BE_REWIRED"
msgid ""
"Cannot ${action} ${type}. External network cannot be changed while the "
"virtual machine is running."
msgstr ""
"Невозможно ${action} ${type}. Нельзя изменять свойства внешней сети "
"пока выполняется ВМ."

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_NETWORK_CANNOT_HAVE_MTU"
msgid "Cannot ${action} ${type}. External network cannot have MTU set."
msgstr ""
"Невозможно ${action} ${type}. Внешняя сеть не может иметь "
"настроенный MTU."

msgctxt "ACTION_TYPE_FAILED_MANAGEMENT_NETWORK_REQUIRED"
msgid ""
"Cannot ${action} ${type}. The management network '${NetworkName}' must be "
"required, please change the network to be required and try again."
msgstr ""
"Невозможно ${action} ${type}. Сеть управления «${NetworkName}» "
"должна указываться как требуемая, укажите сеть как требуемую "
"и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_NETWORK_ADDRESS_CANNOT_BE_CHANGED"
msgid ""
"Cannot ${action} ${type}. The address of the network "
"'${ACTION_TYPE_FAILED_NETWORK_ADDRESS_CANNOT_BE_CHANGED_LIST}' cannot be "
"modified without reinstalling the host, since this address was used to "
"create the host's certification."
msgstr ""
"Невозможно ${action} ${type}. Адрес сети "
"«${ACTION_TYPE_FAILED_NETWORK_ADDRESS_CANNOT_BE_CHANGED_LIST}» нельзя "
"изменить "
"без переустановки хоста, потому что этот адрес был использован для "
"создания сертификата хоста."

msgctxt "ACTION_TYPE_FAILED_NETWORK_ADDRESS_BRICK_IN_USE"
msgid ""
"Cannot ${action} ${type}. The address of the network "
"'${ACTION_TYPE_FAILED_NETWORK_ADDRESS_BRICK_IN_USE_LIST}' cannot be modified "
"without removing or replacing the bricks, since this address was used to "
"create gluster volume bricks."
msgstr ""
"Невозможно ${action} ${type}. Адрес сети "
"«${ACTION_TYPE_FAILED_NETWORK_ADDRESS_BRICK_IN_USE_LIST}» нельзя изменить "
"без удаления или замены элементов, поскольку этот адрес использовался для "
"создания "
"элементов тома gluster."

msgctxt "PLUGGED_UNLINKED_VM_INTERFACE_WITH_EXTERNAL_NETWORK_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Plugged and unlinked VM network interface with "
"external network is not supported."
msgstr ""
"Невозможно ${action} ${type}. Подключённый и несвязанный сетевой интерфейс ВМ "
"с внешней сетью — не поддерживается."

msgctxt "CANNOT_PREVIEW_ACTIVE_SNAPSHOT"
msgid "Cannot preview Active VM snapshot."
msgstr "Невозможно провести предварительный просмотр снимка активной ВМ."

msgctxt "TAGS_SPECIFIED_TAG_CANNOT_BE_THE_PARENT_OF_ITSELF"
msgid "Operation canceled, recursive Tag hierarchy cannot be defined."
msgstr "Отмена операции, рекурсивная иерархия тегов не может быть определена."

msgctxt "VM_CANNOT_MOVE_TO_CLUSTER_IN_OTHER_STORAGE_POOL"
msgid "VM can be moved only to a Cluster in the same Data Center."
msgstr ""
"ВМ можно перенести только на кластер в пределах одного и того же дата-центра."

msgctxt "VM_CLUSTER_IS_NOT_VALID"
msgid "Cannot ${action} ${type}. Cluster ID is not valid."
msgstr "Невозможно ${action} ${type}. Недопустимый ID кластера."

msgctxt "NETWORK_CANNOT_REMOVE_MANAGEMENT_NETWORK"
msgid ""
"The Management Network ('${NetworkName}') is mandatory and cannot be removed."
msgstr ""
"Сеть управления («${NetworkName}») является обязательной и её нельзя удалить."

msgctxt "ACTION_TYPE_FAILED_MANAGEMENT_NETWORK_UNSET"
msgid ""
"Cannot ${action} ${type}. Unsetting management network property is not "
"allowed."
msgstr ""
"Невозможно ${action} ${type}. Свойство отмены сети управления не "
"поддерживается."

msgctxt "NETWORK_CANNOT_REMOVE_ISCSI_BOND_NETWORK"
msgid ""
"The Network ('${NetworkName}') could not be removed since several iSCSI "
"bonds (${IscsiBonds_COUNTER}) are using this network:\n"
" ${IscsiBonds}.\n"
"Please remove the network first from those iSCSI bonds, and try again."
msgstr ""
"Не удалось удалить сеть («${NetworkName}»), т.к. несколько связей iSCSI "
"(${IscsiBonds_COUNTER}) используют эту сеть:\n"
" ${IscsiBonds}.\n"
"Сначала удалите сеть из этих связей iSCSI и повторите попытку."

msgctxt "NETWORK_OLD_NETWORK_NOT_SPECIFIED"
msgid "Previous network name is required."
msgstr "Требуется прежнее имя сети."

msgctxt "ACTION_TYPE_FAILED_DETECTED_ACTIVE_VMS"
msgid ""
"Cannot ${action} ${type}. VMs in a state other than \"Down\" were detected.\n"
"-Please ensure all VMs associated with this Storage Domain are stopped and "
"in the Down state first."
msgstr ""
"Невозможно ${action} ${type}. Обнаружены ВМ с другим состоянием, чем "
"«Отключена».\n"
"— Убедитесь сначала, что все ВМ с привязкой к этому домену хранилища "
"остановлены "
"и имеют статус «Отключена»."

msgctxt "ACTION_TYPE_FAILED_HOST_NOT_EXIST"
msgid "The selected Host does not exist."
msgstr "Выбранный хост не существует."

msgctxt "TAGS_SPECIFY_TAG_IS_IN_USE"
msgid "The specified Tag name already exists."
msgstr "Указанное имя метки уже существует."

msgctxt "ACTION_LIST_CANNOT_BE_EMPTY"
msgid "Actions list cannot be empty."
msgstr "Список операций не может быть пустым."

msgctxt "ACTION_TYPE_FAILED_BOOKMARK_INVALID_ID"
msgid "Cannot ${action} ${type}. Bookmark ID is not valid."
msgstr "Невозможно ${action} ${type}. Недопустимый ID закладки."

msgctxt "ACTION_TYPE_FAILED_VDS_STATUS_ILLEGAL"
msgid ""
"Cannot ${action} ${type}. Operation can be performed only when Host status "
"is ${hostStatus}."
msgstr ""
"Невозможно ${action} ${type}. Операция может быть выполнена только когда "
"хост имеет статус ${hostStatus}."

msgctxt "ACTION_TYPE_FAILED_VM_CLUSTER_DIFFERENT_ARCHITECTURES"
msgid ""
"Cannot ${action} ${type}. The VM and the destination cluster architectures "
"do not match."
msgstr ""
"Невозможно ${action} ${type}. Архитектуры виртуальной машины и "
"кластера назначения не совпадают."

msgctxt "ACTION_TYPE_FAILED_MIGRATE_BETWEEN_TWO_CLUSTERS"
msgid ""
"Cannot ${action} ${type}. If target Cluster is not specified VM can be "
"migrated only between Hosts in the same Cluster.\n"
"-Please select target Host in the same Cluster to run the VM or specify a "
"target Cluster."
msgstr ""
"Невозможно ${action} ${type}. Если целевой кластер не указан, то миграция "
"ВМ может быть выполнена только между хостами одного и того же кластера.\n"
"— Для запуска ВМ выберите целевой хост на том же кластере или укажите "
"целевой кластер."

msgctxt "STORAGE_DOMAIN_DOES_NOT_EXIST"
msgid "Storage Domain doesn't exist."
msgstr "Домен хранилища не существует."

msgctxt "VDS_CANNOT_CHECK_VERSION_HOST_NON_RESPONSIVE"
msgid "Cannot get Host version when Host is in Non Responsive status."
msgstr ""
"Невозможно получить версию хоста, если хост имеет статус «не отвечает»."

msgctxt "ACTION_TYPE_FAILED_VDS_INTERMITENT_CONNECTIVITY"
msgid ""
"Due to intermittent connectivity to this Host, fence operations are not "
"allowed at this time. The system is trying to reconnect, please try again in "
"30 seconds."
msgstr ""
"В связи с периодически возникающими ошибками соединения с этим хостом, "
"операции блокады "
"в данное время не разрешены. Система пытается восстановить подключение, "
"повторите попытку "
"через 30 секунд."

msgctxt "ACTION_TYPE_FAILED_PM_ENABLED_WITHOUT_AGENT"
msgid ""
"Cannot ${action} ${type}. Power Management is enabled for Host but no Agent "
"type selected."
msgstr ""
"Невозможно ${action} ${type}. Управление питанием для хоста включено, "
"но тип агента не выбран."

msgctxt "ACTION_TYPE_FAILED_PM_ENABLED_WITHOUT_AGENT_CREDENTIALS"
msgid ""
"Cannot ${action} ${type}. Power Management is enabled for Host but Agent "
"credentials are missing."
msgstr ""
"Невозможно ${action} ${type}. Управление питанием для хоста включено, "
"но отсутствуют учётные данные агента."

msgctxt "ACTION_TYPE_FAILED_AGENT_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Selected Power Management Agent is not supported."
msgstr ""
"Невозможно ${action} ${type}. Выбранный агент управления питанием не "
"поддерживается."

msgctxt "ACTION_TYPE_FAILED_MOM_UPDATE_VDS_VERSION"
msgid ""
"Cannot ${action} ${type}. The cluster's compatibility version doesn't "
"support MoM Policy update."
msgstr ""
"Невозможно ${action} ${type}. Версия совместимости кластера не поддерживает "
"обновление политики MoM."

msgctxt "NETWORK_BOND_NOT_ATTACH_TO_NETWORK"
msgid "Bond is not attached to Network."
msgstr "Связь не присоединена к сети."

msgctxt "NETWORK_INTERFACE_NOT_ATTACH_TO_NETWORK"
msgid "Network Interface is not attached to Logical Network."
msgstr "Сетевой интерфейс не присоединён к логической сети."

msgctxt "NETWORK_INTERFACE_IN_USE_BY_VLAN"
msgid ""
"Bonding cannot be applied on an Interface where VLAN is defined.\n"
"-Please remove VLAN from the interface."
msgstr ""
"Соединение может применяться только к интерфейсу с определённым VLAN.\n"
"— Удалите VLAN из интерфейса."

msgctxt "NETWORK_ALREADY_ATTACHED_TO_CLUSTER"
msgid "Logical Network is already attached to Cluster."
msgstr "Логическая сеть уже присоединена к кластеру."

msgctxt "ACTION_TYPE_FAILED_VDS_WITH_SAME_HOST_EXIST"
msgid "Cannot ${action} ${type}. Host with the same address already exists."
msgstr "Невозможно ${action} ${type}. Хост с таким адресом уже существует."

msgctxt "ACTION_TYPE_FAILED_VDS_WITH_INVALID_SSH_PORT"
msgid "Cannot ${action} ${type}. Invalid SSH port was entered."
msgstr "Невозможно ${action} ${type}. Введён неправильный порт SSH."

msgctxt "ACTION_TYPE_FAILED_VDS_WITH_INVALID_SSH_USERNAME"
msgid "Cannot ${action} ${type}. Invalid SSH user name was entered."
msgstr ""
"Невозможно ${action} ${type}. Введено неправильное имя пользователя SSH."

msgctxt "ACTION_TYPE_FAILED_VDS_WITH_SAME_UUID_EXIST"
msgid "Cannot ${action} ${type}. Host with the same UUID already exists."
msgstr "Невозможно ${action} ${type}. Хост с таким UUID уже существует."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_NUM_OF_MONITORS"
msgid ""
"Cannot ${action} ${type}. Illegal number of monitors is provided, max "
"allowed number of monitors is 1 for VNC and the max number in the "
"ValidNumOfMonitors configuration variable for SPICE."
msgstr ""
"Невозможно ${action} ${type}. Указано недопустимое число мониторов, "
"максимально допустимое число мониторов: 1 для VNC, для SPICE определено в "
"переменной ValidNumOfMonitors."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_SINGLE_DEVICE_OS_TYPE"
msgid ""
"Cannot ${action} ${type}. Cannot set single display device to non Linux "
"operating system."
msgstr ""
"Невозможно ${action} ${type}. Невозможно настроить устройство с одним "
"экраном для не-Linux ОС."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_SINGLE_DEVICE_DISPLAY_TYPE"
msgid ""
"Cannot ${action} ${type}. Cannot set single display device via VNC display."
msgstr ""
"Невозможно ${action} ${type}. Нельзя настроить устройство с одним "
"экраном через монитор VNC."

msgctxt ""
"ACTION_TYPE_FAILED_ILLEGAL_OS_TYPE_IS_NOT_SUPPORTED_BY_ARCHITECTURE_TYPE"
msgid ""
"Cannot ${action} ${type}. Selected operating system is not supported by the "
"architecture."
msgstr ""
"Невозможно ${action} ${type}. Выбранная ОС не поддерживается архитектурой."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_WATCHDOG_MODEL_IS_NOT_SUPPORTED_BY_OS"
msgid ""
"Cannot ${action} ${type}. Selected watchdog model is not supported by the "
"operating system."
msgstr ""
"Невозможно ${action} ${type}. Выбранная модель watchdog не поддерживается ОС."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_VM_DISPLAY_TYPE_IS_NOT_SUPPORTED_BY_OS"
msgid ""
"Cannot ${action} ${type}. Selected display type is not supported by the "
"operating system."
msgstr ""
"Невозможно ${action} ${type}. Выбранный тип монитора не поддерживается ОС."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_FLOPPY_IS_NOT_SUPPORTED_BY_OS"
msgid ""
"Cannot ${action} ${type}. Floppy devices are not supported by the operating "
"system."
msgstr ""
"Невозможно ${action} ${type}. Устройства гибких дисков не поддерживаются ОС."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_OS_TYPE_DOES_NOT_SUPPORT_VIRTIO_SCSI"
msgid ""
"Cannot ${action} ${type}. Selected operation system does not support VirtIO-"
"SCSI. Please disable VirtIO-SCSI for the VM."
msgstr ""
"Невозможно ${action} ${type}. Выбранная операционная система не поддерживает "
"VirtIO-SCSI. Отключите VirtIO-SCSI для этой ВМ."

msgctxt "ACTION_TYPE_FAILED_SUSPEND_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Selected VM has an architecture that does not "
"support suspension."
msgstr ""
"Невозможно ${action} ${type}. Выбранная ВМ имеет архитектуру, "
"не поддерживающую спящий режим."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_SINGLE_DEVICE_INCOMPATIBLE_VERSION"
msgid ""
"Cannot ${action} ${type}. Cluster does not support Single Qxl Pci devices."
msgstr ""
"Невозможно ${action} ${type}. Кластер не поддерживает устройства Single Qxl "
"PCI."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_DOMAIN_NAME"
msgid ""
"Cannot ${action} ${type}. Illegal Domain name: ${Domain}. Domain name has "
"unsupported special character ${Char}."
msgstr ""
"Невозможно ${action} ${type}. Недопустимое имя домена: ${Domain}, "
"специальный символ ${Char} не поддерживается."

msgctxt "ACTION_TYPE_FAILED_ILLEGAL_ARCHITECTURE_TYPE_INCOMPATIBLE"
msgid ""
"Cannot ${action} ${type}. Architecture does not match the expected value."
msgstr ""
"Невозможно ${action} ${type}. Архитектура не соответствует ожидаемому "
"значению."

msgctxt "ACTION_TYPE_FAILED_CANNOT_DECREASE_COMPATIBILITY_VERSION"
msgid "Cannot decrease data center compatibility version."
msgstr "Невозможно понизить версию совместимости дата-центра."

msgctxt "ACTION_TYPE_FAILED_CANNOT_DECREASE_COMPATIBILITY_VERSION_UNDER_DC"
msgid ""
"Cannot decrease cluster compatibility version beneath data center "
"compatibility version."
msgstr ""
"Невозможно понизить версию совместимости кластера ниже номера версии "
"совместимости дата-центра."

msgctxt ""
"ACTION_TYPE_FAILED_DECREASING_COMPATIBILITY_VERSION_CAUSES_STORAGE_FORMAT_DOWN"
"GRADING"
msgid ""
"Cannot ${action} ${type}. This action will cause storage format downgrading "
"which is not supported. The following storage domains cannot be downgraded: "
"${formatDowngradedDomains}."
msgstr ""
"Невозможно ${action} ${type}. Эта операция вызовет неподдерживаемое понижение "
"версии формата хранилища. Нельзя понизить версии следующих доменов хранилища: "
"${formatDowngradedDomains}."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_DOMAINS_ARE_NOT_SUPPORTED_IN_DOWNGRADED_VERSION"
msgid ""
"Cannot ${action} ${type}. The following storage domains are not supported in "
"the selected version: ${unsupportedVersionDomains}."
msgstr ""
"Невозможно ${action} ${type}. Следующие домены хранилища не поддерживаются в "
"выбранной версии: ${unsupportedVersionDomains}."

msgctxt "ACTION_TYPE_FAILED_GIVEN_VERSION_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Selected Compatibility Version is not supported."
msgstr ""
"Невозможно ${action} ${type}. Выбранная версия совместимости не "
"поддерживается."

msgctxt "ACTION_TYPE_FAILED_DATA_CENTER_VERSION_DOESNT_SUPPORT_LIVE_SNAPSHOT"
msgid ""
"Cannot ${action} ${type}. Selected data center compatibility version does "
"not support live snapshot."
msgstr ""
"Невозможно ${action} ${type}. Выбранная версия совместимости дата-центра не "
"поддерживает "
"динамические снимки."

msgctxt "NETWORK_ADDR_MANDATORY_IN_STATIC_IP"
msgid "Network address must be specified when using static ip"
msgstr ""
"При использовании статического адреса IP должен быть указан сетевой адрес."

msgctxt "NETWORK_ADDR_MANDATORY_FOR_ROLE_NETWORK"
msgid ""
"Cannot ${action} ${type}. IP address has to be set for the NIC that bears a "
"role network. Network: ${NetworkName}, Nic: ${nicName} on host ${hostName} "
"violates that rule."
msgstr ""
"Невозможно ${action} ${type}. Адрес IP должен быть настроен для NIC, несущей "
"ролевую сеть. "
"Сеть: ${NetworkName}, Nic: ${nicName} на хосте ${hostName}нарушает это "
"правило."

msgctxt "ACTION_TYPE_FAILED_OBJECT_LOCKED"
msgid ""
"Cannot ${action} ${type}. Related operation is currently in progress. Please "
"try again later."
msgstr ""
"Невозможно ${action} ${type}. Сейчас выполняются связанные операции. "
"Повторите попытку позже."

msgctxt "ACTION_TYPE_FAILED_NETWORK_IS_USED"
msgid ""
"Cannot ${action} ${type}. The network is currently in use. Please wait and "
"try again later."
msgstr ""
"Невозможно ${action} ${type}. В настоящий момент сеть используется. Подождите "
"и повторите попытку."

msgctxt "ACTION_TYPE_FAILED_VM_POOL_IS_USED_FOR_CREATE_VM"
msgid ""
"Cannot ${action} ${type}. This VM Pool is currently in use to create VM "
"${VmName}."
msgstr ""
"Невозможно ${action} ${type}. Этот пул ВМ в данный момент используется для "
"создания ВМ "
"${VmName}."

msgctxt "ACTION_TYPE_FAILED_TEMPLATE_IS_USED_FOR_CREATE_VM"
msgid ""
"Cannot ${action} ${type}. This template is currently in use to create VM "
"${VmName}."
msgstr ""
"Невозможно ${action} ${type}. В данный момент шаблон используется для "
"создания ВМ ${VmName}."

msgctxt "ACTION_TYPE_FAILED_SNAPSHOT_IS_BEING_TAKEN_FOR_VM"
msgid ""
"Cannot ${action} ${type}. Snapshot is currently being created for VM "
"${VmName}."
msgstr ""
"Невозможно ${action} ${type}. В данный момент создаётся снимок для ВМ "
"${VmName}."

msgctxt "ACTION_TYPE_FAILED_VM_IS_HIBERNATING"
msgid "Cannot ${action} ${type}. VM is hibernating."
msgstr "Невозможно ${action} ${type}. ВМ переходит в режим гибернации."

msgctxt "ACTION_TYPE_FAILED_DISK_IS_USED_FOR_CREATE_VM"
msgid ""
"Cannot ${action} ${type}. This disk is currently in use to create VM "
"${VmName}."
msgstr ""
"Невозможно ${action} ${type}. Сейчас этот диск используется для "
"создания ВМ ${VmName}."

msgctxt "ACTION_TYPE_FAILED_DISK_IS_BEING_REMOVED"
msgid "Cannot ${action} ${type}. Disk ${DiskName} is being removed."
msgstr ""
"Невозможно ${action} ${type}. Диск ${DiskName} в данный момент удаляется."

msgctxt "ACTION_TYPE_FAILED_OVF_DISK_IS_BEING_USED"
msgid "Cannot ${action} ${type}. Disk OVF data is currently being updated."
msgstr "Невозможно ${action} ${type}. Диск с данными OVF сейчас обновляется."

msgctxt "ACTION_TYPE_FAILED_UNSUPPORTED_OVF"
msgid "Cannot ${action} ${type}. There is no OVF for the specific entity."
msgstr "Невозможно ${action} ${type}. Отсутствует OVF для заданного объекта."

msgctxt "ACTION_TYPE_FAILED_DOMAIN_OVF_ON_UPDATE"
msgid ""
"Cannot ${action} ${type}. Storage Domain OVF data is currently being updated."
msgstr ""
"Невозможно ${action} ${type}. Данные OVF домена хранилища сейчас "
"обновляются."

msgctxt "ACTION_TYPE_FAILED_DISK_IS_BEING_MIGRATED"
msgid "Cannot ${action} ${type}. Disk ${DiskName} is being moved or copied."
msgstr ""
"Невозможно ${action} ${type}. Диск ${DiskName} сейчас перемещается или "
"копируется."

msgctxt "ACTION_TYPE_FAILED_DESTINATION_AND_SOURCE_STORAGE_SUB_TYPES_DIFFERENT"
msgid ""
"Cannot ${action} ${type}. Source and target domains must both be either file "
"domains or block domains in this Data Center compatibility version."
msgstr ""
"Невозможно ${action} ${type}. В этой версии совместимости дата-центра "
"исходный и "
"целевой домены должны быть либо файловыми либо блочными."

msgctxt "ACTION_TYPE_FAILED_TEMPLATE_IS_BEING_EXPORTED"
msgid "Cannot ${action} ${type}. Template ${TemplateName} is being exported."
msgstr ""
"Невозможно ${action} ${type}. Шаблон ${TemplateName} сейчас экспортируется."

msgctxt "ACTION_TYPE_FAILED_NO_VALID_DOMAINS_STATUS_FOR_TEMPLATE_DISKS"
msgid ""
"Cannot ${action} ${type}. Can't find Domain(s) in ${applicableStatus} status "
"for some of the Template disks.\n"
" Please make sure that there is at least one Storage Domain in applicable "
"status for each one of the disks :\n"
"${disksInfo}"
msgstr ""
"Невозможно ${action} ${type}. Не удалось найти домен(ы) со статусом "
"${applicableStatus} для некоторых дисков шаблона.\n"
" Убедитесь, что для каждого из дисков существует как минимум один домен "
"хранилища с применимым статусом:\n"
"${disksInfo}"

msgctxt "ACTION_TYPE_FAILED_VM_IS_BEING_IMPORTED"
msgid "Cannot ${action} ${type}. VM ${VmName} is being imported."
msgstr "Невозможно ${action} ${type}. ВМ ${VmName} сейчас импортируется."

msgctxt "ACTION_TYPE_FAILED_VM_IS_BEING_MIGRATED"
msgid "Cannot ${action} ${type}. VM ${VmName} is being migrated."
msgstr "Невозможно ${action} ${type}. ВМ ${VmName} в процессе миграции."

msgctxt "ACTION_TYPE_FAILED_TEMPLATE_IS_BEING_REMOVED"
msgid "Cannot ${action} ${type}. Template ${TemplateName} is being removed."
msgstr "Невозможно ${action} ${type}. Шаблон ${TemplateName} сейчас удаляется."

msgctxt "ACTION_TYPE_FAILED_VM_IS_BEING_REMOVED_FROM_EXPORT_DOMAIN"
msgid ""
"Cannot ${action} ${type}. VM ${VmName} is being removed from export domain."
msgstr ""
"Невозможно ${action} ${type}. ВМ ${VmName} сейчас удаляется с домена экспорта."

msgctxt "ACTION_TYPE_FAILED_DISK_IS_USED_BY_GET_ALIGNMENT"
msgid ""
"Cannot ${action} ${type}. Disk ${DiskAlias} alignment is currently being "
"determined."
msgstr ""
"Невозможно ${action} ${type}. В данный момент идёт поиск невыравненных "
"разделов диска ${DiskAlias}."

msgctxt "ACTION_TYPE_FAILED_DISK_IS_BEING_EXPORTED"
msgid "Cannot ${action} ${type}. Disk ${DiskAlias} is being exported."
msgstr "Невозможно ${action} ${type}. Диск ${DiskAlias} сейчас экспортируется."

msgctxt "NETWORK_BOND_HAVE_ATTACHED_VLANS"
msgid "Bond attached to vlan, remove bonds vlan first"
msgstr "Связь присоединена к VLAN, сначала удалите связи VLAN."

msgctxt "NETWORK_INTERFACE_CONNECT_TO_VLAN"
msgid "Cannot attach non vlan network to vlan interface"
msgstr "Невозможно присоединить не VLAN сеть к интерфейсу VLAN."

msgctxt "ACTION_TYPE_FAILED_DISK_MAX_SIZE_EXCEEDED"
msgid "Cannot create disk more than ${max}_disk_size GB"
msgstr "Невозможно создать диск объёмом более ${max}_disk_size ГБ"

msgctxt "NETWORK_HOST_IS_BUSY"
msgid ""
"Cannot edit Network while Host is Active, change the Host to Maintenance "
"mode and try again."
msgstr ""
"Невозможно редактировать настройки сети, пока хост в активном состоянии. "
"Переведите хост в режим поддержки и повторите попытку."

msgctxt "VM_CANNOT_EXPORT_RAW_FORMAT"
msgid "Cannot change format to RAW on export VM."
msgstr "Невозможно изменить формат на RAW для экспортируемой ВМ."

msgctxt "VM_PINNING_FORMAT_INVALID"
msgid "CPU pinning format invalid."
msgstr "Недопустимый формат привязки к ЦПУ."

msgctxt "VM_PINNING_VCPU_DOES_NOT_EXIST"
msgid "CPU pinning validation failed - virtual CPU does not exist in vm."
msgstr ""
"Сбой проверки привязки к ЦПУ — виртуальный ЦПУ не существует на машине."

msgctxt "VM_PINNING_PCPU_DOES_NOT_EXIST"
msgid "CPU pinning validation failed - CPU does not exist in host."
msgstr "Сбой проверки привязки к ЦПУ — ЦПУ не существует на хосте."

msgctxt "VM_PINNING_DUPLICATE_DEFINITION"
msgid "Cannot configure CPU pinning twice to the same vCPU."
msgstr ""
"Невозможно настроить привязку к ЦПУ дважды для одного и того "
"же виртуального ЦПУ."

msgctxt "VM_PINNING_PINNED_TO_NO_CPU"
msgid "Cannot pin a vCPU to no pCPU."
msgstr ""
"Невозможно привязать виртуальный ЦПУ не "
"к физическому ЦПУ."

msgctxt "ACTION_TYPE_FAILED_TEMPLATE_NOT_FOUND_ON_EXPORT_DOMAIN"
msgid ""
"Cannot export VM. Template ${TemplateName} does not exist on the export "
"domain. if you want to export VM without its Template please use "
"TemplateMustExists=false"
msgstr ""
"Невозможно экспортировать ВМ. Шаблон ${TemplateName} не существует "
"в домене экспорта. Если необходимо экспортировать ВМ без её шаблона, "
"используйте 'TemplateMustExists=false'."

msgctxt "ACTION_TYPE_FAILED_VM_NOT_FOUND_ON_EXPORT_DOMAIN"
msgid "Cannot ${action} ${type}. VM not exists in export domain"
msgstr "Невозможно ${action} ${type}. ВМ не существует в домене экспорта."

msgctxt "ACTION_NOT_SUPPORTED_FOR_CLUSTER_POOL_LEVEL"
msgid ""
"The Action ${action} ${type} is not supported for this Cluster or Data "
"Center compatibility version"
msgstr ""
"Действие ${action} ${type} не поддерживается этой версией совместимости "
"кластера/дата-центра."

msgctxt "ACTION_TYPE_FAILED_MAX_CPU_PER_SOCKET"
msgid "Cannot ${action} ${type}. Max number of cpu per socket exceeded"
msgstr ""
"Невозможно ${action} ${type}. Превышено максимальное число ЦПУ "
"на сокет."

msgctxt "ACTION_TYPE_FAILED_MAX_NUM_CPU"
msgid "Cannot ${action} ${type}. Max number of cpu exceeded"
msgstr "Невозможно ${action} ${type}. Превышено максимальное число ЦПУ."

msgctxt "ACTION_TYPE_FAILED_MAX_NUM_SOCKETS"
msgid "Cannot ${action} ${type}. Max number of sockets exceeded"
msgstr "Невозможно ${action} ${type}. Превышено максимальное число сокетов."

msgctxt "VMT_CANNOT_IMPORT_TEMPLATE_EXISTS"
msgid ""
"Import Template failed - Template Id already exist in the system. Please "
"remove the Template (${TemplateName}) from the system first"
msgstr ""
"Сбой импорта шаблона: ID шаблона уже существует в системе. Сначала "
"удалите шаблон (${TemplateName}) из системы."

msgctxt "VMT_CANNOT_IMPORT_TEMPLATE_VERSION_MISSING_BASE"
msgid ""
"Cannot import Template Version, Base Template for this Version is missing, "
"please first Import Base Template, or Import Version as Base Template using "
"Clone."
msgstr ""
"Невозможно импортировать версию шаблона, отсутствует базовый шаблон для этой "
"версии. "
"Импортируйте сначала базовый шаблон или же импортируйте версию как базовый "
"шаблон, "
"используя клонирование."

msgctxt "VM_CANNOT_IMPORT_TEMPLATE_NAME_EXISTS"
msgid ""
"Import Template failed - Template Name already exist in the system. Please "
"rename the Template in the system first"
msgstr ""
"Сбой импорта шаблона — имя шаблона уже существует в системе. Сначала "
"переименуйте системный шаблон."

msgctxt "VM_CANNOT_IMPORT_VM_EXISTS"
msgid ""
"Import VM failed - VM Id already exist in the system. Please remove the VM "
"(${VmName}) from the system first"
msgstr ""
"Сбой импорта ВМ — ID машины уже есть в системе."
" Удалите сначала ВМ (${VmName}) из системы."

msgctxt "VM_CANNOT_IMPORT_VM_NAME_EXISTS"
msgid ""
"Import VM failed - VM Name already exist in the system. Please rename the VM "
"in the system first"
msgstr ""
"Сбой импорта ВМ — имя машины уже есть в системе. Переименуйте сначала "
"ВМ в системе."

msgctxt "TAGS_CANNOT_EDIT_READONLY_TAG"
msgid "Cannot edit readonly tag"
msgstr "Невозможно изменить метку, доступную только для чтения"

msgctxt "VM_CANNOT_REMOVE_HAS_RUNNING_TASKS"
msgid "Cannot force remove VM when there are running tasks."
msgstr "Невозможно форсировать удаление ВМ, если выполняются задачи."

msgctxt "ACTION_TYPE_FAILED_IMPORTED_TEMPLATE_IS_MISSING"
msgid ""
"Cannot import and collapse VM, Template is missing in domain ${DomainName}"
msgstr ""
"Невозможно импортировать и свернуть ВМ, в домене ${DomainName} "
"отсутствует шаблон."

msgctxt "VM_POOL_CANNOT_CREATE_WITH_NO_VMS"
msgid "Cannot create Pool with 0 VMs"
msgstr "Невозможно создать пул с 0 машин."

msgctxt "ACTION_TYPE_FAILED_DISK_BOOT_IN_USE"
msgid ""
"Cannot ${action} ${type}. Disk ${DiskName} in VM ${VmName} is already marked "
"as boot."
msgstr ""
"Невозможно ${action} ${type}. Диск ${DiskName} машины ${VmName} уже помечен "
"как загрузочный."

msgctxt "ACTION_TYPE_FAILED_VMS_BOOT_IN_USE"
msgid ""
"Cannot ${action} ${type}. The VMs ${VmsName} already have a disk marked as "
"boot."
msgstr ""
"Невозможно ${action} ${type}. Машина ${VmsName} уже имеет диск, помеченный "
"как загрузочный."

msgctxt "VM_OR_TEMPLATE_ILLEGAL_PRIORITY_VALUE"
msgid ""
"VM priority value passed the permitted max value, value should be between 0 "
"and ${MaxValue}."
msgstr ""
"Значение приоритета ВМ превышает максимальное значение. Значение должно "
"быть в интервале между 0 и ${MaxValue}."

msgctxt "ACTION_TYPE_FAILED_MIN_CPU_PER_SOCKET"
msgid "Minimum number of CPU per socket cannot be less than 1"
msgstr "Минимальное количество ЦПУ на сокет не может быть меньше 1."

msgctxt "ACTION_TYPE_FAILED_MIN_NUM_SOCKETS"
msgid "Minimum number of sockets cannot be less then 1"
msgstr "Минимальное число сокетов не может быть меньше 1."

msgctxt "ACTION_TYPE_FAILED_SPECIFY_DOMAIN_IS_NOT_EXPORT_DOMAIN"
msgid ""
"Cannot ${action} ${type}. The specified domain '${storageDomainName}' is not "
"an export domain."
msgstr ""
"Невозможно ${action} ${type}. Указанный домен «${storageDomainName}» не "
"является доменом экспорта."

msgctxt "NETWORK_ADDR_IN_STATIC_IP_BAD_FORMAT"
msgid "Bad format of IP address"
msgstr "Неверный формат IP адреса"

msgctxt "NETWORK_ADDR_IN_GATEWAY_BAD_FORMAT"
msgid "Bad format of gateway address"
msgstr "Неверный формат адреса шлюза"

msgctxt "NETWORK_ADDR_IN_SUBNET_BAD_FORMAT"
msgid "Bad format of subnet mask"
msgstr "Неверный формат маски подсети"

msgctxt "UPDATE_NETWORK_ADDR_IN_SUBNET_BAD_FORMAT"
msgid ""
"This field must contain a subnet in either of the following formats:\n"
"	xxx.xxx.xxx.xxx where xxx is between 0 and 255.\n"
"	xx where xx is between 0-32"
msgstr ""
"Это поле должно содержать подсеть в одном из следующих форматов:\n"
"	xxx.xxx.xxx.xxx где xxx имеет значение между 0 и 255.\n"
"	xx где xx имеет значение между 0-32"

msgctxt "UPDATE_NETWORK_ADDR_IN_SUBNET_BAD_VALUE"
msgid "Invalid subnet's prefix"
msgstr "Недопустимый префикс подсети"

msgctxt "NETWORK_BOND_NAME_BAD_FORMAT"
msgid ""
"Bad bond name, it must begin with the prefix 'bond' followed by a number."
msgstr ""
"Неправильное имя связки, в начале имени должен стоять префикс «bond», "
"далее должно идти число."

msgctxt "NETWORK_CANNOT_CONTAIN_BOND_NAME"
msgid "Bad network name, network cannot start with 'bond'"
msgstr "Неправильное имя сети, имя не может начинаться с «bond»."

msgctxt "RHEVH_LOCALFS_WRONG_PATH_LOCATION"
msgid "Local Storage folder on RHEV Hypervisor must be located under ${path}"
msgstr ""
"Папка локального хранилища на гипервизоре RHEV должна "
"иметь путь ${path}"

msgctxt "ACTION_TYPE_FAILED_STORAGE_POOL_IS_NOT_LOCAL"
msgid "Data Center must be \"Local Storage\""
msgstr "Дата-центр должен быть «локальным хранилищем»"

msgctxt "VDS_CANNOT_ADD_MORE_THEN_ONE_HOST_TO_LOCAL_STORAGE"
msgid "Cannot add more than one Host to \"Local Storage\" Data Center"
msgstr ""
"Невозможно добавить больше одного хоста в дата-цент, "
"определённый как «локальное хранилище»"

msgctxt "VDS_CANNOT_REMOVE_HOST_WITH_LOCAL_STORAGE"
msgid ""
"Cannot remove Host, as it contains a local Storage Domain. Please activate "
"the Host and remove the Data Center first.\n"
"- If Host cannot be activated, use the Force-Remove option on the Data "
"Center object (select the Data Center and right click on it with the mouse).\n"
"- Please note that this action is destructive."
msgstr ""
"Невозможно удалить хост, он содержит локальный домен хранилища. Сначала "
"активируйте хост и удалите дата-центр.\n"
"— Если хост нельзя активировать, используйте действие «Принудительно удалить» "
"на объект дата-центра (выберите дата-центр и сделайте щелчок ПКМ).\n"
"— Внимание: это деструктивное действие."

msgctxt "VDS_GROUP_CANNOT_ADD_MORE_THEN_ONE_HOST_TO_LOCAL_STORAGE"
msgid "Cannot add more than one Cluster to \"Local Storage\" Data Center"
msgstr ""
"Невозможно добавить больше одного кластера в дата-центр «локальное хранилище»"

msgctxt "VDS_GROUP_SELECTION_ALGORITHM_MUST_BE_SET_TO_NONE_ON_LOCAL_STORAGE"
msgid ""
"Selection algorithm must be set to \"None\" on \"Local Storage\" Data Center"
msgstr ""
"Для алгоритма выбора в дата-центре «локальное хранилище» должен быть "
"указан параметр «Отсутствует»."

msgctxt "VDS_GROUP_CANNOT_DETACH_DATA_DOMAIN_FROM_LOCAL_STORAGE"
msgid "\"Local Storage\" data domain cannot be detached from Data Center"
msgstr ""
"Домен данных «локальное хранилище» нельзя отсоединить "
"от дата-центра"

msgctxt "DATA_CENTER_POSIX_STORAGE_NOT_SUPPORTED_IN_CURRENT_VERSION"
msgid ""
"\"File based storage\" is not supported with data center compatibility "
"version."
msgstr ""
"«Файловое хранилище» не поддерживается версией совместимости дата-центра."

msgctxt "DATA_CENTER_GLUSTER_STORAGE_NOT_SUPPORTED_IN_CURRENT_VERSION"
msgid ""
"\"Gluster based storage\" is not supported with current data center "
"compatibility version."
msgstr ""
"«Хранилище Gluster» не поддерживается текущей версией "
"совместимости дата-центра."

msgctxt "DATA_CENTER_CINDER_STORAGE_NOT_SUPPORTED_IN_CURRENT_VERSION"
msgid ""
"Cinder based storage is not supported with current data center compatibility "
"version."
msgstr ""
"Хранилище Cinder не поддерживается текущей версией "
"совместимости дата-центра."

msgctxt "VDS_CANNOT_UPDATE_CLUSTER"
msgid ""
"Updating Host's Cluster cannot be performed through update Host action, "
"please use Change Host Cluster action instead."
msgstr ""
"Операция обновления кластера хоста не может быть выполнена с помощью "
"действия «Обновить хост», выберите «Сменить кластер хоста»."

msgctxt "VM_CANNOT_UPDATE_CLUSTER"
msgid ""
"Updating VM's Cluster cannot be performed through update VM action, please "
"use Change VM Cluster action instead."
msgstr ""
"Операция обновления кластера ВМ не может быть выполнена с помощью "
"действия «Обновить ВМ», выберите «Сменить кластер ВМ»."

msgctxt "VM_STATUS_NOT_VALID_FOR_UPDATE"
msgid "Cannot update a VM in this status. Try stopping the VM first."
msgstr ""
"Невозможно обновить ВМ в этом статусе. Попробуйте сначала оставить "
"выполнение машины."

msgctxt "PERMISSION_ADD_FAILED_PERMISSION_NOT_SENT"
msgid "Cannot add permission, no permission sent"
msgstr "Невозможно добавить права доступа, права доступа не были посланы."

msgctxt "PERMISSION_ADD_FAILED_INVALID_ROLE_ID"
msgid "Cannot add permission, invalid Role id"
msgstr "Не удалось добавить права доступа, недопустимый ID роли"

msgctxt "PERMISSION_ADD_FAILED_INVALID_OBJECT_ID"
msgid "Cannot add permission, invalid object ID or type"
msgstr ""
"Не удалось добавить права доступа, недопустимый ID или "
"тип объекта."

msgctxt "PERMISSION_ADD_FAILED_USER_ID_MISMATCH"
msgid "Cannot add permission, no user/group ID or ID's mismatch"
msgstr ""
"Невозможно добавить права доступа, отсутствует ID пользователя/группы, "
"или ID не совпадают."

msgctxt "PERMISSION_ADD_FAILED_ONLY_SYSTEM_SUPER_USER_CAN_GIVE_ADMIN_ROLES"
msgid ""
"Cannot add permission, only system super user can give permissions with "
"admin Role"
msgstr ""
"Невозможно добавить права доступа, только системный суперпользователь "
"может выдавать права доступа в роли администратора."

msgctxt ""
"PERMISSION_REMOVE_FAILED_ONLY_SYSTEM_SUPER_USER_CAN_REMOVE_ADMIN_ROLES"
msgid ""
"Cannot remove permission, only system super user can remove permissions with "
"admin Role"
msgstr ""
"Невозможно удалить права доступа, только системный суперпользователь "
"может удалять права доступа в роли администратора."

msgctxt "PERMISSION_ADD_FAILED_VM_IN_POOL"
msgid ""
"Cannot add permission, VM is part of VM-Pool, permission should be given on "
"VM-Pool instead"
msgstr ""
"Невозможно добавить права доступа, ВМ является частью пула машин, права "
"нужно добавлять пулу ВМ."

msgctxt "VDS_GROUP_CPU_UTILIZATION_MUST_BE_IN_VALID_RANGE"
msgid "CPU utilization threshold must be between 0 and 100."
msgstr "Порог использования ЦПУ должен быть в интервале от 0 до 100."

msgctxt ""
"VDS_GROUP_CPU_LOW_UTILIZATION_PERCENTAGE_MUST_BE_LOWER_THAN_HIGH_PERCENTAGE"
msgid ""
"The lower CPU utilization threshold must be lower than the defined upper "
"threshold."
msgstr ""
"Нижний порог загрузки ЦПУ должен быть меньше установленного "
"верхнего порога."

msgctxt ""
"VDS_GROUP_CPU_HIGH_UTILIZATION_PERCENTAGE_MUST_BE_DEFINED_WHEN_USING_EVENLY_DI"
"STRIBUTED"
msgid ""
"High CPU utilization threshold must be defined when using evenly distributed "
"policy."
msgstr ""
"При использовании политики равномерного распределения необходимо "
"определить верхний порог загрузки ЦПУ."

msgctxt ""
"VDS_GROUP_BOTH_LOW_AND_HIGH_CPU_UTILIZATION_PERCENTAGE_MUST_BE_DEFINED_WHEN_US"
"ING_POWER_SAVING"
msgid ""
"Both low and high CPU utilization thresholds must be defined when using "
"power saving policy."
msgstr ""
"При использовании политики сохранения энергии необходимо определить "
"как нижний, так и верхний пороги загрузки ЦПУ."

msgctxt "VDS_GROUP_AT_LEAST_ONE_SERVICE_MUST_BE_ENABLED"
msgid "At least one service [virt / gluster] must be enabled on the cluster."
msgstr ""
"На кластере должна быть включена по крайней мере одна служба [virt / gluster]."

msgctxt "VDS_GROUP_CANNOT_DISABLE_VIRT_WHEN_CLUSTER_CONTAINS_VMS"
msgid "Cannot disable virt service on the cluster as it contains VMs."
msgstr ""
"Невозможно отключить службу virt на кластере, поскольку он содержит ВМ."

msgctxt "VDS_GROUP_CANNOT_DISABLE_GLUSTER_WHEN_CLUSTER_CONTAINS_VOLUMES"
msgid "Cannot disable gluster service on the cluster as it contains volumes."
msgstr ""
"Невозможно выключить службу gluster на кластере, поскольку он "
"содержит тома."

msgctxt "NETWORK_ATTACH_ILLEGAL_GATEWAY"
msgid "The default gateway should be set only on the Management Network"
msgstr "Шлюз по умолчанию должен быть настроен только в сети управления."

msgctxt "SLAVE_INTERFACE_IS_MISCONFIGURED"
msgid ""
"A slave interface is not properly configured. Please verify slaves do not "
"contain any of the following properties: network name, boot protocol, IP "
"address, netmask, gateway or vlan-ID notation (as part of interface's name "
"or explicitly)."
msgstr ""
"Подчинённый интерфейс настроен неправильно. Убедитесь, что у подчинённых "
"интерфейсов нет следующих параметров: имени сети, протокола загрузки, "
"адреса IP, маски сети, нотации шлюза или vlan-ID (в составе имени интерфейса "
"или явным образом)."

msgctxt "IMPROPER_INTERFACE_IS_LABELED"
msgid ""
"Cannot ${action} ${type}. An improper network interface is labeled. Please "
"verify labels are provided only to interfaces or to bonds (not to slaves nor "
"vlans)."
msgstr ""
"Невозможно ${action} ${type}. Метка присвоена неправильному сетевому "
"интерфейсу. Убедитесь, что метки присвоены только интерфейсам или связям "
"(но не подчинённым интерфейсам или vlan)."

msgctxt "IMPROPER_BOND_IS_LABELED"
msgid ""
"Cannot ${action} ${type}. An improper bond '${bondName}' is labeled. Please "
"verify labels are provided only to bonds with at least two slaves."
msgstr ""
"Невозможно ${action} ${type}. Метка назначена неподходящей связи "
"«${bondName}». Убедитесь, "
"что метки назначены только для связей, имеющих минимум два подчинённых "
"интерфейса."

msgctxt "INTERFACE_ALREADY_LABELED"
msgid ""
"Cannot ${action} ${type}. The network interface '${LabeledNic}' is already "
"labeled with the specified label '${NicLabel}'."
msgstr ""
"Невозможно ${action} ${type}. Указанная метка «${NicLabel}» уже присвоена "
"сетевому интерфейсу «${LabeledNic}»."

msgctxt "INTERFACE_NOT_LABELED"
msgid ""
"Cannot ${action} ${type}. The network interface is not labeled with the "
"specified label."
msgstr ""
"Невозможно ${action} ${type}. Сетевому интерфейсу не присвоена указанная "
"метка."

msgctxt "NETWORK_INTERFACE_IS_NOT_BOND"
msgid ""
"Cannot ${action} ${type}. The given network interface "
"'${NETWORK_INTERFACE_IS_NOT_BOND_ENTITY}' is not a bond."
msgstr ""
"Невозможно ${action} ${type}. Указанный сетевой интерфейс "
"«${NETWORK_INTERFACE_IS_NOT_BOND_ENTITY}» не является связью."

msgctxt "MISSING_DIRECTORY_ELEMENT_ID"
msgid "User/group ID cannot be empty."
msgstr "ID пользователя/группы не может быть пустым"

msgctxt "NETWORK_ILEGAL_NETWORK_NAME"
msgid ""
"Network name must be 1-15 long and can contain only 'a-z', 'A-Z', '0-9', '_' "
"or '-' characters"
msgstr ""
"Имя сети должно быть от 1 до 15 знаков длинной и содержать только "
"символы «a-z», «A-Z», «0-9», «_» или «-»."

msgctxt "VALIDATION_STORAGE_CONNECTION_INVALID"
msgid "Mount path is illegal, please use [IP:/path or FQDN:/path] convention."
msgstr ""
"Недопустимый путь монтирования, используйте обозначение "
"[IP:/путь или FQDN:/путь]."

msgctxt "VALIDATION_STORAGE_CONNECTION_EMPTY_VFSTYPE"
msgid "VFS type cannot be empty."
msgstr "Тип VFS не может быть пустым."

msgctxt "VALIDATION_STORAGE_CONNECTION_MOUNT_OPTIONS_CONTAINS_MANAGED_PROPERTY"
msgid ""
"Cannot ${action} ${type}. Custom mount options contain the following "
"duplicate managed options: ${invalidOptions}."
msgstr ""
"Невозможно ${action} ${type}. Пользовательские параметры монтирования "
"содержат следующие дублирующие управляемые параметры: ${invalidOptions}."

msgctxt "VALIDATION_STORAGE_CONNECTION_EMPTY_IQN"
msgid "Target details cannot be empty."
msgstr "Сведения об объекте IQN не могут быть пустыми."

msgctxt "VALIDATION_STORAGE_CONNECTION_EMPTY_CONNECTION"
msgid "${fieldName} field cannot be empty."
msgstr "Поле ${fieldName} не может быть пустым."

msgctxt "VALIDATION_STORAGE_CONNECTION_NFS_RETRANS"
msgid "NFS Retransmissions should be between 0 and 32767"
msgstr ""
"Число повторных отправлений NFS должно находиться в интервале между 0 и 32767"

msgctxt "VALIDATION_STORAGE_CONNECTION_NFS_TIMEO"
msgid "NFS Timeout should be between 1 and 6000"
msgstr "Время ожидания NFS должно находиться в пороге между 1 и 6000"

msgctxt "VALIDATION_TAGS_INVALID_TAG_NAME"
msgid ""
"Invalid tag name. Only alphanumeric chars, '-' and '_' characters are valid"
msgstr ""
"Недопустимое имя метки. Допускаются только буквенно-цифровые символы, "
"«-» и «_»."

msgctxt "VALIDATION_VM_POOLS_NAME_NOT_NULL"
msgid "Pool name is required"
msgstr "Требуется имя пула"

msgctxt "VALIDATION_VM_POOLS_SPICE_PROXY_HOSTNAME_OR_IP"
msgid "SPICE proxy address must be in form [protocol://]hostname or ip[:port]"
msgstr ""
"Адрес прокси SPICE должен быть вида: [protocol://]имя_хоста или ip[:порт]"

msgctxt "VALIDATION_ROLES_NAME_NOT_NULL"
msgid "Role name is required"
msgstr "Требуется имя роли"

msgctxt "VALIDATION_ROLES_NAME_MAX"
msgid "Role name must not exceed 126 characters"
msgstr "Имя роли не должно превышать 126 символов"

msgctxt "VALIDATION_VDS_GROUP_NAME_NOT_NULL"
msgid "Cluster name is required"
msgstr "Требуется имя кластера"

msgctxt "VALIDATION_VDS_GROUP_NAME_MAX"
msgid "Cluster name must not exceed 40 characters"
msgstr "Имя кластера не должно превышать 40 символов"

msgctxt "VALIDATION_VDS_GROUP_MIGRATE_ON_ERROR_NOT_NULL"
msgid "Cluster migrate on error option is required"
msgstr "Кластеру требуется параметр migrate on error"

msgctxt "VALIDATION_VDS_GROUP_SPICE_PROXY_HOSTNAME_OR_IP"
msgid "SPICE proxy address must be in form [protocol://]hostname or ip[:port]"
msgstr ""
"Адрес прокси SPICE должен быть вида: [protocol://]имя_хоста или ip[:порт]"

msgctxt "VALIDATION_STORAGE_POOL_ID_NOT_NULL"
msgid "Data Center ID is required"
msgstr "Требуется ID дата-центра"

msgctxt "VALIDATION_VDS_HOSTNAME_HOSTNAME_OR_IP"
msgid "Host address must be a FQDN or a valid IP address"
msgstr "Адрес хоста должен быть FQDN или допустимым адресом IP"

msgctxt "VALIDATION_VDS_POWER_MGMT_ADDRESS_HOSTNAME_OR_IP"
msgid "Host power management address must be a FQDN or a valid IP address"
msgstr ""
"Адрес управления питанием хоста должен быть FQDN или допустимым "
"адресом IP"

msgctxt "VALIDATION_VM_NUM_OF_MONITORS_EXCEEDED"
msgid "VM exceeded the number of allowed monitors"
msgstr "ВМ превысила число разрешённых мониторов"

msgctxt "VALIDATION_VM_TEMPLATE_NAME_MAX"
msgid "VM Template name must not exceed 40 characters"
msgstr "Имя шаблона ВМ не должно превышать 40 символов"

msgctxt "VALIDATION_DISK_INTERFACE_NOT_NULL"
msgid "Interface is required"
msgstr "Требуется интерфейс"

msgctxt "VALIDATION_VOLUME_TYPE_NOT_NULL"
msgid "Volume type is required"
msgstr "Требуется тип тома"

msgctxt "VALIDATION_DISK_DESCRIPTION_INVALID"
msgid ""
"Disk description must be formed only from alpha-numeric characters and "
"special characters that conform to the standard ASCII character set."
msgstr ""
"Описание диска должно включать только буквенно-цифровые и специальные "
"символы, отвечающие стандарту ASCII."

msgctxt "VALIDATION_DISK_ALIAS_INVALID"
msgid "Disk alias name must be formed of alpha-numeric characters or \"-_.\""
msgstr ""
"Псевдоним диска может включать только буквенно-цифровые символы, «-» или «_»."

msgctxt "VALIDATION_VOLUME_FORMAT_NOT_NULL"
msgid "Volume format is required"
msgstr "Требуется форматирование тома"

msgctxt "VALIDATION_DISK_IMAGE_DESCRIPTION_NOT_EMPTY"
msgid "Snapshot description cannot be empty"
msgstr "Описание снимка не может быть пустым"

msgctxt "VALIDATION_DISK_IMAGE_DESCRIPTION_MAX"
msgid "Snapshot description must not exceed 4000 characters"
msgstr "Описание снимка не должно превышать 4000 символов"

msgctxt "VALIDATION_EVENTS_EMAIL_FORMAT"
msgid "email format is not valid"
msgstr "недопустимый формат электронной почты"

msgctxt "VALIDATION_INVALID_CORRELATION_ID"
msgid ""
"The correlation ID must be formed of 'a-z', 'A-Z', '0-9', '_' or '-' "
"characters"
msgstr ""
"ID корреляции может содержать только символы «a-z», «A-Z», «0-9», «_» или «-»"

msgctxt "VALIDATION_ROLES_NAME_INVALID"
msgid "Role name must be formed of 'a-z', 'A-Z', '0-9', '_' or '-'"
msgstr ""
"Имя роли может содержать только символы  «a-z», «A-Z», «0-9», «_» или «-»"

msgctxt "VALIDATION_VDS_GROUP_NAME_INVALID"
msgid ""
"Cluster name must be formed of alphanumeric characters, numbers or \"-_\""
msgstr ""
"Имя кластера может состоять только из буквенно-цифровых символов, «-» или «_»."

msgctxt "VALIDATION_VDS_NAME_INVALID"
msgid "Host name must be formed of alphanumeric characters, numbers or \"-_.\""
msgstr ""
"Имя хоста может состоять только из буквенно-цифровых символов, «-» или «_»."

msgctxt "VALIDATION_STORAGE_DOMAIN_NAME_INVALID"
msgid ""
"Storage Domain name must be formed of 'a-z', 'A-Z', '0-9', '_' or '-' "
"characters"
msgstr ""
"Имя домена хранилища должно состоять из символов «a-z», «A-Z», «0-9», «_» или "
"«-»."

msgctxt "VALIDATION_STORAGE_DOMAIN_DESCRIPTION_INVALID"
msgid ""
"Storage Domain description must be formed of \"a-z0-9A-Z\" or \"-_\" "
"characters"
msgstr ""
"Описание домена хранилища должно состоять из символов «a-z0-9A-Z» "
"или «-_»."

msgctxt "VALIDATION_STORAGE_DOMAIN_DESCRIPTION_MAX"
msgid "Storage Domain description must not exceed 4000 characters"
msgstr "Описание домена хранилища не должно превышать 4000 символов"

msgctxt "VALIDATION_STORAGE_DOMAIN_WARNING_LOW_SPACE_INDICATOR_RANGE"
msgid ""
"Storage Domain's \"Warning Low Space Indicator\" must be an integer between "
"0 and 100."
msgstr ""
"«Индикатор предупреждения о недостатке места» домена хранилища должен "
"быть целым числом в диапазоне от 0 до 100."

msgctxt "VALIDATION_STORAGE_DOMAIN_CRITICAL_SPACE_ACTION_BLOCKER_RANGE"
msgid ""
"Storage Domain's \"Critical Space Action Blocker\" must be a non-negative "
"integer."
msgstr ""
"«Блокировщик действия при критической нехватке места» домена хранилища "
"должен быть целым неотрицательным числом."

msgctxt "VALIDATION_DATA_CENTER_DESCRIPTION_INVALID"
msgid "Data Center description must be formed of ASCII charis only"
msgstr "Описание дата-центра должно состоять только из символов ASCII"

msgctxt "VALIDATION_DATA_CENTER_NAME_INVALID"
msgid ""
"Data Center name must be formed of 'a-z', 'A-Z', '0-9', '_' or '-' "
"characters"
msgstr ""
"Имя дата-центра может содержать только символы  «a-z», «A-Z», «0-9», «_» или "
"«-»"

msgctxt "VALIDATION_ID_NULL"
msgid "ID is required."
msgstr "Необходим ID"

msgctxt "VALIDATION_NETWORK_ID_OR_NETWORK_NAME_MUST_BE_SET"
msgid "Either network id or network name must be provided."
msgstr "Необходимо указать ID сети или имя сети."

msgctxt "NIC_LABEL_VALIDATION_NIC_ID_OR_NIC_NAME_MUST_BE_SET"
msgid "Nic Label must provide either nic id or nic name."
msgstr "Метка nic должна содержать либо nic id либо имя nic."

msgctxt "LABEL_ON_NETWORK_LABEL_CANNOT_BE_NULL"
msgid "Nic Label cannot have null label."
msgstr "Метка Nic не может иметь нулевую метку"

msgctxt "VALIDATION_NAME_NULL"
msgid "Name is required."
msgstr "Требуется имя"

msgctxt "VALIDATION_NAME_INVALID"
msgid "Name must be formed of alphanumeric characters, numbers or \"-_\"."
msgstr ""
"Имя может состоять только из буквенно-цифровых символов, чисел или «-_»."

msgctxt "VALIDATION_NAME_INVALID_WITH_DOT"
msgid "Name must be formed of alphanumeric characters, numbers or \"-_.\"."
msgstr ""
"Имя может состоять только из буквенно-цифровых символов, чисел или «-_.»."

msgctxt "VALIDATION_URL_NULL"
msgid "URL is required."
msgstr "Требуется URL"

msgctxt "VALIDATION_URL_INVALID"
msgid "URL is invalid."
msgstr "Недопустимый URL"

msgctxt "VALIDATION_PROVIDER_TYPE_NULL"
msgid "Provider type is required."
msgstr "Требуется тип поставщика"

msgctxt "VALIDATION_QUOTA_NAME_INVALID"
msgid "Quota name must be formed of 'a-z', 'A-Z', '0-9', '_' or '-' characters"
msgstr ""
"Имя квоты может содержать только символы  «a-z», «A-Z», «0-9», «_» или «-»"

msgctxt "VALIDATION_VM_NETWORK_MAC_ADDRESS_INVALID"
msgid ""
"MAC address must be in format \"HH:HH:HH:HH:HH:HH\" where H is a hexadecimal "
"character (either a digit or A-F, case is insignificant)."
msgstr ""
"MAC адрес должен иметь формат «HH:HH:HH:HH:HH:HH», где H — шестнадцатеричный "
"символ (цифра или буква от A до F, регистр не важен)."

msgctxt "VALIDATION_VM_NETWORK_MAC_ADDRESS_MULTICAST"
msgid "Multi-cast MAC address is not allowed."
msgstr "Групповой MAC адрес не разрешён"

msgctxt "VALIDATION_VM_NETWORK_MAC_ADDRESS_NOT_NULL"
msgid "MAC address is required."
msgstr "Требуется MAC адрес"

msgctxt "VALIDATION_VM_NETWORK_NAME_NOT_NULL"
msgid "Interface name is required."
msgstr "Требуется имя интерфейса."

msgctxt "VALIDATION_CONNECTIVITY_TIMEOUT_INVALID"
msgid "Connectivity timeout is not valid. Timeout must be between 1 and 120."
msgstr "Недопустимый таймаут подключения. Таймаут должен быть между 1 и 120."

msgctxt "VALIDATION_REPETITIVE_IP_IN_VDS"
msgid ""
"Invalid list of interfaces, two or more network interfaces have the same IP."
msgstr ""
"Недопустимый список интерфейсов, два или более сетевых интерфейса имеют "
"одинаковый IP"

msgctxt "VALIDATION_VDS_PORT_RANGE"
msgid "The Port number must be between 1 and 65535."
msgstr "Номер порта должен быть между 1 и 65535."

msgctxt "VALIDATION_VM_POOLS_NUMBER_OF_MAX_ASSIGNED_VMS_OUT_OF_RANGE"
msgid "Number of max assigned VMs per user must be between 1 and 32,767."
msgstr ""
"Максимально допустимое число ВМ, назначенных одному пользователю, "
"должно быть в диапазоне от 1 до 32767."

msgctxt "ERROR_CANNOT_FIND_ISO_IMAGE_PATH"
msgid "Invalid ISO image path"
msgstr "Недопустимый путь до образа ISO"

msgctxt "ERROR_CANNOT_FIND_FLOPPY_IMAGE_PATH"
msgid "Invalid Floppy image path"
msgstr "Недопустимый путь до образа гибкого диска"

msgctxt "VDS_ADD_STORAGE_SERVER_STATUS_MUST_BE_UP"
msgid "Cannot add storage server connection when Host status is not up"
msgstr ""
"Невозможно добавить подключение к серверу хранилища, если хост не "
"имеет статуса «Запущен»."

msgctxt "USER_FAILED_TO_AUTHENTICATE_WRONG_USERNAME_OR_PASSWORD"
msgid "The user name or password is incorrect."
msgstr "Неправильное имя пользователя или пароль"

msgctxt "VDS_GROUP_CANNOT_UPDATE_CPU_WITH_SUSPENDED_VMS"
msgid ""
"Cannot update Cluster and change CPU Cluster level if there are suspended "
"VMs in the Cluster"
msgstr ""
"Невозможно обновить кластер и сменить уровень ЦПУ кластера, если "
"на кластере имеются замороженные ВМ."

msgctxt "VDS_GROUP_CPU_IS_NOT_UPDATABLE"
msgid ""
"Cannot update Cluster and change CPU Cluster name if there are hosts or "
"virtual machines in the Cluster. This CPU name is incompatible with all "
"other available CPUs"
msgstr ""
"Невозможно обновить кластер и сменить имя ЦПУ кластера, если "
"в кластере есть хосты или ВМ. Это имя ЦПУ несовместимо со всеми "
"другими доступными ЦПУ."

msgctxt "USER_FAILED_TO_AUTHENTICATE_ACCOUNT_IS_LOCKED_OR_DISABLED"
msgid "Authentication failed. The user is either locked or disabled"
msgstr "Сбой аутентификации. Пользователь заблокирован или отключён."

msgctxt "ACTION_TYPE_FAILED_INVALID_CUSTOM_PROPERTIES_DUPLICATE_KEYS"
msgid ""
"Cannot ${action} ${type} if some of the specified custom properties appear "
"more than once. The keys are: ${DuplicateKeys}"
msgstr ""
"Невозможно ${action} ${type}, если некоторые из указанных пользовательских "
"свойств "
"указаны больше одного раза. Ключи: ${DuplicateKeys}"

msgctxt "ACTION_TYPE_FAILED_HOSTNAME_CANNOT_CHANGE"
msgid ""
"Host Address can not be modified due to Security restrictions.  In order to "
"change Host Address, Host has to be reinstalled"
msgstr ""
"Ограничения по безопасности запрещают смену адреса хоста. Чтобы "
"сменить адрес хоста, хост необходимо переустановить."

msgctxt "CAN_DO_ACTION_DATABASE_CONNECTION_FAILURE"
msgid ""
"Action failed due to database connection failure. Please check connectivity "
"to your Database server."
msgstr ""
"Сбой выполнения операции произошёл из-за ошибки соединения. Проверьте "
"возможность подключения к серверу базы данных."

msgctxt "ACTION_TYPE_FAILED_DESCRIPTION_MAY_NOT_CONTAIN_SPECIAL_CHARS"
msgid ""
"Cannot ${action} ${type}. The given description contains special characters.\n"
"Only alpha-numeric and some special characters that conform to the standard "
"ASCII character set are allowed."
msgstr ""
"Невозможно ${action} ${type}. Указанное описание содержит специальные "
"символы.\n"
"Разрешены только буквенно-цифровые и некоторые специальные символы, "
"отвечающие "
"стандарту ASCII."

msgctxt "ACTION_TYPE_FAILED_EMULATED_MACHINE_MAY_NOT_CONTAIN_SPECIAL_CHARS"
msgid ""
"Cannot ${action} ${type}. The given emulated machine contains special "
"characters. Only lower-case and upper-case letters, numbers, '_', '-', '.', "
"'+' are allowed."
msgstr ""
"Невозможно ${action} ${type}. Указанная эмулированная машина содержит "
"специальные символы. Разрешены только прописные и строчные буквы, цифры "
"и символы: «_»,«-», «.», «+»."

msgctxt "ACTION_TYPE_FAILED_CPU_NAME_MAY_NOT_CONTAIN_SPECIAL_CHARS"
msgid ""
"Cannot ${action} ${type}. The given CPU name contains special characters. "
"Only lower-case and upper-case letters, numbers, '_', '-', '.', '+' are "
"allowed."
msgstr ""
"Невозможно ${action} ${type}. Указанное имя ЦПУ содержит специальные "
"символы. Разрешены только прописные и строчные буквы, цифры "
"и символы: «_»,«-», «.», «+»."

msgctxt ""
"ACTION_TYPE_FAILED_LINUX_BOOT_PARAMS_MAY_NOT_CONTAIN_TRIMMING_WHITESPACES"
msgid ""
"Cannot ${action} ${type}. Linux boot parameters contain trimming whitespace "
"characters."
msgstr ""
"Невозможно ${action} ${type}. В параметры загрузки Linux обрезаны символы "
"пробелов."

msgctxt "ACTION_TYPE_FAILED_NAME_LENGTH_IS_TOO_LONG"
msgid "Cannot ${action} ${type}. The given name is too long."
msgstr "Невозможно ${action} ${type}. Указано слишком длинное имя."

msgctxt "ACTION_TYPE_FAILED_NAME_MAY_NOT_BE_EMPTY"
msgid "Can not ${action} ${type}. The given name is empty."
msgstr "Невозможно ${action} ${type}. Указано пустое имя."

msgctxt "ACTION_TYPE_FAILED_TEMPLATE_IS_INCOMPATIBLE"
msgid ""
"Can not ${action} ${type}. The selected template is not compatible with "
"Cluster architecture."
msgstr ""
"Невозможно ${action} ${type}. Выбранный шаблон не совместим с архитектурой "
"кластера."

msgctxt "ACTION_TYPE_FAILED_NAME_MAY_NOT_CONTAIN_SPECIAL_CHARS"
msgid ""
"Can not ${action} ${type}. The given name contains special characters. Only "
"lower-case and upper-case letters, numbers, '_', '-', '.' are allowed."
msgstr ""
"Невозможно ${action} ${type}. Указанное имя содержит специальные символы. "
"Разрешены только прописные и строчные буквы, цифры и символы: «_»,«-», «.»."

msgctxt "ACTION_TYPE_FAILED_INVALID_POOL_NAME"
msgid ""
"Can not ${action} ${type}. The given name is invalid for pool name. Only "
"lower-case and upper-case letters, numbers, '_', '-', '.', and one mask "
"sequence are allowed."
msgstr ""
"Невозможно ${action} ${type}. Указанное имя пула является "
"недопустимым. Разрешены только прописные и строчные буквы, цифры, "
"символы: «_»,«-», «.», а также последовательность записи маски."

msgctxt "ACTION_TYPE_FAILED_INVALID_VDS_HOSTNAME"
msgid ""
"Can not ${action} ${type}. The given Host name is invalid. Only Host names "
"corresponding to RFC-952 and RFC-1123 are allowed."
msgstr ""
"Невозможно  ${action} ${type}. Указано недопустимое имя хоста. Разрешены "
"только имена хостов, соответствующие стандартам RFC-952 и RFC-1123."

msgctxt "VM_CANNOT_RUN_ONCE_WITH_ILLEGAL_SYSPREP_PARAM"
msgid "Missing UserName or Password."
msgstr "Отсутствует имя пользователя или пароль."

msgctxt "ACTION_TYPE_FAILED_CANNOT_REMOVE_BUILTIN_GROUP_EVERYONE"
msgid "Cannot remove the built-in group \"Everyone\"."
msgstr "Невозможно удалить встроенную группу «Каждый»."

msgctxt "ACTION_TYPE_FAILED_IMPORT_DATA_DOMAIN_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. The operation is not supported for this Data "
"Center version."
msgstr ""
"Невозможно ${action} ${type}. Операция не поддерживается версией "
"дата-центра."

msgctxt "ACTION_TYPE_FAILED_VDS_NOT_MATCH_VALID_STATUS"
msgid ""
"Cannot ${action} ${type}. Valid Host statuses are \"Non operational\", "
"\"Maintenance\" or \"Connecting\"."
msgstr ""
"Невозможно ${action} ${type}. Допустимые статусы хоста: «Нерабочий», "
"«Обслуживается» или «Идёт подключение»."

msgctxt "CANNOT_REMOVE_STORAGE_DOMAIN_INVALID_HOST_ID"
msgid "Can not Remove Storage Domain - the underlying Host ID is invalid."
msgstr "Невозможно удалить домен хранилища — недопустимый ID базового хоста."

msgctxt "ERROR_CANNOT_DEACTIVATE_MASTER_WITH_LOCKED_DOMAINS"
msgid ""
"You are trying to deactivate a Master storage domain while there are locked "
"domains in the Data Center. Please wait for the operations on those domains "
"to finish first."
msgstr ""
"Попытка деактивировать главный домен хранилища, тогда "
"как имеются заблокированные домены в дата-центре. Сначала "
"дождитесь завершения выполнения операций на этих доменах."

msgctxt "TAGS_CANNOT_REMOVE_TAG_NOT_EXIST"
msgid "Cannot remove tag. Tag does not exist."
msgstr "Невозможно удалить тег. Тег не существует."

msgctxt "AUDIT_LOG_CANNOT_REMOVE_AUDIT_LOG_NOT_EXIST"
msgid "Cannot remove audit log. Audit Log does not exist."
msgstr "Невозможно удалить журнал аудита. Журнал не существует."

msgctxt "VAR__TYPE__VM__CLUSTER"
msgid "$type VM Cluster"
msgstr "$type кластера ВМ"

msgctxt "VDS_FENCE_OPERATION_FAILED"
msgid "Cannot ${action} ${type}. Fence operation failed."
msgstr "Невозможно ${action} ${type}. Сбой проведения операции блокады."

msgctxt "VM_CANNOT_WITHOUT_ACTIVE_STORAGE_DOMAIN_ISO"
msgid "Cannot ${action} without active ISO domain."
msgstr "Невозможно ${action} без активного домена ISO."

msgctxt "ERROR_CANNOT_DEACTIVATE_STORAGE_DOMAIN_WITH_ISO_ATTACHED"
msgid ""
"Cannot ${action} ${type}. The ISO Storage Domain is being used by the "
"following VMs: ${VmNames}."
msgstr ""
"Невозможно ${action} ${type}. Домен хранилища ISO используется следующими "
"ВМ: ${VmNames}."

msgctxt "MAC_ADDRESS_IS_IN_USE"
msgid "Mac Address is in use."
msgstr "MAC адрес используется"

msgctxt "CAN_DO_ACTION_GENERAL_FAILURE"
msgid "General command validation failure."
msgstr "Общий сбой проверки команд."

msgctxt "ERROR_CANNOT_REMOVE_ACTIVE_STORAGE_POOL"
msgid "Cannot remove an active Data Center."
msgstr "Невозможно удалить активный дата-центр"

msgctxt "ROLE_TYPE_CANNOT_BE_EMPTY"
msgid "Invalid Role Type."
msgstr "Недопустимый тип роли"

msgctxt "CANNOT_ADD_ACTION_GROUPS_TO_ROLE_TYPE"
msgid "Cannot add administrator's action group to a User Role."
msgstr ""
"Невозможно добавить группу административных операций "
"к роли пользователя."

msgctxt "STORAGE_DOMAIN_NOT_ATTACHED_TO_STORAGE_POOL"
msgid "Storage Domain is already detached from the Data Center."
msgstr "Домен хранилища уже отсоединён от дата-центра"

msgctxt "VDS_APPROVE_VDS_NOT_FOUND"
msgid "Cannot approve Host - Host does not exists."
msgstr "Невозможно утвердить хост. Хост не существует."

msgctxt ""
"ACTION_TYPE_FAILED_VM_CANNOT_IMPORT_VM_ARCHITECTURE_NOT_SUPPORTED_BY_CLUSTER"
msgid ""
"Cannot ${action} ${type}. The selected cluster doesn't support VM "
"architecture"
msgstr ""
"Невозможно ${action} ${type}. Выбранный кластер не поддерживает архитектуру "
"ВМ."

msgctxt ""
"ACTION_TYPE_FAILED_VM_CANNOT_IMPORT_TEMPLATE_ARCHITECTURE_NOT_SUPPORTED_BY_CLU"
"STER"
msgid ""
"Cannot ${action} ${type}. The selected cluster doesn't support Template's "
"architecture"
msgstr ""
"Невозможно ${action} ${type}. Выбранный кластер не поддерживает "
"архитектуру шаблона."

msgctxt ""
"ACTION_TYPE_FAILED_VM_CANNOT_IMPORT_VM_WITH_NOT_SUPPORTED_ARCHITECTURE"
msgid "Cannot ${action} ${type}. The selected VM has undefined architecture"
msgstr ""
"Невозможно ${action} ${type}. Выбранная ВМ имеет неопределённую архитектуру."

msgctxt ""
"ACTION_TYPE_FAILED_VM_CANNOT_IMPORT_TEMPLATE_WITH_NOT_SUPPORTED_ARCHITECTURE"
msgid ""
"Cannot ${action} ${type}. The selected Template has undefined architecture"
msgstr ""
"Невозможно ${action} ${type}. Выбранный шаблон имеет неопределённую "
"архитектуру."

msgctxt "ACTION_TYPE_FAILED_VDS_HA_MAINT_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Setting hosted engine maintenance mode is only "
"supported on cluster version 3.4 and above."
msgstr ""
"Невозможно ${action} ${type}. Установка режима обслуживания ЦУ поддерживается "
"только "
"на кластере версии 3.4 и выше."

msgctxt "ACTION_TYPE_FAILED_VDS_HA_NOT_CONFIGURED"
msgid "Cannot ${action} ${type}. Hosted Engine is not configured on this host."
msgstr ""
"Невозможно ${action} ${type}. Виртуализированный ЦУ не настроен на этом хосте."

msgctxt "ACTION_TYPE_FAILED_QUOTA_NOT_EXIST"
msgid "Cannot ${action} ${type}. Quota doesn't exist."
msgstr "Невозможно ${action} ${type}. Квота не существует."

msgctxt "ACTION_TYPE_FAILED_QUOTA_IS_NOT_VALID"
msgid "Cannot ${action} ${type}. Quota is not valid."
msgstr "Невозможно ${action} ${type}. Недопустимая квота."

msgctxt "ACTION_TYPE_FAILED_NO_QUOTA_SET_FOR_DOMAIN"
msgid ""
"Cannot ${action} ${type}. No quota is defined for the selected domain. "
"Assign a quota to the domain or select a different domain."
msgstr ""
"Невозможно ${action} ${type}. Не определена квота для выбранного домена. "
"Назначьте квоту для этого домена или выберите другой домен."

msgctxt "ACTION_TYPE_FAILED_QUOTA_IS_NO_LONGER_AVAILABLE_IN_SYSTEM"
msgid ""
"Cannot ${action} ${type}. The quota associated with VM ${VmName} is no "
"longer available. This may be a result of an import or snapshot restoring "
"actions. Please reassign a quota to this VM."
msgstr ""
"Невозможно ${action} ${type}. Квота, связанная с ВМ ${VmName}, больше "
"недоступна.  Это может быть результатом импорта или восстановления снимка. "
"Переназначьте квоту для этой ВМ."

msgctxt "ACTION_TYPE_FAILED_QUOTA_LIMIT_IS_SPECIFIC_AND_GENERAL"
msgid ""
"Cannot ${action} ${type}. Limitation can not be configured as specific and "
"general for the same Quota. Please choose whether the limitation should be "
"enforced on the Data Center (global) or for specific storage domain or "
"cluster."
msgstr ""
"Невозможно ${action} ${type}. Ограничение не может быть настроено "
"одновременно как конкретное и как общее для одной и той же квоты. "
"Выберите, будет ли ограничение принудительно применено к дата-центру "
"(глобальное) или к конкретному домену хранилища или кластеру."

msgctxt "ACTION_TYPE_FAILED_QUOTA_IN_USE_BY_VM_OR_DISK"
msgid ""
"Cannot ${action} ${type}. Quota is still in use by a VM or a disk and Data "
"Center's Quota enforcement is enabled."
msgstr ""
"Невозможно ${action} ${type}. Квота всё ещё используется машиной или диском, "
"и включено принудительное применении квоты к дата-центру."

msgctxt "ACTION_TYPE_FAILED_QUOTA_STORAGE_LIMIT_EXCEEDED"
msgid "Cannot ${action} ${type}. Quota has insufficient storage resources."
msgstr ""
"Невозможно ${action} ${type}. У квоты нет достаточных ресурсов хранилища."

msgctxt "ACTION_TYPE_FAILED_QUOTA_VDS_GROUP_LIMIT_EXCEEDED"
msgid "Cannot ${action} ${type}. Quota has insufficient cluster resources."
msgstr ""
"Невозможно ${action} ${type}. У квоты нет достаточных ресурсов кластера."

msgctxt "USER_NOT_AUTHORIZED_TO_CONSUME_QUOTA"
msgid ""
"Cannot ${action} ${type}. The user is not a consumer of the Quota assigned "
"to the resource."
msgstr ""
"Невозможно ${action} ${type}. Пользователь не является получателем квоты, "
"назначенной этому ресурсу."

msgctxt "USER_CANNOT_RUN_QUERY_NOT_PUBLIC"
msgid "Permission denied. Query you try to run not public."
msgstr "Доступ запрещён. Попытка выполнить запрос, не являющийся общим."

msgctxt "USER_CANNOT_RUN_ACTION_INTERNAL_COMMAND"
msgid "Permission denied. Action you try to run is internal only."
msgstr ""
"Доступ запрещён. Попытка выполнить действие, являющееся "
"только внутренним."

msgctxt "ERROR_CANNOT_ATTACH_ACTION_GROUP_TO_ROLE_ATTACHED"
msgid ""
"Cannot attach action group to Role. This action group is already attached to "
"Role."
msgstr ""
"Невозможно присоединить группу действия к роли. Эта группа действия "
"уже присоединена к роли."

msgctxt "ERROR_CANNOT_DETACH_ACTION_GROUP_TO_ROLE_NOT_ATTACHED"
msgid ""
"Cannot detach action group from Role. This action group is not attached to "
"this Role."
msgstr ""
"Невозможно отсоединить группу действия от роли. Эта группа действия не "
"присоединена к этой роли."

msgctxt "VDS_CANNOT_CONNECT_TO_SERVER"
msgid ""
"Cannot ${action} ${type}. Connecting to host via SSH has failed, verify that "
"the host is reachable (IP address, routable address etc.) You may refer to "
"the engine.log file for further details."
msgstr ""
"Невозможно ${action} ${type}. Сбой подключения к хосту по SSH, убедитесь в "
"доступности хоста (адрес IP, маршрутизируемый адрес и т.д.). Подробности см. "
"в файле engine.log."

msgctxt "VDS_CANNOT_AUTHENTICATE_TO_SERVER"
msgid ""
"Cannot ${action} ${type}. SSH authentication failed, verify authentication "
"parameters are correct (Username/Password, public-key etc.) You may refer to "
"the engine.log file for further details."
msgstr ""
"Невозможно ${action} ${type}. Сбой аутентификации SSH. Убедитесь в "
"правильности параметров аутентификации (имя пользователя/пароль, "
"открытый ключ и т.п.). Подробности см. в файле engine.log."

msgctxt "VDS_SECURITY_CONNECTION_ERROR"
msgid "Cannot ${action} ${type}. SSH connection failed, ${ErrorMsg}."
msgstr "Невозможно ${action} ${type}. Сбой подключения SSH, ${ErrorMsg}."

msgctxt "ERROR_CANNOT_ADD_STORAGE_POOL_WITHOUT_DATA_AND_ISO_DOMAINS"
msgid "Cannot create Data Center - There must be at least one Data storage."
msgstr ""
"Невозможно создать дата-центр, необходимо как минимум одно хранилище "
"данных."

msgctxt "ERROR_CANNOT_ADD_STORAGE_POOL_WITH_DIFFERENT_STORAGE_FORMAT"
msgid "Cannot add data storages to pool. Storages should have same format."
msgstr ""
"Невозможно добавить хранилища данных в  пул. Хранилища должны "
"иметь одинаковый формат."

msgctxt "EN_UNKNOWN_TAG_NAME"
msgid "Unknown tag name."
msgstr "Неизвестное имя тега."

msgctxt "ERROR_CANNOT_REMOVE_POOL_WITH_NETWORKS"
msgid ""
"Cannot remove Data Center with networks, please remove all networks first."
msgstr "Невозможно удалить дата-центр с сетями, сначала удалите все сети."

msgctxt "NETWORK_CAN_NOT_REMOVE_DEFAULT_NETWORK"
msgid "Cannot remove the default Network."
msgstr "Невозможно удалить сеть по умолчанию."

msgctxt "ACTION_TYPE_FAILED_NETWORK_LABEL_RENAMING_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Renaming a network label while configured on hosts "
"interfaces is not supported. Please unlabel the network first and later "
"specify the new one."
msgstr ""
"Невозможно ${action} ${type}. Переименование метки сети при наличии "
"конфигурации "
"на интерфейсах хостов не поддерживается. Сначала удалите метку с сети и позже "
"укажите новую."

msgctxt "ACTION_TYPE_FAILED_DETECTED_PINNED_VMS"
msgid ""
"Cannot ${action} ${type}. The following VMs are set to run specifically only "
"on this Host: ${VmNames}.\n"
"In order to ${action} ${type}, you need to remove the association between "
"the VMs and the Host (Using Edit VM properties)."
msgstr ""
"Невозможно ${action} ${type}. Следующие ВМ настроены на выполнение "
"только на этом хосте: ${VmNames}.\n"
"Чтобы выполнить ${action} ${type}, необходимо удалить привязку машин и хоста "
"(в меню «Свойства ВМ»)."

msgctxt "HOT_PLUG_IS_NOT_SUPPORTED"
msgid ""
"Activate/Deactivate while VM is running, is only supported for Clusters of "
"version 3.1 and above."
msgstr ""
"Активация/деактивация во время выполнения ВМ поддерживается только "
"для кластеров версии 3.1. и выше."

msgctxt "HOT_PLUG_CPU_IS_NOT_SUPPORTED"
msgid ""
"Hot plugging a CPU is not supported for cluster version ${clusterVersion} "
"and architecture ${architecture}."
msgstr ""
"Горячее подключение ЦПУ не поддерживается для кластера версии "
"${clusterVersion} "
"и архитектуры ${architecture}."

msgctxt "HOT_UNPLUG_CPU_IS_NOT_SUPPORTED"
msgid ""
"Hot un-plugging a CPU is not supported for cluster version ${clusterVersion} "
"and architecture ${architecture}."
msgstr ""
"Горячее отключение ЦПУ не поддерживается для кластера версии "
"${clusterVersion} "
"и архитектуры ${architecture}."

msgctxt "HOT_PLUG_MEMORY_IS_NOT_SUPPORTED"
msgid ""
"Hot plugging memory is not supported for cluster version ${clusterVersion} "
"and architecture ${architecture}."
msgstr ""
"Горячее подключение памяти не поддерживается для кластера версии "
"${clusterVersion} "
"и архитектуры ${architecture}."

msgctxt "HOT_UNPLUG_MEMORY_IS_NOT_SUPPORTED"
msgid ""
"Hot un-plugging memory is not supported for cluster version "
"${clusterVersion} and architecture ${architecture}."
msgstr ""
"Горячее отключение памяти не поддерживается для кластера версии "
"${clusterVersion} "
"и архитектуры ${architecture}."

msgctxt "ACTION_TYPE_FAILED_NO_MORE_MEMORY_SLOTS"
msgid ""
"Cannot ${action} ${type}. Exceeded maximum number of available memory slots: "
"${maxMemSlots}."
msgstr ""
"Невозможно ${action} ${type}. Превышено максимальное число доступных "
"слотов памяти: ${maxMemSlots}."

msgctxt "ACTION_TYPE_FAILED_MEMORY_MUST_BE_MULTIPLICATION"
msgid ""
"Cannot ${action} ${type}. Plugged memory must be multiplication of "
"${multiplicationSize}."
msgstr ""
"Невозможно ${action} ${type}. Подключенная память должна быть кратной "
"${multiplicationSize}."

msgctxt "HOT_PLUG_DISK_SNAPSHOT_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Activation/Deactivation of Disk Snapshot is not "
"supported for clusters of version ${clusterVersion}."
msgstr ""
"Невозможно ${action} ${type}. Активация/деактивация снимка диска не "
"поддерживается для версии кластера ${clusterVersion}."

msgctxt "HOT_PLUG_IDE_DISK_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${diskAlias} to ${vmName}. Hot plugging a disk to an IDE "
"interface is not supported."
msgstr ""
"Невозможно ${action} ${diskAlias} to ${vmName}. Горячее подключение "
"диска к интерфейсу IDE не поддерживается."

msgctxt "UNLINKING_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Link state is set to 'Down' on the virtual "
"machine's interface, this is not supported for clusters of version "
"${clusterVersion}."
msgstr ""
"Невозможно ${action} ${type}. Линк на интерфейсе ВМ имеет статус "
"«Отключено», что не поддерживается для кластеров версии ${clusterVersion}."

msgctxt "ACTION_TYPE_FAILED_UNLINKING_OF_PASSTHROUGH_VNIC_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Unlinking of 'passthrough' vm network interface is "
"not supported."
msgstr ""
"Невозможно ${action} ${type}. Отсоединение от «транзитного» сетевого "
"интерфейса ВМ "
"не поддерживается."

msgctxt "NULL_NETWORK_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. There is no network on the virtual machine's "
"interface, this is not supported for clusters of version ${clusterVersion}."
msgstr ""
"Невозможно ${action} ${type}. На интерфейсе ВМ отсутствует сеть, "
"что не поддерживается кластерами версии ${clusterVersion}."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. There is Network QoS on the profile, this is not "
"supported for clusters of version ${clusterVersion}."
msgstr ""
"Невозможно ${action} ${type}. На профиле присутствует сетевой QoS, что "
"не поддерживается кластерами версии ${clusterVersion}."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_QOS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Host Network QoS is not supported in the cluster's "
"compatibility version, but QoS was configured on the following network(s): "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_NOT_SUPPORTED_LIST}."
msgstr ""
"Невозможно ${action} ${type}. QoS сети хоста не поддерживается версией "
"совместимости "
"калстера, но QoS была настроена в следующих сетях: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_NOT_SUPPORTED_LIST}."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_LABELS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Network labels are not supported in the cluster's "
"compatibility version, but are configured on the host's interfaces."
msgstr ""
"Невозможно ${action} ${type}. Метки сети не поддерживаются версией "
"совместимости кластера, "
"но настроены на интерфейсах хоста."

msgctxt "HOT_VM_INTERFACE_UPDATE_IS_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Updating the virtual machine interface while the "
"virtual machine is running is not supported for clusters of version "
"${clusterVersion}."
msgstr ""
"Невозможно ${action} ${type}. Обновление интерфейса виртуальной машины "
"во время её выполнения не поддерживается кластером версии ${clusterVersion}."

msgctxt "ACTION_TYPE_FAILED_VM_INTERFACE_TYPE_IS_NOT_SUPPORTED_BY_OS"
msgid ""
"Cannot ${action} ${type}. The network interface type is not compatible with "
"the selected operating system."
msgstr ""
"Невозможно ${action} ${type}. Тип сетевого интерфейса не совместим с "
"выбранной ОС."

msgctxt "ACTION_TYPE_FAILED_VM_INTERFACE_TYPE_NOT_MATCH_PROFILE"
msgid ""
"Cannot ${action} ${type}. The network interface type doesn't match the "
"profile. Virtual machine interface of type 'PCI Passthrough' should have "
"'Passthrough' profile and vice versa."
msgstr ""
"Невозможно ${action} ${type}. Тип сетевого интерфейса не совпадает с "
"профилем. Интерфейс виртуальной машины типа «транзитный PCI» должен "
"иметь «транзитный» профиль и наоборот."

msgctxt "CANNOT_PERFORM_HOT_UPDATE"
msgid ""
"Cannot ${action} ${type}. Updating some of the properties is not supported "
"while the interface is plugged into a running virtual machine. Please un-"
"plug the interface, update the properties, and then plug it back."
msgstr ""
"Невозможно ${action} ${type}. Обновление некоторых параметров не "
"поддерживается "
"если интерфейс подключен к выполняющейся ВМ. Отключите интерфейс, обновите "
"свойства и затем подключите его снова."

msgctxt "CANNOT_PERFORM_HOT_UPDATE_WITH_PORT_MIRRORING"
msgid ""
"Cannot ${action} ${type}. Update is not possible when 'Port Mirroring' is "
"set on the interface of a running virtual machine."
msgstr ""
"Невозможно ${action} ${type}. Обновление невозможно, если на интерфейсе "
"выполняющейся ВМ настроено зеркалирование портов."

msgctxt "PORT_MIRRORING_REQUIRES_NETWORK"
msgid "Cannot ${action} ${type}. 'Port Mirroring' setting requires a network."
msgstr ""
"Невозможно ${action} ${type}. Параметр «зеркалирование портов» требует "
"наличия сети."

msgctxt "ACTION_TYPE_FAILED_GUEST_OS_VERSION_IS_NOT_SUPPORTED"
msgid "Cannot ${action} ${type}. Guest OS version is not supported."
msgstr "Невозможно ${action} ${type}. Версия гостевой ОС не поддерживается."

msgctxt "HOT_PLUG_DISK_IS_NOT_VIRTIO"
msgid "Can plug only virtIO disks."
msgstr "Можно подключить только диски VirtIO."

msgctxt "HOT_PLUG_DISK_IS_NOT_UNPLUGGED"
msgid "Disk is already activated."
msgstr "Диск уже активирован."

msgctxt "HOT_UNPLUG_DISK_IS_NOT_PLUGGED"
msgid "Disk is already deactivated."
msgstr "Диск уже деактивирован."

msgctxt "CANNOT_ADD_FLOATING_DISK_WITH_PLUG_VM_SET"
msgid ""
"Cannot ${action} ${type}. A disk configured with the \"Activate\" setting "
"cannot be created as a floating disk."
msgstr ""
"Невозможно ${action} ${type}. Диск, настроенный с параметром «Активировать», "
"нельзя создать как «плавающий» диск."

msgctxt "ACTIVATE_DEACTIVATE_NIC_VM_STATUS_ILLEGAL"
msgid ""
"Cannot activate/deactivate VM Network Interface due to VM status. The VM "
"status must be Down or Up."
msgstr ""
"Невозможно активировать/деактивировать сетевой интерфейс ВМ из-за статуса "
"ВМ. ВМ должна иметь статус «Отключена» или «Запущена»."

msgctxt "ACTIVATE_DEACTIVATE_NETWORK_NOT_IN_VDS"
msgid ""
"The Network does not exist on the host the VM is running on.\n"
" Either add the Network to the Host or migrate the VM to a Host that has "
"this Network."
msgstr ""
"Сеть отсутствует на хосте, где выполняется ВМ.\n"
" Добавьте сеть на хост, либо перенесите ВМ на хост с сетью."

msgctxt "VM_CANNOT_RUN_FROM_DISK_WITHOUT_PLUGGED_DISK"
msgid ""
"Cannot ${action} ${type} without at least one active disk.\n"
"Please activate a disk and rerun the VM."
msgstr ""
"Невозможно ${action} ${type} если нет как минимум одного активного "
"диска.\n"
"Активируйте диск и перезапустите ВМ."

msgctxt "SHAREABLE_DISK_IS_NOT_SUPPORTED_FOR_DISK"
msgid ""
"Cannot ${action} ${type}. Disk cannot be shareable if it depends on a "
"snapshot. In order to share it, remove the disk's snapshots."
msgstr ""
"Невозможно ${action} ${type}. Диск нельзя сделать общим ресурсом, если он "
"зависит от снимка. Чтобы сделать диск общим, удалите снимки диска."

msgctxt "SHAREABLE_DISK_IS_NOT_SUPPORTED_BY_VOLUME_FORMAT"
msgid ""
"Cannot ${action} ${type}. Disk's volume format is not supported for "
"shareable disk."
msgstr ""
"Невозможно ${action} ${type}. Формат тома диска не поддерживается для "
"диска, который можно сделать общим."

msgctxt "ERROR_CANNOT_DETACH_DISK_WITH_SNAPSHOT"
msgid ""
"Cannot ${action} ${type}. The disk is already configured in a snapshot. In "
"order to detach it, remove the disk's snapshots."
msgstr ""
"Невозможно ${action} ${type}, диск уже настроен в снимке. Чтобы отсоединить "
"его, "
"удалите снимки диска."

msgctxt "DISK_IS_ALREADY_SHARED_BETWEEN_VMS"
msgid ""
"Cannot ${action} ${type}. Disk is shared between vms and cannot become "
"unshareable . Detach the disk from the rest of the vms it is attached to and "
"then update the disk to be unshareable."
msgstr ""
"Невозможно ${action} ${type}. Диск является общим для виртуальных машин, "
"и его нельзя сделать не общим ресурсом. Отсоедините диск от остальных ВМ, "
"к которым он присоединён, и затем измените его параметры общего доступа."

msgctxt "SCSI_GENERIC_IO_IS_NOT_SUPPORTED_FOR_IMAGE_DISK"
msgid ""
"Cannot ${action} ${type}. SCSI Generic IO is not supported for image disk."
msgstr ""
"Невозможно ${action} ${type}. SCSI Generic IO не поддерживается для диска "
"снимка."

msgctxt "SCSI_PASSTHROUGH_IS_NOT_SUPPORTED_FOR_READ_ONLY_DISK"
msgid ""
"Cannot ${action} ${type}. SCSI device pass-through is not supported for a "
"read-only disk."
msgstr ""
"Невозможно ${action} ${type}. «Промежуточное» устройство SCSI не "
"поддерживается для "
"дисков, доступных только для чтения."

msgctxt "VIRTIO_SCSI_INTERFACE_IS_NOT_AVAILABLE_FOR_CLUSTER_LEVEL"
msgid "Virtio-SCSI interface is only available on cluster level 3.3 or higher."
msgstr "Интерфейс Virtio-SCSI доступен только на кластере уровня 3.3 и выше."

msgctxt "CANNOT_PERFORM_ACTION_VIRTIO_SCSI_IS_DISABLED"
msgid "Cannot ${action} ${type}. VirtIO-SCSI is disabled for the VM"
msgstr "Невозможно ${action} ${type}. VirtIO-SCSI отключён на ВМ."

msgctxt "CANNOT_DISABLE_VIRTIO_SCSI_PLUGGED_DISKS"
msgid ""
"Cannot disable VirtIO-SCSI when disks with a VirtIO-SCSI interface are "
"plugged into the VM."
msgstr ""
"Невозможно отключить VirtIO-SCSI, если к ВМ подключены диски с интерфейсом "
"VirtIO-SCSI."

msgctxt "USER_PASSWORD_EXPIRED"
msgid "Cannot Login. User Password has expired, Please change your password."
msgstr ""
"Невозможно выполнить вход. Срок действия пароля пользователя истёк. "
"Смените пароль."

msgctxt "USER_CANNOT_LOGIN_DOMAIN_NOT_SUPPORTED"
msgid ""
"Cannot Login. The Domain provided is not configured, please contact your "
"system administrator."
msgstr ""
"Невозможно выполнить вход. Указанный домен не настроен, свяжитесь с "
"системным администратором."

msgctxt "VM_POOL_CANNOT_DECREASE_VMS_FROM_POOL"
msgid "Cannot decrease VMs from VM-Pool."
msgstr "Невозможно уменьшить машины из пула ВМ"

msgctxt "MOVE_VM_CLUSTER_MISSING_NETWORK"
msgid ""
"Cannot ${action} ${type}. Selected Cluster is missing one or more networks "
"${networks} that is used by VM."
msgstr ""
"Невозможно ${action} ${type}. На выбранном кластере отсутствует одна или "
"более "
"сетей ${networks}, используемых ВМ."

msgctxt ""
"ACTION_TYPE_FAILED_STORAGE_POOL_WITH_DEFAULT_VDS_GROUP_CANNOT_BE_LOCALFS"
msgid ""
"Default Cluster cannot be moved to the Data Center that has local Storage."
msgstr ""
"Кластер по умолчанию нельзя перенести в дата-центр, где есть локальное "
"хранилище."

msgctxt "DEFAULT_CLUSTER_CANNOT_BE_ON_LOCALFS"
msgid ""
"Data Center containing the default Cluster does not support local Storage."
msgstr ""
"Дата-центр, содержащий кластер по умолчанию, не поддерживает локальное "
"хранилище."

msgctxt "VM_CANNOT_CANCEL_MIGRATION_WHEN_VM_IS_NOT_MIGRATING"
msgid "Cannot cancel migration for non migrating VM."
msgstr "Невозможно отменить миграцию для ВМ без возможности миграции."

msgctxt "VM_CANNOT_CANCEL_CONVERSION_WHEN_VM_IS_NOT_BEING_CONVERTED"
msgid "Cannot cancel conversion for non converted VM."
msgstr "Невозможно отменить преобразование для непреобразованной ВМ."

msgctxt "ACTION_TYPE_FAILED_VM_SNAPSHOT_NOT_IN_PREVIEW"
msgid ""
"Cannot ${action} ${type} to a Snapshot that is not being previewed. Please "
"select the correct Snapshot to restore to: Either the one being previewed, "
"or the one before the preview."
msgstr ""
"Невозможно ${action} ${type} для снимка, если в данный момент не производится "
"его предварительный просмотр. Выберите корректный снимок, до которого "
"необходимо "
"восстановиться: либо тот, который в данный момент просматривается, либо "
"снимок до просмотра."

msgctxt "ACTION_TYPE_FAILED_SHAREABLE_DISK_NOT_SUPPORTED"
msgid ""
"Cannot ${action} a shareable ${type} (${diskAliases}). This operation is not "
"supported."
msgstr ""
"Невозможно ${action} ${type} (${diskAliases}), который можно сделать общим. "
"Эта операция не поддерживается."

msgctxt "ACTION_TYPE_FAILED_DISK_NOT_EXIST"
msgid "Cannot ${action} ${type}. The specified disk does not exist."
msgstr "Невозможно ${action} ${type}. Указанный диск не существует."

msgctxt "ACTION_TYPE_FAILED_DISKS_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. The following disk(s) ID(s) does not exist: "
"${diskIds}."
msgstr ""
"Невозможно ${action} ${type}. Следующие ID дисков не существуют: ${diskIds}."

msgctxt "ACTION_TYPE_FAILED_DISK_SNAPSHOTS_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. The following disk snapshot(s) ID(s) does not "
"exist: ${diskSnapshotIds}."
msgstr ""
"Невозможно ${action} ${type}. Следующие снимки дисков не "
"существуют: ${diskSnapshotIds}."

msgctxt "ACTION_TYPE_FAILED_DISK_SNAPSHOTS_ACTIVE"
msgid ""
"Cannot ${action} ${type}. The following disk snapshot(s) is active: "
"${diskSnapshotIds}."
msgstr ""
"Невозможно ${action} ${type}. Следующие снимки дисков являются активными: "
"${diskSnapshotIds}."

msgctxt "ACTION_TYPE_FAILED_NO_DISKS_SPECIFIED"
msgid "Cannot ${action} ${type}. No disks have been specified."
msgstr "Невозможно ${action} ${type}. Диски не были указаны."

msgctxt "ACTION_TYPE_FAILED_CANT_MOVE_SHAREABLE_DISK_TO_GLUSTERFS"
msgid ""
"Cannot ${action} ${type}. ${diskAlias} is shareable, which Gluster domains "
"do not support."
msgstr ""
"Невозможно ${action} ${type}. ${diskAlias} может быть общим ресурсом, что "
"не поддерживается доменами Gluster."

msgctxt "ACTION_TYPE_FAILED_DISK_IS_NOT_VM_DISK"
msgid ""
"Cannot ${action} ${type}. The following disk(s) are not attached to any VM: "
"${diskAliases}."
msgstr ""
"Невозможно ${action} ${type}. Следующие диски не присоединены ни к одной "
"ВМ: ${diskAliases}."

msgctxt "ACTION_TYPE_FAILED_SOURCE_AND_TARGET_SAME"
msgid ""
"Cannot ${action} ${type}. The source and target storage domains are the same."
msgstr ""
"Невозможно ${action} ${type}. Исходный и целевой домены хранилищ — одно и то "
"же."

msgctxt "ACTION_TYPE_FAILED_BASE_TEMPLATE_DOES_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. Base Template does not exist for this Template "
"Version."
msgstr ""
"Невозможно ${action} ${type}. Для этой версии шаблона не существует базового "
"шаблона."

msgctxt "ACTION_TYPE_FAILED_NOT_A_VM_NETWORK"
msgid ""
"Failed ${action} ${type}. The following networks (${networks}) are not "
"defined as VM networks."
msgstr ""
"Сбой ${action} ${type}. Следующие сети не определены как сети ВМ: ${networks}"

msgctxt "ACTION_TYPE_FAILED_NETWORK_NOT_IN_CLUSTER"
msgid ""
"Failed ${action} ${type}. The following networks (${networks}) are not "
"defined in the cluster."
msgstr ""
"Сбой ${action} ${type}. Следующие сети не определены в кластере: ${networks}."

msgctxt "ACTION_TYPE_FAILED_INTERFACE_NETWORK_NOT_CONFIGURED"
msgid ""
"Failed ${action} ${type}. One or more network interfaces have incomplete "
"network configuration. Please configure these interfaces and try again."
msgstr ""
"Сбой ${action} ${type}. Один или больше сетевых интерфейсов имеют неполную "
"конфигурацию сети. Настройте эти интерфейсы и повторите попытку."

msgctxt "NON_VM_NETWORK_NOT_SUPPORTED_FOR_POOL_LEVEL"
msgid "Non-VM networks are not supported in this Data-Center."
msgstr "Этот дата-центр поддерживает только ВМ сети."

msgctxt "GLUSTER_NETWORK_NOT_SUPPORTED_FOR_POOL_LEVEL"
msgid ""
"Gluster networks are not supported in this cluster compatibility version."
msgstr "Сети Gluster не поддерживаются этой версией совместимости кластера."

msgctxt "VMPAYLOAD_INVALID_PAYLOAD_TYPE"
msgid "VM Payload only supported in CDROM or Floppy devices"
msgstr "Полезная нагрузка ВМ поддерживается только на устройствах CD и флоппи."

msgctxt "VMPAYLOAD_SIZE_EXCEEDED"
msgid "Payload is limited to ${size}K"
msgstr "Полезная нагрузка ограничена ${size}к"

msgctxt "VMPAYLOAD_FLOPPY_EXCEEDED"
msgid "Payload Floppy cannot be used when using an additional Floppy"
msgstr ""
"Гибкий диск с полезной нагрузкой нельзя использовать, если "
"используется дополнительный гибкий диск."

msgctxt "VMPAYLOAD_FLOPPY_WITH_SYSPREP"
msgid "Payload floppy deivce cannot be used with Sysprep via floppy device."
msgstr ""
"Устройство гибкого диска с полезной нагрузкой нельзя использовать с Sysprep "
"на флоппи."

msgctxt "VMPAYLOAD_CDROM_WITH_CLOUD_INIT"
msgid "Payload cdrom deivce cannot be used with Cloud-Init via cdrom device."
msgstr ""
"Устройство CDrom с полезной нагрузкой нельзя использовать с Cloud-Init на "
"CDrom."

msgctxt "VMPAYLOAD_CDROM_OR_CLOUD_INIT_MAXIMUM_DEVICES"
msgid ""
"CD-ROM Payload or Cloud-init is being used while there are too many devices "
"using the IDE or the sPAPR VSCSI bus."
msgstr ""
"CD-ROM с полезной нагрузкой или с Cloud-init используется, если большое "
"количество "
"устройств работают с шиной IDE или sPAPR VSCSI."

msgctxt "ERROR_GET_STORAGE_DOMAIN_LIST"
msgid "Cannot get Storage Domains list."
msgstr "Невозможно получить список доменов хранилища"

msgctxt "ERROR_GET_IMAGE_LIST"
msgid ""
"Cannot get list of images from Storage Domain '${sdName}'. Please try again "
"later."
msgstr ""
"Не удалось получить список образов из домена хранилища «${sdName}». "
"Повторите попытку позже."

msgctxt "ACTION_TYPE_FAILED_VDS_CLUSTER_DIFFERENT_ARCHITECTURES"
msgid ""
"Cannot ${action} ${type}. The host and the destination cluster architectures "
"do not match."
msgstr ""
"Невозможно ${action} ${type}. Архитектуры хоста и целевого кластера не "
"совпадают."

msgctxt "ACTION_TYPE_FAILED_VM_CANNOT_BE_PINNED_TO_CPU_AND_MIGRATABLE"
msgid "Migratable VM's cannot be pinned to CPU."
msgstr "ВМ с возможностью миграции не могут быть закреплены за ЦПУ"

msgctxt "ACTION_TYPE_FAILED_VM_CANNOT_BE_PINNED_TO_CPU_WITH_UNDEFINED_HOST"
msgid "Cannot set host CPU pinning when host is not selected"
msgstr "Невозможно создать закрепление хоста за ЦПУ, если хост не выбран"

msgctxt "ACTION_TYPE_FAILED_NETWORK_INTERFACE_MAC_INVALID"
msgid ""
"Cannot ${action} ${type}. The Network Interface ${IfaceName} has an invalid "
"MAC address ${MacAddress}. MAC address must be in format \"HH:HH:HH:HH:HH:"
"HH\" where H is a hexadecimal character (either a digit or A-F, case is "
"insignificant)."
msgstr ""
"Невозможно ${action} ${type}. У сетевого интерфейса ${IfaceName} недопустимый "
"MAC адрес ${MacAddress}. MAC адрес должен быть в формате «HH:HH:HH:HH:HH:HH», "
"где H — шестнадцатеричный символ (цифра или буква от A до F, регистр не "
"важен)."

msgctxt "MIGRATE_PAUSED_EIO_VM_IS_NOT_SUPPORTED"
msgid "Migrating a VM in paused status due to I/O error is not supported."
msgstr ""
"Перенос ВМ, имеющей статус «приостановлена» в связи с ошибкой "
"ввода/вывода, не поддерживается."

msgctxt "VDS_GROUP_ENABLING_BOTH_VIRT_AND_GLUSTER_SERVICES_NOT_ALLOWED"
msgid ""
"Cannot ${action} ${type}. Enabling both Virt and Gluster services is not "
"allowed."
msgstr ""
"Невозможно ${action} ${type}. Одновременное включение служб Virt и Gluster "
"запрещено."

msgctxt "VM_INTERFACE_NOT_EXIST"
msgid "Cannot ${action} ${type}. The VM Network Interface does not exist."
msgstr "Невозможно ${action} ${type}. Сетевой интерфейс ВМ не существует."

msgctxt "HOST_NETWORK_INTERFACE_NOT_EXIST"
msgid "Cannot ${action} ${type}. The host network interface does not exist."
msgstr "Невозможно ${action} ${type}. Сетевой интерфейс хоста не существует."

msgctxt "ACTION_TYPE_FAILED_CANNOT_REMOVE_ACTIVE_DEVICE"
msgid ""
"Cannot ${action} ${type}. The VM Network Interface is plugged to a running "
"VM."
msgstr ""
"Невозможно ${action} ${type}. Сетевой интерфейс подключен к выполняющейся ВМ."

msgctxt "ACTION_TYPE_FAILED_IMPORT_CLONE_NOT_COLLAPSED"
msgid ""
"Cannot ${action} ${type}. The VM ${VmName} has snapshots that must be "
"collapsed."
msgstr ""
"Невозможно ${action} ${type}. У машины ${VmName} есть снимки, которые нужно "
"свернуть."

msgctxt "ACTION_TYPE_FAILED_IMPORT_UNREGISTERED_NOT_COLLAPSED"
msgid "Cannot ${action} ${type}. Unregistered VM can not be collapsed."
msgstr "Невозможно ${action} ${type}. Незарегистрированную ВМ нельзя свернуть."

msgctxt "USER_CANNOT_FORCE_RECONNECT_TO_VM"
msgid ""
"Console connection denied. Another user has already accessed the console of "
"this VM. The VM should either be rebooted to allow another user to access "
"it, or changed by an admin to not enforce a reboot between users accessing "
"its console."
msgstr ""
"В доступе к консоли отказано. Доступ к консоли этой ВМ уже получил другой "
"пользователь. "
"Чтобы дать другому пользователю доступ к консоли, эту ВМ нужно либо "
"перезагрузить, "
"либо администратор должен отменить принудительную перезагрузку между "
"доступами "
"пользователей к консоли."

msgctxt "ENGINE_IS_RUNNING_IN_MAINTENANCE_MODE"
msgid "Engine is running in Maintenance mode and is not accepting commands."
msgstr "ЦУ работает в режиме обслуживания и не принимает команд."

msgctxt "ENGINE_IS_RUNNING_IN_PREPARE_MODE"
msgid "This action is not allowed when Engine is preparing for maintenance."
msgstr "Это действие не разрешено, если ЦУ подготавливается к обслуживанию."

msgctxt "SERVER_ALREADY_EXISTS_IN_ANOTHER_CLUSTER"
msgid "One or more servers are already part of an existing cluster."
msgstr "Один или более серверов уже являются частью существующего кластера."

msgctxt "SSH_AUTHENTICATION_FAILED"
msgid "SSH Authentication failed. Please make sure password is correct."
msgstr "Сбой SSH аутентификации. Убедитесь в правильности пароля."

msgctxt "GLUSTER_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Gluster service is not supported in compatibility "
"version ${compatibilityVersion}."
msgstr ""
"Невозможно ${action} ${type}. Служба Gluster не поддерживается версией "
"совместимости ${compatibilityVersion}."

msgctxt "GLUSTER_VOLUME_ID_INVALID"
msgid "Volume id is invalid."
msgstr "Недопустимый ID тома"

msgctxt "ACTION_TYPE_FAILED_CLUSTER_IS_NOT_VALID"
msgid "Cannot ${action} ${type}. Cluster ID is not valid."
msgstr "Невозможно ${action} ${type}. Недопустимый ID кластера."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_DOES_NOT_SUPPORT_GLUSTER"
msgid "Cannot ${action} ${type}. Cluster does not support Gluster service."
msgstr "Невозможно ${action} ${type}. Кластер не поддерживает службу Gluster."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_NAME_ALREADY_EXISTS"
msgid "Cannot ${action} ${type}. Volume name ${volumeName} already exists."
msgstr "Невозможно ${action} ${type}. Имя тома ${volumeName} уже существует."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_INVALID"
msgid "Cannot ${action} ${type}. Gluster Volume is not valid."
msgstr "Невозможно ${action} ${type}. Недопустимый том Gluster."

msgctxt "ACTION_TYPE_FAILED_BRICKS_REQUIRED"
msgid "Cannot ${action} ${type}. Bricks are required."
msgstr "Невозможно ${action} ${type}. Требуются элементы"

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_ADD_BRICK_FORCE_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Adding bricks with 'Force' option is not supported."
msgstr ""
"Невозможно ${action} ${type}. Добавление элементов с параметром "
"«принудительно» "
"не поддерживается."

msgctxt "ACTION_TYPE_FAILED_REPLICA_COUNT_MIN_2"
msgid ""
"Cannot ${action} ${type}. Replica count must be > 2 for a REPLICATE volume."
msgstr ""
"Невозможно ${action} ${type}. Число реплик для реплицируемого тома "
"должно быть > 2."

msgctxt "ACTION_TYPE_FAILED_INVALID_BRICK_COUNT_FOR_DISTRIBUTED_REPLICATE"
msgid ""
"Cannot ${action} ${type}. Number of bricks must be a multiple of replica "
"count for a DISTRIBUTED REPLICATE volume."
msgstr ""
"Невозможно ${action} ${type}. Число элементов для распределённого и "
"реплицируемого тома должно быть кратным числу реплик."

msgctxt "ACTION_TYPE_FAILED_INVALID_BRICK_COUNT_FOR_REPLICATE"
msgid ""
"Cannot ${action} ${type}. Number of bricks must be equal to the replica "
"count for a REPLICATED volume."
msgstr ""
"Невозможно ${action} ${type}. Число элементов должно быть равным число реплик "
"для тома REPLICATED."

msgctxt "ACTION_TYPE_FAILED_STRIPE_COUNT_MIN_4"
msgid ""
"Cannot ${action} ${type}. Stripe count must be > 4 for a STRIPED volume."
msgstr ""
"Невозможно ${action} ${type}. Число чередований должно быть > 4 для "
"чередующегося тома (STRIPED)"

msgctxt "ACTION_TYPE_FAILED_INVALID_BRICK_COUNT_FOR_STRIPE"
msgid ""
"Cannot ${action} ${type}. Number of bricks must be equal to stripe count for "
"a STRIPE volume."
msgstr ""
"Невозможно ${action} ${type}. Для разделённого по частям тома число элементов "
"должно быть равно числу частей."

msgctxt "ACTION_TYPE_FAILED_INVALID_BRICK_COUNT_FOR_DISTRIBUTED_STRIPE"
msgid ""
"Cannot ${action} ${type}. Number of bricks must be a multiple of stripe "
"count for a DISTRIBUTED STRIPE volume."
msgstr ""
"Невозможно ${action} ${type}. Для распределённого по частям тома число "
"элементов должно быть кратным числу частей."

msgctxt "ACTION_TYPE_FAILED_INVALID_BRICK_COUNT_FOR_STRIPED_REPLICATE"
msgid ""
"Cannot ${action} ${type}. Number of bricks must be multiple of stripe count "
"and replicate count for a STRIPED REPLICATE volume."
msgstr ""
"Невозможно ${action} ${type}. Для реплицируемого по частям тома число "
"элементов "
"должно быть кратным числу частей."

msgctxt ""
"ACTION_TYPE_FAILED_INVALID_BRICK_COUNT_FOR_DISTRIBUTED_STRIPED_REPLICATE"
msgid ""
"Cannot ${action} ${type}. Number of bricks must be a non-trivial multiple of "
"stripe count and replicate count for a DISTRIBUTED STRIPED REPLICATE volume."
msgstr ""
"Невозможно ${action} ${type}. Для распределенного по частям и реплицируемого "
"тома "
"число элементов должно быть нетривиальным кратным числу частей и числу реплик."

msgctxt "ACTION_TYPE_FAILED_INVALID_BRICK_SERVER_ID"
msgid "Cannot ${action} ${type}. Invalid host id in brick."
msgstr "Невозможно ${action} ${type}. Недопустимый ID хоста в элементе."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_ALREADY_STARTED"
msgid "Cannot ${action} ${type}. Gluster Volume ${volumeName} already started."
msgstr "Невозможно ${action} ${type}. Том Gluster ${volumeName} уже запущен."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_ALREADY_STOPPED"
msgid "Cannot ${action} ${type}. Gluster Volume ${volumeName} already stopped."
msgstr ""
"Невозможно ${action} ${type}. Том Gluster ${volumeName} уже остановлен."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_IS_UP"
msgid "Cannot ${action} ${type}. Gluster Volume ${volumeName} is up."
msgstr "Невозможно ${action} ${type}. Том Gluster ${volumeName} работает."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_HAS_SNAPSHOTS"
msgid ""
"Cannot ${action} ${type}. Gluster Volume ${volumeName} has ${noOfSnapshots} "
"snapshots.\n"
"To delete the volume, first delete all the snapshots under it."
msgstr ""
"Невозможно ${action} ${type}. Том Gluster ${volumeName} имеет "
"${noOfSnapshots} "
"снимков.\n"
"Чтобы удалить том, сначала удалите все снимки."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_IS_DOWN"
msgid "Cannot ${action} ${type}. Gluster Volume is down."
msgstr "Невозможно ${action} ${type}. Том Gluster не работает."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_IS_NOT_THINLY_PROVISIONED"
msgid "Cannot ${action} ${type}. Gluster volume is not thinly provisioned."
msgstr "Невозможно ${action} ${type}. Том Gluster не имеет тонкой подготовки."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_SNAPSHOT_CONFIG_PARAMS_IS_EMPTY"
msgid ""
"Cannot ${action} ${type}. No gluster volume snapshot parameters for update."
msgstr ""
"Невозможно ${action} ${type}. Нет параметров снимка тома Gluster для "
"обновления."

msgctxt ""
"ACTION_TYPE_FAILED_GLUSTER_VOLUME_SNAPSHOT_CONFIG_PARAM_VALUE_IS_EMPTY"
msgid ""
"Cannot ${action} ${type}. The value of gluster volume snapshot parameter "
"${snapshotConfigParam} is empty."
msgstr ""
"Невозможно ${action} ${type}. Пустое значение параметра снимка тома gluster "
"${snapshotConfigParam}."

msgctxt "ACTION_TYPE_FAILED_START_DATE_BEFORE_CURRENT_DATE"
msgid ""
"Cannot ${action} ${type}. The value of start date is before current date."
msgstr ""
"Невозможно ${action} ${type}. Дата начала имеет значение раньше текущей даты."

msgctxt "ACTION_TYPE_FAILED_END_BY_DATE_BEFORE_CURRENT_DATE"
msgid "Cannot ${action} ${type}. The value of end date is before current date."
msgstr ""
"Невозможно ${action} ${type}. Даты окончания имеет значение раньше текущей "
"даты."

msgctxt "ACTION_TYPE_FAILED_END_BY_DATE_BEFORE_START_DATE"
msgid ""
"Cannot ${action} ${type}. The value of end date cannot be before start date."
msgstr ""
"Невозможно ${action} ${type}. Значение даты окончания не может быть раньше "
"даты начала."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_REMOVE_ALL_BRICKS_FROM_VOLUME"
msgid "Cannot ${action} ${type}. Cannot remove all the bricks from a Volume."
msgstr "Невозможно ${action} ${type}. Нельзя удалить все элементы из тома."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_SHOULD_BE_STARTED"
msgid "Cannot ${action} ${type}. Gluster Volume should be started."
msgstr "Невозможно ${action} ${type}. Должен быть запущен том Volume."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_NOT_DISTRIBUTED"
msgid "Cannot ${action} ${type}. Gluster Volume is not distributed."
msgstr "Невозможно ${action} ${type}. Том Gluster не является распределённым."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_DISTRIBUTED_AND_HAS_SINGLE_BRICK"
msgid "Cannot ${action} ${type}. Gluster Volume has a single brick."
msgstr "Невозможно ${action} ${type}. Том Gluster имеет только один элемент."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_REMOVE_BRICKS_NOT_STARTED"
msgid "Cannot ${action} ${type}. Remove brick not started."
msgstr "Невозможно ${action} ${type}. Удаление элементов не началось."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_REMOVE_BRICKS_NOT_FINISHED"
msgid "Cannot ${action} ${type}. Remove brick not finished."
msgstr "Невозможно ${action} ${type}. Удаление элементов не закончилось."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_REMOVE_BRICKS_PARAMS_INVALID"
msgid ""
"Cannot ${action} ${type}. The list of bricks does not match with the bricks "
"used while starting the action. Valid bricks: ${validBricks}"
msgstr ""
"Невозможно ${action} ${type}. Список элементов не совпадает с элементами, "
"используемыми во время начала операции. Корректные элементы: ${validBricks}"

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_INVALID_TASK_TYPE"
msgid "Cannot ${action} ${type}. Invalid task type."
msgstr "Невозможно ${action} ${type}. Некорректный тип задания."

msgctxt "ACTION_TYPE_FAILED_NOT_A_GLUSTER_VOLUME_BRICK"
msgid ""
"Cannot ${action} ${type}. Replacing brick is not a Gluster volume brick."
msgstr ""
"Невозможно ${action} ${type}. Заменяемый элемент не является элементом "
"тома Gluster."

msgctxt "VDS_CANNOT_REMOVE_HOST_HAVING_GLUSTER_VOLUME"
msgid "Cannot ${action} ${type}. Server having Gluster volume."
msgstr "Невозможно ${action} ${type}. На сервере имеется том Gluster"

msgctxt "CPU_TYPE_UNSUPPORTED_IN_THIS_CLUSTER_VERSION"
msgid ""
"Host CPU type is not supported in this cluster compatibility version or is "
"not supported at all."
msgstr ""
"Тип ЦПУ хоста не поддерживается версией совместимости кластера "
"или не поддерживается совсем."

msgctxt "ACTION_TYPE_FAILED_NO_GLUSTER_HOST_TO_PEER_PROBE"
msgid ""
"Cannot ${action} ${type}. There is no available server in the cluster to "
"probe the new server."
msgstr ""
"Невозможно ${action} ${type}. На кластере нет доступного сервера для "
"пробы (peer probe) нового сервера."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_BRICK_INVALID"
msgid "Cannot ${action} ${type}. Invalid gluster brick."
msgstr "Невозможно ${action} ${type}. Недопустимый элемент gluster."

msgctxt "ACTION_TYPE_FAILED_BRICK_ID_REQUIRED"
msgid "Cannot ${action} ${type}. Brick ID is required."
msgstr "Невозможно ${action} ${type}. Требуется ID элемента."

msgctxt "ACTION_TYPE_FAILED_ONE_OR_MORE_BRICKS_ARE_DOWN"
msgid "Cannot ${action} ${type}. One or more bricks are down."
msgstr "Невозможно ${action} ${type}. Один или более элементов отключены."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_REDUCE_REPLICA_COUNT_MORE_THAN_ONE"
msgid ""
"Cannot ${action} ${type}. Replica count cannot be reduced by more than one."
msgstr ""
"Невозможно ${action} ${type}. Число реплик не может быть уменьшено более, чем "
"на единицу."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_REDUCE_REPLICA_COUNT_WITH_DATA_MIGRATION"
msgid ""
"Cannot ${action} ${type}. Data migration is not needed while reducing the "
"replica count."
msgstr ""
"Невозможно ${action} ${type}. При уменьшении количества реплик перенос данных "
"не требуется."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_INCREASE_REPLICA_COUNT"
msgid ""
"Cannot ${action} ${type}. Replica count cannot be increased when removing "
"bricks."
msgstr ""
"Невозможно ${action} ${type}. Число реплик не может быть увеличено во время "
"удаления "
"элементов."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_REDUCE_REPLICA_COUNT"
msgid ""
"Cannot ${action} ${type}. Replica count cannot be reduced when adding bricks."
msgstr ""
"Невозможно ${action} ${type}. Число реплик не может быть уменьшено во время "
"добавления элементов."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_INCREASE_REPLICA_COUNT_MORE_THAN_ONE"
msgid ""
"Cannot ${action} ${type}. Replica count cannot be increased by more than one."
msgstr ""
"Невозможно ${action} ${type}. Число реплик не может быть увеличено более, чем "
"на единицу."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_INCREASE_STRIPE_COUNT_MORE_THAN_ONE"
msgid ""
"Cannot ${action} ${type}. Stripe count cannot be increased by more than one."
msgstr ""
"Невозможно ${action} ${type}. Число чередований не может быть увеличено "
"более, чем на единицу."

msgctxt "ACTION_TYPE_FAILED_CAN_NOT_REDUCE_STRIPE_COUNT"
msgid "Cannot ${action} ${type}. Stripe count can not be reduced."
msgstr ""
"Невозможно ${action} ${type}. Число чередований не может быть уменьшено."

msgctxt "ACTION_TYPE_FAILED_DUPLICATE_BRICKS"
msgid "Cannot ${action} ${type}. Duplicate entries found for brick ${brick}."
msgstr ""
"Невозможно ${action} ${type}. Найдены дублирующие компоненты для элемента "
"${brick}."

msgctxt "ACTION_TYPE_FAILED_ADD_BRICK_TO_DISPERSE_VOLUME_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Addition of bricks to disperse volume is not "
"supported."
msgstr ""
"Невозможно ${action} ${type}. Добавление элементов для дисперсии тома не "
"поддерживается."

msgctxt ""
"ACTION_TYPE_FAILED_ADD_BRICK_TO_DISTRIBUTED_DISPERSE_VOLUME_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Addition of bricks to distributed disperse volume "
"is not supported."
msgstr ""
"Невозможно ${action} ${type}. Добавление элементов в распределённый "
"дисперсный "
"том не поддерживается."

msgctxt "ACTION_TYPE_FAILED_REMOVE_BRICK_FROM_DISPERSE_VOLUME_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Removal of bricks from disperse volume is not "
"supported."
msgstr ""
"Невозможно ${action} ${type}. Удаление элементов из дисперсного "
"тома не поддерживается."

msgctxt ""
"ACTION_TYPE_FAILED_REMOVE_BRICK_FROM_DISTRIBUTED_DISPERSE_VOLUME_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Removal of bricks from distributed disperse volume "
"is not supported."
msgstr ""
"Невозможно ${action} ${type}. Удаление элементов из распределённого "
"дисперсного "
"тома не поддерживается."

msgctxt "ACTION_TYPE_FAILED_CREATION_OF_DISPERSE_VOLUME_NOT_SUPPORTED"
msgid "Cannot ${action} ${type}. Creation of disperse volume is not supported."
msgstr ""
"Невозможно ${action} ${type}. Создание дисперсного тома не поддерживается."

msgctxt "ACTION_TYPE_FAILED_BRICK_ALREADY_EXISTS_IN_VOLUME"
msgid ""
"Cannot ${action} ${type}. Brick ${brick} is already used by the volume "
"${volumeName}."
msgstr ""
"Невозможно ${action} ${type}. Элемент ${brick} уже используется томом "
"${volumeName}."

msgctxt "ACTION_TYPE_FAILED_NO_UP_SERVER_FOUND"
msgid "Cannot ${action} ${type}. No up server found in ${clusterName}."
msgstr ""
"Невозможно ${action} ${type}. На кластере ${clusterName} не найдено "
"запущенных серверов."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_HOOK_ID_IS_REQUIRED"
msgid "Cannot ${action} ${type}. Gluster hook id is required."
msgstr "Невозможно ${action} ${type}. Требуется ID ловушки Gluster."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_HOOK_DOES_NOT_EXIST"
msgid "Cannot ${action} ${type}. Gluster hook does not exist."
msgstr "Невозможно ${action} ${type}. Ловушка Gluster не существует."

msgctxt "SERVER_ALREADY_PART_OF_ANOTHER_CLUSTER"
msgid "Server ${server} is already part of another cluster."
msgstr "Сервер ${server} уже является частью другого кластера."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_HOOK_NO_CONFLICT_SERVERS"
msgid ""
"Cannot ${action} ${type}. There are no conflicting servers to add or update "
"hook."
msgstr ""
"Невозможно ${action} ${type}. Отсутствуют конфликтующие сервера, "
"для которых нужно обновить или добавить ловушку."

msgctxt "ACTION_TYPE_FAILED_SERVER_STATUS_NOT_UP"
msgid "Cannot ${action} ${type}. The server ${VdsName} is not UP."
msgstr "Невозможно ${action} ${type}. Сервер ${VdsName} не запущен."

msgctxt "CLUSTER_ALL_SERVERS_NOT_UP"
msgid "One or more servers in the cluster is down"
msgstr "Один или больше серверов кластера не запущены."

msgctxt "ACTION_TYPE_FAILED_VOLUME_OPERATION_IN_PROGRESS"
msgid ""
"Cannot ${action} ${type}. A task is in progress on the volume ${volumeName} "
"in cluster ${vdsGroup}."
msgstr ""
"Невозможно ${action} ${type}. На томе ${volumeName} кластера "
"${vdsGroup} выполняется задача."

msgctxt "ACTION_TYPE_FAILED_IMAGE_DOWNLOAD_ERROR"
msgid ""
"Cannot ${action} ${type}. The image content could not be detected. Please "
"try to re-import the image."
msgstr ""
"Невозможно ${action} ${type}. Содержимое образа не должно удаляться. "
"Попробуйте "
"повторно импортировать образ."

msgctxt "ACTION_TYPE_FAILED_IMAGE_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Only bare RAW and QCOW2 image formats are "
"supported."
msgstr ""
"Невозможно ${action} ${type}. Поддерживаются только форматы образов RAW и "
"QCOW2."

msgctxt "ACTION_TYPE_FAILED_IMAGE_UNRECOGNIZED"
msgid ""
"Cannot ${action} ${type}. Either the image is missing, or its format is "
"corrupted or unrecognizable."
msgstr ""
"Невозможно ${action} ${type}. Образ либо отсутствует, либо его формат "
"повреждён/его невозможно распознать."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_REBALANCE_NOT_STARTED"
msgid ""
"Cannot ${action} ${type}. Rebalance is not running on the volume "
"${volumeName} in cluster ${vdsGroup}."
msgstr ""
"Невозможно ${action} ${type}. Перебалансировка не запущена на томе "
"${volumeName} кластера ${vdsGroup}."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_CANNOT_STOP_REBALANCE_IN_PROGRESS"
msgid ""
"Cannot ${action} ${type}. Rebalance is running on the volume ${volumeName} "
"in cluster ${vdsGroup}."
msgstr ""
"Невозможно ${action} ${type}. Перебалансировка выполняется на томе "
"${volumeName} кластера ${vdsGroup}."

msgctxt ""
"ACTION_TYPE_FAILED_GLUSTER_VOLUME_CANNOT_STOP_REMOVE_BRICK_IN_PROGRESS"
msgid ""
"Cannot ${action} ${type}. Remove brick operation is running on the volume "
"${volumeName} in cluster ${vdsGroup}."
msgstr ""
"Невозможно ${action} ${type}. Операция удаления элементов выполняется на томе "
"${volumeName} кластера ${vdsGroup}."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_OPERATION_INPROGRESS"
msgid ""
"Cannot ${action} ${type}. Gluster operation is in progress in cluster. "
"Please try again."
msgstr ""
"Невозможно ${action} ${type}. На кластере выполняется операция Gluster. "
"Повторите попытку."

msgctxt "ACTION_TYPE_FAILED_GEOREP_SESSION_INVALID"
msgid "Cannot ${action} ${type}. Geo-replication session not found."
msgstr "Невозможно ${action} ${type}. Сеанс георепликации не найден."

msgctxt "ACTION_TYPE_FAILED_GEOREP_SESSION_LOCKED"
msgid ""
"Cannot ${action} ${type}. Another operation is in progress on this geo-"
"replication session. Please try again."
msgstr ""
"Невозможно ${action} ${type}. В этом сеансе георепликации выполняется "
"другая операция. Повторите попытку."

msgctxt "ACTION_TYPE_FAILED_GEOREP_SESSION_STOPPED"
msgid "Cannot ${action} ${type}. Geo-replication session is stopped."
msgstr "Невозможно ${action} ${type}. Сеанс георепликации остановлен."

msgctxt "ACTION_TYPE_FAILED_GEOREP_SESSION_ALREADY_STARTED"
msgid "Cannot ${action} ${type}. Geo-replication session is already started."
msgstr "Невозможно ${action} ${type}. Сеанс георепликации уже запущен."

msgctxt "ACTION_TYPE_FAILED_GEOREP_SESSION_ALREADY_PAUSED"
msgid "Cannot ${action} ${type}. Geo-replication session is paused."
msgstr "Невозможно ${action} ${type}. Сеанс георепликации приостановлен."

msgctxt "ACTION_TYPE_FAILED_VOLUME_SNAPSHOT_NOT_SUPPORTED"
msgid "Cannot ${action} ${type}. Gluster volume snapshot is not supported."
msgstr "Невозможно ${action} ${type}. Снимки тома Gluster не поддерживаются."

msgctxt "ACTION_TYPE_FAILED_REMOTE_CLUSTER_NOT_MAINTAINED_BY_ENGINE"
msgid "Cannot ${action} ${type}. Remote cluster is not maintained by engine."
msgstr "Невозможно ${action} ${type}. Удалённый кластер не управляется ЦУ."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_SNAPSHOT_NAME_IS_EMPTY"
msgid "Cannot ${action} ${type}. Gluster volume snapshot name is empty."
msgstr "Невозможно ${action} ${type}. Пустое имя снимка тома Gluster."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_SNAPSHOT_DOES_NOT_EXIST"
msgid ""
"Cannot ${action} ${type}. Gluster volume snapshot ${snapname} does not exist."
msgstr ""
"Невозможно ${action} ${type}. Снимок тома Gluster ${snapname} не существует."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_NO_SNAPSHOTS_EXIST"
msgid ""
"Cannot ${action} ${type}. No gluster volume snapshots exist for volume "
"${volumeName}."
msgstr ""
"Невозможно ${action} ${type}. Для тома ${volumeName} не существует снимков "
"тома Gluster."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_SNAPSHOT_ALREADY_ACTIVATED"
msgid ""
"Cannot ${action} ${type}. Gluster volume snapshot ${snapname} is already "
"activated."
msgstr ""
"Невозможно ${action} ${type}. Снимок тома Gluster ${snapname} уже активирован."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_VOLUME_SNAPSHOT_ALREADY_DEACTIVATED"
msgid ""
"Cannot ${action} ${type}. Gluster volume snapshot ${snapname} is already de-"
"activated."
msgstr ""
"Невозможно ${action} ${type}. Снимок тома Gluster ${snapname} уже отключён."

msgctxt "GLUSTER_TASKS_NOT_SUPPORTED_FOR_CLUSTER_LEVEL"
msgid ""
"Cannot ${action} ${type}. Gluster task management is not supported in "
"compatibility version ${compatibilityVersion}."
msgstr ""
"Невозможно ${action} ${type}. Управление задачами Gluster не поддерживается "
"версией совместимости ${compatibilityVersion}."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DEVICE_LOCKED"
msgid "Cannot ${action} ${type}. Storage Device is locked."
msgstr "Невозможно ${action} ${type}. Устройство хранения заблокировано."

msgctxt "ACTION_TYPE_FAILED_STORAGE_DEVICE_REQUIRED"
msgid "Cannot ${action} ${type}. At least one storage device is required."
msgstr ""
"Невозможно ${action} ${type}. Требуется минимум одно устройство хранения."

msgctxt "ACTION_TYPE_FAILED_DIFFERENT_STORAGE_DEVICE_TYPES_SELECTED"
msgid ""
"Cannot ${action} ${type}. Different types of storage devices are selected."
msgstr "Невозможно ${action} ${type}. Выбраны разные типы устройств хранения."

msgctxt "ACTION_TYPE_FAILED_DEVICE_IS_ALREADY_IN_USE"
msgid ""
"Cannot ${action} ${type}. Storage Device ${storageDevice} is already in use."
msgstr ""
"Невозможно ${action} ${type}. Устройство хранения ${storageDevice} уже "
"используется."

msgctxt "ACTION_TYPE_FAILED_VOLUME_ASYNC_OPERATION_IN_PROGRESS"
msgid ""
"Cannot ${action} ${type}. A ${asyncTask} operation is in progress on the "
"volume ${volumeName} in cluster ${vdsGroup}."
msgstr ""
"Невозможно ${action} ${type}. Операция ${asyncTask} выполняется на томе "
"${volumeName} кластера ${vdsGroup}."

msgctxt "ACTION_TYPE_FAILED_GLUSTER_CLI_SCHEDULING_ENABLED"
msgid "Cannot ${action} ${type}. Gluster CLI based scheduling is enabled."
msgstr "Невозможно ${action} ${type}. Включено планирование Gluster CLI."

msgctxt "ACTION_TYPE_FAIL_VDS_CANNOT_CONNECT_TO_GLUSTERFS"
msgid ""
"Cannot ${action} ${type}. Host ${VdsName} cannot connect to Glusterfs. "
"Verify that glusterfs-cli package is installed on the host."
msgstr ""
"Невозможно ${action} ${type}. Хост ${VdsName} не может подключиться к "
"Glusterfs. "
"Убедитесь, что на хосте установлен пакет glusterfs-cli."

msgctxt "ACTION_TYPE_FAILED_TAG_ID_REQUIRED"
msgid "Cannot ${action} ${type}. Tag ID is required."
msgstr "Невозможно ${action} ${type}. Требуется ID тега."

msgctxt "ACTION_TYPE_FAILED_QOS_OUT_OF_RANGE_VALUES"
msgid ""
"Cannot ${action} ${type}. Values are out of range\n"
"(Legal range is: ${range})."
msgstr ""
"Невозможно ${action} ${type}. Значения вне диапазона.\n"
"(Допустимый диапазон: ${range})"

msgctxt "ACTION_TYPE_FAILED_QOS_STORAGE_POOL_NOT_EXIST"
msgid "Cannot ${action} ${type}. Invalid data center."
msgstr "Невозможно ${action} ${type}. Недопустимый дата-центр."

msgctxt "ACTION_TYPE_FAILED_QOS_STORAGE_POOL_NOT_CONSISTENT"
msgid "Cannot ${action} ${type}. Cannot change QoS data center."
msgstr "Невозможно ${action} ${type}. Невозможно изменить дата-центр QoS."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_MISSING_VALUES"
msgid ""
"Cannot ${action} ${type}. All three values are needed in order to define QoS "
"on each network directions."
msgstr ""
"Невозможно ${action} ${type}. Все три значения требуются для определения "
"QoS на каждом направлении сети."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_NEGATIVE_VALUES"
msgid "Cannot ${action} ${type}. Negative values are not allowed."
msgstr "Невозможно ${action} ${type}. Отрицательные значения запрещены."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_NAME_EXIST"
msgid "Cannot ${action} ${type}. Duplicate QoS name in Data Center."
msgstr "Невозможно ${action} ${type}. Дублирующее имя QoS в дата-центре."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_MISSING_DATA"
msgid "Cannot ${action} ${type}. Invalid QoS."
msgstr "Невозможно ${action} ${type}. Недопустимый QoS."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_NOT_FOUND"
msgid "Cannot ${action} ${type}. QoS entity not found."
msgstr "Невозможно ${action} ${type}. Не найден объект QoS."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_INVALID_DC_ID"
msgid ""
"Cannot ${action} ${type}. Data Center does not contain the specific QoS "
"entity."
msgstr ""
"Невозможно ${action} ${type}. Дата-центр не содержит указанный объект QoS."

msgctxt "ACTION_TYPE_FAILED_NETWORK_QOS_PEAK_LOWER_THAN_AVERAGE"
msgid "Cannot ${action} ${type}. Peak cannot be set lower than Average."
msgstr ""
"Невозможно ${action} ${type}. Пиковая нагрузка не может быть настроена "
"ниже, чем средняя."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_QOS_MISSING_VALUES"
msgid ""
"Cannot ${action} ${type}. Weighted share must be specified to complete QoS "
"configuration."
msgstr ""
"Невозможно ${action} ${type}. Для окончательного определения параметров QoS "
"необходимо указать взвешенную долю."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_QOS_SETUP_NETWORKS_MISSING_VALUES"
msgid ""
"Cannot ${action} ${type}. Weighted share must be specified to complete QoS "
"configuration, but the following network(s) are missing it: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_SETUP_NETWORKS_MISSING_VALUES_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Для окончательного определения параметров QoS "
"необходимо указать взвешенную долю, но она отсутствует для следующих сетей: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_SETUP_NETWORKS_MISSING_VALUES_LIST}."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INCONSISTENT_VALUES"
msgid ""
"Cannot ${action} ${type}. If both are provided, rate limit must not be lower "
"than committed rate."
msgstr ""
"Невозможно ${action} ${type}. Если указаны оба, то предел скорости не "
"может быть ниже, чем гарантированная скорость."

msgctxt ""
"ACTION_TYPE_FAILED_HOST_NETWORK_QOS_SETUP_NETWORKS_INCONSISTENT_VALUES"
msgid ""
"Cannot ${action} ${type}. If both are provided, rate limit must not be lower "
"than committed rate. However, this is not the case with the following "
"network(s): "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_SETUP_NETWORKS_INCONSISTENT_VALUES_LIST}"
"."
msgstr ""
"Невозможно ${action} ${type}. Если указаны оба, то предел скорости не "
"может быть ниже, чем гарантированная скорость. Тем не менее, для следующих "
"сетей "
"это не так: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_SETUP_NETWORKS_INCONSISTENT_VALUES_LIST}"
"."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INTERFACES_WITHOUT_QOS"
msgid ""
"Cannot ${action} ${type}. All or none of the networks attached to an "
"interface must have QoS configured, but on the following interface(s) some "
"of the networks are missing QoS: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INTERFACES_WITHOUT_QOS_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Все или ни одна из сетей, присоединённых "
"к интерфейсу, должны иметь настроенный QoS, но на следующих интерфейсах "
"у некоторых сетей QoS отсутствует: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INTERFACES_WITHOUT_QOS_LIST}."

msgctxt "ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INVALID_INTERFACE_SPEED"
msgid ""
"Cannot ${action} ${type}. Speed must be properly reported for interfaces "
"where QoS is configured, but is reported as missing or zero for the "
"following interfaces: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INVALID_INTERFACE_SPEED_LIST}."
msgstr ""
"Невозможно ${action} ${type}. Для интерфейсов с настроенным QoS данные о "
"скорости "
"должны сообщаться правильно, но для следующих интерфейсов сообщается "
"отсутствие данных "
"или данные с нулевым значением: "
"${ACTION_TYPE_FAILED_HOST_NETWORK_QOS_INVALID_INTERFACE_SPEED_LIST}."

msgctxt "QOS_NAME_NOT_NULL"
msgid "QoS name cannot be empty."
msgstr "Имя QoS не может быть пустым."

msgctxt "QOS_NAME_INVALID"
msgid "QoS name must be formed of 'a-z', 'A-Z', '0-9', '_' or '-' characters"
msgstr ""
"Имя QoS может содержать только символы  «a-z», «A-Z», «0-9», «_» или «-»"

msgctxt "QOS_NAME_TOO_LONG"
msgid "QoS name length must be under 50 characters."
msgstr "Имя QoS не должно превышать 50 символов."

#, fuzzy
msgctxt "ACTION_TYPE_FAILED_STORAGE_QOS_ILLEGAL_VALUES"
msgid ""
"Cannot ${action} ${type}.\n"
"QoS have non-zero total value with non-zero read/write for either throughput "
"or IOps or both."
msgstr "Невозможно ${action} ${type}.\n"

msgctxt "ACTION_TYPE_FAILED_QOS_MISSING_VALUES"
msgid "Cannot ${action} ${type}. QoS element has missing values."
msgstr "Невозможно ${action} ${type}. У элемента QoS отсутствуют значения."

msgctxt "ACTION_TYPE_FAILED_QOS_NEGATIVE_VALUES"
msgid "Cannot ${action} ${type}. QoS element cannot have negative values."
msgstr ""
"Невозможно ${action} ${type}. Элемент QoS не может иметь отрицательные "
"значения."

msgctxt "ACTION_TYPE_FAILED_QOS_NAME_EXIST"
msgid "Cannot ${action} ${type}. QoS element name already exists."
msgstr "Невозможно ${action} ${type}. Имя элемента QoS уже существует."

msgctxt "ACTION_TYPE_FAILED_QOS_NOT_FOUND"
msgid "Cannot ${action} ${type}. QoS element not found."
msgstr "Невозможно ${action} ${type}. Элемент QoS не найден."

msgctxt "ACTION_TYPE_FAILED_PROFILE_MISSING"
msgid "Cannot ${action} ${type}. Profile isn't provided."
msgstr "Невозможно ${action} ${type}. Не указан профиль."

msgctxt "ACTION_TYPE_FAILED_PROFILE_NOT_EXISTS"
msgid "Cannot ${action} ${type}. Profile not exists."
msgstr "Невозможно ${action} ${type}. Профиль не существует."

msgctxt "ACTION_TYPE_FAILED_PROFILE_NAME_IN_USE"
msgid "Cannot ${action} ${type}. Profile name is in use."
msgstr "Невозможно ${action} ${type}. Имя профиля используется."

msgctxt "ACTION_TYPE_FAILED_CANNOT_CHANGE_PROFILE"
msgid "Cannot ${action} ${type}. Cannot change profile."
msgstr "Невозможно ${action} ${type}. Невозможно изменить профиль."

msgctxt "ACTION_TYPE_FAILED_PROFILE_IN_USE"
msgid "Cannot ${action} ${type}. Profile is in use."
msgstr "Невозможно ${action} ${type}. Профиль используется."

msgctxt "ACTION_TYPE_DISK_PROFILE_EMPTY"
msgid "Cannot ${action} ${type}. Disk Profile is empty."
msgstr "Невозможно ${action} ${type}. Пустой профиль диска."

msgctxt "ACTION_TYPE_FAILED_DISK_PROFILE_NOT_FOUND"
msgid "Cannot ${action} ${type}. Disk Profile wasn't found."
msgstr "Невозможно ${action} ${type}. Не найден профиль диска."

msgctxt "ACTION_TYPE_DISK_PROFILE_STORAGE_DOMAIN_NOT_PROVIDED"
msgid "Cannot ${action} ${type}. Storage Domain wasn't provided."
msgstr "Невозможно ${action} ${type}. Не указан домен хранилища."

msgctxt "ACTION_TYPE_DISK_PROFILE_NOT_MATCH_STORAGE_DOMAIN"
msgid ""
"Cannot ${action} ${type}. Disk Profile ${diskProfile} with id "
"${diskProfileId} is not assigned to Storage Domain ${storageDomain}."
msgstr ""
"Невозможно ${action} ${type}. Профиль диска ${diskProfile} с ID "
"${diskProfileId} не назначен домену хранилища ${storageDomain}."

msgctxt "ACTION_TYPE_CPU_PROFILE_NOT_MATCH_CLUSTER"
