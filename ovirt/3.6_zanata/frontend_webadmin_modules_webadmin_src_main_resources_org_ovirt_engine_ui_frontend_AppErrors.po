msgstr "Невозможно ${action} ${type}. Имя используется"

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_LOCKED"
msgid ""
"Cannot ${action} ${type}. Scheduling Policy is locked, and cannot be "
"editable."
msgstr ""
"Невозможно ${action} ${type}. Политика планирования заблокирована "
"и может редактироваться."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_INUSE"
msgid ""
"Cannot ${action} ${type}. Scheduling Policy is attached to cluster(s): "
"${clusters}, please unassign cluster(s)."
msgstr ""
"Невозможно ${action} ${type}. Политика планирования присоединена к кластерам: "
"${clusters}, отмените привязку кластеров."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_UNKNOWN_POLICY_UNIT"
msgid "Cannot ${action} ${type}. Policy unit is unknown."
msgstr "Невозможно ${action} ${type}. Неизвестный элемент политики."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_FILTER_NOT_IMPLEMENTED"
msgid "Cannot ${action} ${type}. Policy unit doesn't implement Filtering."
msgstr ""
"Невозможно ${action} ${type}. Элемент политики не осуществляет фильтрации."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_FUNCTION_NOT_IMPLEMENTED"
msgid "Cannot ${action} ${type}. Policy unit doesn't implement score function."
msgstr ""
"Невозможно ${action} ${type}. Элемент политики не осуществляет функцию оценки."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_BALANCE_NOT_IMPLEMENTED"
msgid ""
"Cannot ${action} ${type}. Policy unit doesn't implement load balancing logic."
msgstr ""
"Невозможно ${action} ${type}. Элемент политики не применяет логику "
"балансировки нагрузки."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_ONLY_ONE_FILTER_CAN_BE_FIRST"
msgid ""
"Cannot ${action} ${type}. Only a single filter can be selected as first."
msgstr ""
"Невозможно ${action} ${type}. Только отдельный фильтр может быть выбран как "
"первый."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_ONLY_ONE_FILTER_CAN_BE_LAST"
msgid "Cannot ${action} ${type}. Only a single filter can be selected as last."
msgstr ""
"Невозможно ${action} ${type}. Только отдельный фильтр может быть выбран как "
"последний."

msgctxt "ACTION_TYPE_FAILED_CLUSTER_POLICY_FUNCTION_FACTOR_NEGATIVE"
msgid "Cannot ${action} ${type}. Function factor cannot be negative."
msgstr ""
"Невозможно ${action} ${type}. Фактор функции не может быть отрицательным."

msgctxt ""
"ACTION_TYPE_FAILED_CANNOT_REMOVE_POLICY_UNIT_ATTACHED_TO_CLUSTER_POLICY"
msgid ""
"Cannot ${action} ${type}. Policy unit is attached to the following cluster "
"policies: ${cpNames}."
msgstr ""
"Невозможно ${action} ${type}. Элемент политики присоединён к следующим "
"политикам "
"кластера: ${cpNames}."

msgctxt "VAR__TYPE__EXTERNAL_EVENT"
msgid "$type External Event"
msgstr "$type внешнее событие"

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_EVENT_ILLEGAL_ORIGIN"
msgid "Cannot ${action} ${type}. Illegal Origin for External Event : oVirt"
msgstr ""
"Невозможно ${action} ${type}. Недопустимый источник внешнего события: oVirt"

msgctxt "ACTION_TYPE_FAILED_EXTERNAL_EVENT_NOT_FOUND"
msgid "Cannot ${action} ${type}.External Event does not exist."
msgstr "Невозможно ${action} ${type}. Внешнее событие не существует."

msgctxt "FAILED_TO_RUN_LDAP_QUERY"
msgid "Failed to run LDAP query, please check server logs for more info."
msgstr "Сбой выполнения запроса LDAP, подробности см. в журналах сервера."

msgctxt "VAR__TYPE__EXTERNAL_JOB"
msgid "$type External Job"
msgstr "$type внешняя задача"

msgctxt "VAR__TYPE__EXTERNAL_STEP"
msgid "$type External Step"
msgstr "$type внешний шаг"

msgctxt "VAR__ACTION__END"
msgid "$action End"
msgstr "$action закончить"

msgctxt "VAR__ACTION__CLEAR"
msgid "$action Clear"
msgstr "$action очистить"

msgctxt "ACTION_TYPE_EMPTY_DESCRIPTION"
msgid "Cannot ${action} ${type}. Description can not be empty."
msgstr "Невозможно ${action} ${type}. Описание не может быть пустым."

msgctxt "ACTION_TYPE_NOT_EXTERNAL"
msgid ""
"Cannot ${action} ${type}. External steps can be added only to external jobs "
"or steps."
msgstr ""
"Невозможно ${action} ${type}. Внешние шаги можно добавить только "
"внешним заданиям или шагам."

msgctxt "ACTION_TYPE_NO_PARENT"
msgid ""
"Cannot ${action} ${type}. Step should be a child of the Job or other Step."
msgstr ""
"Невозможно ${action} ${type}. Шаг должен быть потомком задания или "
"другого шага."

msgctxt "ACTION_TYPE_NO_JOB"
msgid "Cannot ${action} ${type}. Job not found."
msgstr "Невозможно ${action} ${type}. Задание не найдено."

msgctxt "ACTION_TYPE_NO_STEP"
msgid "Cannot ${action} ${type}. Step not found."
msgstr "Невозможно ${action} ${type}. Шаг не найден."

msgctxt "ACTION_TYPE_NON_MONITORED"
msgid "Cannot ${action} ${type}. Non monitored command."
msgstr "Невозможно ${action} ${type}. Команда не отслеживается."

msgctxt "ACTION_TYPE_EMPTY_ACTION_TYPE"
msgid "Cannot ${action} ${type}.Action type cannot be empty."
msgstr "Невозможно ${action} ${type}. Тип действия не может быть пустым."

msgctxt "QOS_CPU_SHARES_OUT_OF_RANGE"
msgid ""
"Cannot set the cpu shares to the requested value. It has to be between 0 and "
"262144."
msgstr ""
"Невозможно указать требуемое значение для ресурсов ЦПУ. Оно должно "
"быть между 0 и 262144."

msgctxt "ERROR_CANNOT_RUN_ALIGNMENT_SCAN_VM_IS_RUNNING"
msgid ""
"Cannot ${action} ${type}. Alignment scan of a disk attached to a running VM "
"is only supported with RAW virtual disks."
msgstr ""
"Невозможно ${action} ${type}. Поиск невыравненных блоков диска, "
"присоединённого к выполняющейся ВМ, поддерживается только для "
"виртуальных дисков RAW."

msgctxt "ACTION_TYPE_FAILED_ALIGNMENT_SCAN_STORAGE_TYPE"
msgid ""
"Cannot ${action} ${type} ${diskAlias}. Alignment scan is only supported for "
"disks located on block storage domains."
msgstr ""
"Невозможно ${action} ${type} ${diskAlias}. Поиск невыравненных блоков "
"поддерживается "
"только для дисков, расположенных в доменах блочного хранилища."

msgctxt "SCHEDULING_ALL_HOSTS_FILTERED_OUT"
msgid ""
"Cannot ${action} ${type}. There is no host that satisfies current scheduling "
"constraints. See below for details:"
msgstr ""
"Невозможно ${action} ${type}. Отсутствует хост, отвечающий требованиям "
"ограничения расписания. "
"Подробности см. ниже:"

msgctxt "SCHEDULING_HOST_FILTERED_REASON"
msgid ""
"The host ${hostName} did not satisfy ${filterType} filter ${filterName}."
msgstr "Хост ${hostName} не соответствует ${filterType} фильтру ${filterName}."

msgctxt "SCHEDULING_HOST_FILTERED_REASON_WITH_DETAIL"
msgid ""
"The host ${hostName} did not satisfy ${filterType} filter ${filterName} "
"because ${detailMessage}."
msgstr ""
"Хост ${hostName} не соответствует ${filterType} фильтру ${filterName}. "
"Причина: ${detailMessage}."

msgctxt "VAR__DETAIL__NETWORK_MISSING"
msgid "$detailMessage network(s) ${networkNames} are missing"
msgstr "$detailMessage отсутствуют сети ${networkNames}"

msgctxt "VAR__DETAIL__DISPLAY_NETWORK_MISSING"
msgid "$detailMessage display network ${DisplayNetwork} was missing"
msgstr "$detailMessage отсутствовала сеть передачи экрана ${DisplayNetwork}"

msgctxt "VAR__DETAIL__DISPLAY_NETWORK_HAS_NO_BOOT_PROTOCOL"
msgid ""
"$detailMessage the display network ${DisplayNetwork} must have a DHCP or "
"Static boot protocol when configured on a host"
msgstr ""
"$detailMessage при настройке на хосте, сеть передачи экрана ${DisplayNetwork} "
"должна иметь DHCP или статический протокол загрузки"

msgctxt "VAR__DETAIL__NO_SUITABLE_VF"
msgid ""
"$detailMessage there are no free virtual functions which are suitable for "
"virtual nic(s) ${vnicNames}. A virtual function is considered as suitable if "
"the VF's configuration of its physical function contains the virtual nic's "
"network/network label"
msgstr ""
"$detailMessage отсутствуют свободные виртуальные функции, подходящие "
"для виртуальных NIC ${vnicNames}. Виртуальная функция считается подходящей, "
"если конфигурация её физической функции содержит сеть/метку сети "
"виртуальной NIC."

msgctxt "VAR__DETAIL__AFFINITY_FAILED_POSITIVE"
msgid ""
"$detailMessage it did not match positive affinity rules ${affinityRules}"
msgstr ""
"$detailMessage не соответствует правилам положительной схожести "
"${affinityRules}"

msgctxt "VAR__DETAIL__AFFINITY_FAILED_NEGATIVE"
msgid "$detailMessage it matched negative affinity rules ${affinityRules}"
msgstr ""
"$detailMessage соответствует правилам отрицательной схожести "
"${affinityRules}"

msgctxt "VAR__DETAIL__LOW_CPU_LEVEL"
msgid ""
"$detailMessage its CPU level ${hostCPULevel} is lower than the VM requires "
"${vmCPULevel}"
msgstr ""
"$detailMessage уровень ЦПУ ${hostCPULevel} ниже требований ВМ "
"${vmCPULevel}"

msgctxt "VAR__DETAIL__UNSUPPORTED_EMULATED_MACHINE"
msgid ""
"$detailMessage it doesn't support the emulated machine "
"'${vmEmulatedMachine}' which is required by the VM. Host supported emulated "
"machines are: ${hostEmulatedMachines}."
msgstr ""
"$detailMessage не поддерживает эмулированную машину «${vmEmulatedMachine}», "
"согласно требованиям ВМ. Эмулированные машины, поддерживаемые хостом: "
"${hostEmulatedMachines}."

msgctxt "VAR__DETAIL__SWAP_VALUE_ILLEGAL"
msgid "$detailMessage its swap value was illegal"
msgstr "$detailMessage недопустимое значение подкачки"

msgctxt "VAR__DETAIL__NOT_ENOUGH_MEMORY"
msgid ""
"$detailMessage has availabe ${availableMem} MB memory. Insufficient free "
"memory to run the VM"
msgstr ""
"$detailMessage доступно ${availableMem} Мб ОЗУ. Недостаточно свободной памяти "
"для запуска ВМ."

msgctxt "VAR__DETAIL__NOT_MEMORY_PINNED_NUMA"
msgid ""
"$detailMessage cannot accommodate memory of VM's pinned virtual NUMA nodes "
"within host's physical NUMA nodes."
msgstr ""
"$detailMessage невозможно вместить память прикреплённых виртуальных узлов "
"NUMA "
"машины в физические узлы NUMA хоста."

msgctxt "VAR__DETAIL__NOT_ENOUGH_CORES"
msgid "$detailMessage it does not have enough cores to run the VM"
msgstr "$detailMessage недостаточно ядер для запуска ВМ"

msgctxt "VAR__DETAIL__NUMA_PINNING_FAILED"
msgid "$detailMessage it has insufficient NUMA node free memory to run the VM"
msgstr "$detailMessage недостаточно свободной памяти узла NUMA для запуска ВМ"

msgctxt "VAR__DETAIL__NOT_HE_HOST"
msgid "$detailMessage it is not a Hosted Engine host."
msgstr "$detailMessage не является хостом виртуализированного ЦУ"

msgctxt "VAR__DETAIL__SAME_HOST"
msgid "$detailMessage it currently hosts the VM."
msgstr "$detailMessage в данный момент содержит ВМ"

msgctxt "VAR__DETAIL__HOSTDEV_DISABLED"
msgid "$detailMessage it does not support host device passthrough."
msgstr "$detailMessage не поддерживает транзитное хост-устройство"

msgctxt "VAR__DETAIL__HOST_DEVICE_UNAVAILABLE"
msgid "$detailMessage some of the required host devices are unavailable."
msgstr "$detailMessage некоторые из требуемых хост-устройств недоступны"

msgctxt "SCHEDULING_NO_HOSTS"
msgid ""
"There are no hosts to use. Check that the cluster contains at least one host "
"in Up state."
msgstr ""
"Нет хостов, доступных для использования. Убедитесь, что хотя бы один "
"из хостов кластера имеет статус «Запущен»."

msgctxt "VAR__FILTERTYPE__EXTERNAL"
msgid "$filterType external"
msgstr "$filterType внешний"

msgctxt "VAR__FILTERTYPE__INTERNAL"
msgid "$filterType internal"
msgstr "$filterType внутренний"

msgctxt "POWER_MANAGEMENT_ACTION_ON_ENTITY_ALREADY_IN_PROGRESS"
msgid "Another power management action, ${action}, is already in progress."
msgstr "Другая операция управления питанием (${action}) уже выполняется."

msgctxt "LABELED_NETWORK_INTERFACE_NOT_FOUND"
msgid "A labeled network interface could not be found."
msgstr "Не удалось найти сетевой интерфейс с меткой."

msgctxt "NETWORK_LABEL_CONFLICT"
msgid ""
"The networks represented by label cannot be configured on the same network "
"interface."
msgstr ""
"Сеть, представленная меткой, не может быть настроена на том же "
"сетевом интерфейсе."

msgctxt "AFFINITY_GROUP_NAME_TOO_LONG"
msgid "Cannot ${action} ${type}. Name field input is too long."
msgstr "Невозможно ${action} ${type}. Слишком длинное имя."

msgctxt "AFFINITY_GROUP_NAME_INVALID"
msgid "Cannot ${action} ${type}. Name field is invalid."
msgstr "Невозможно ${action} ${type}. Неверно заполнено поле имени."

msgctxt "AFFINITY_GROUP_DESCRIPTION_INVALID"
msgid "Cannot ${action} ${type}. Description field is invalid."
msgstr "Невозможно ${action} ${type}. Неверно заполнено поле описания."

msgctxt "ACTION_TYPE_FAILED_AFFINITY_GROUP_INVALID_CLUSTER_ID"
msgid "Cannot ${action} ${type}. Cluster is empty."
msgstr "Невозможно ${action} ${type}. Пустой кластер."

msgctxt "ACTION_TYPE_FAILED_INVALID_CLUSTER_FOR_AFFINITY_GROUP"
msgid "Cannot ${action} ${type}. Provided Cluster is invalid."
msgstr "Невозможно ${action} ${type}. Указан недопустимый кластер."

msgctxt "ACTION_TYPE_FAILED_INVALID_VM_FOR_AFFINITY_GROUP"
msgid "Cannot ${action} ${type}. Provided VM is invalid (does not exist)."
msgstr "Невозможно ${action} ${type}. Указана недопустимая ВМ (не существует)."

msgctxt "ACTION_TYPE_FAILED_VM_NOT_IN_AFFINITY_GROUP_CLUSTER"
msgid "Cannot ${action} ${type}. Provided VM resides in another Cluster."
msgstr ""
"Невозможно ${action} ${type}. Указанная ВМ расположена на другом кластере."

msgctxt "ACTION_TYPE_FAILED_DUPLICTE_VM_IN_AFFINITY_GROUP"
msgid "Cannot ${action} ${type}. Duplicate VM."
msgstr "Невозможно ${action} ${type}. Дублирующая ВМ."

msgctxt "ACTION_TYPE_FAILED_INVALID_AFFINITY_GROUP_ID"
msgid "Cannot ${action} ${type}. No matching Affinity Group."
msgstr "Невозможно ${action} ${type}. Отсутствует совпадающая группа схожести."

msgctxt "ACTION_TYPE_FAILED_CANNOT_CHANGE_CLUSTER_ID"
msgid "Cannot ${action} ${type}. Cannot change Cluster for Affinity Group."
msgstr ""
"Невозможно ${action} ${type}. Невозможно сменить кластер группы схожести."

msgctxt "ACTION_TYPE_FAILED_AFFINITY_GROUP_NAME_EXISTS"
msgid "Cannot ${action} ${type}. Affinity Group name already exists."
msgstr "Невозможно ${action} ${type}. Имя группы схожести уже существует."

msgctxt "ACTION_TYPE_FAILED_POSITIVE_AFFINITY_GROUP"
msgid ""
"VM is associated with a positive Affinity Group (${affinityGroupName}) and "
"require to run on the same Host (${hostName}) as the other group VMs"
msgstr ""
"У ВМ есть привязка к группе положительной схожести (${affinityGroupName}), "
"поэтому она должна выполняться на том же хосте (${hostName}), где выполняются "
"и другие ВМ группы."

msgctxt "ACTION_TYPE_FAILED_NEGATIVE_AFFINITY_GROUP"
msgid ""
"VM is associated with a negative Affinity Group and require to run on "
"separte Host, which doesn't run the other group VMs"
msgstr ""
"У ВМ есть привязка к группе отрицательной схожести, поэтому она должна "
"выполняться на отдельном хосте, где не выполняются машины другой группы."

msgctxt "ACTION_TYPE_FAILED_MIX_POSITIVE_NEGATIVE_AFFINITY_GROUP"
msgid ""
"VM is associated with both positive and negative Affinity Groups, please "
"reconfigure VM's affinity groups"
msgstr ""
"У ВМ есть привязка как к группе положительной схожести, так и к "
"группе отрицательной схожести. Перенастройте группы схожести ВМ."

msgctxt "ACTION_TYPE_FAILED_AFFINITY_RULES_COLLISION"
msgid ""
"Affinity Group collision detected in unified affinity group:\n"
"${UnifiedAffinityGroups}\n"
"and negative affinity group:\n"
"${negativeAR}"
msgstr ""
"Обнаружен конфликт группы схожести в группе двойной схожести:\n"
"${UnifiedAffinityGroups}\n"
"и в группе отрицательной схожести:\n"
"${negativeAR}"

msgctxt "VALIDATION_ISCSI_BOND_NAME_MAX"
msgid "iSCSI bond name must not exceed 50 characters"
msgstr "Имя связи iSCSI не должно превышать 50 символов"

msgctxt "VALIDATION_ISCSI_BOND_NAME_NOT_NULL"
msgid "iSCSI bond name is required"
msgstr "Требуется имя связи iSCSI"

msgctxt "VALIDATION_ISCSI_BOND_DESCRIPTION_MAX"
msgid "iSCSI bond description must not exceed 4000 characters"
msgstr "Описание связи iSCSI не должно превышать 4000 символов"

msgctxt "VALIDATION_ISCSI_BOND_NAME_INVALID_CHARACTER"
msgid ""
"iSCSI bond name must be formed from alpha-numeric characters, periods (.), "
"hyphens (-), and underscores (_)."
msgstr ""
"Имя связи iSCSI должно состоять из буквенно-цифровых символов, точек (.), "
"дефисов (-) и знаков нижнего подчёркивания (_)."

msgctxt "ISCSI_BOND_NOT_EXIST"
msgid "Cannot ${action} ${type}. The specified iSCSI bond does not exist."
msgstr "Невозможно ${action} ${type}. Указанная связь iSCSI не существует."

msgctxt "ISCSI_BOND_WITH_SAME_NAME_EXIST_IN_DATA_CENTER"
msgid ""
"Cannot ${action} ${type}. iSCSI bond with the same name already exists in "
"the Data Center."
msgstr ""
"Невозможно ${action} ${type}. Связь iSCSI с таким именем уже существует "
"в дата-центре."

msgctxt "ISCSI_BOND_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. iSCSI Bond is only supported on Data Center "
"compatibility versions 3.4 and higher."
msgstr ""
"Невозможно ${action} ${type}. Связи iSCSI поддерживаются только версией "
"совместимости дата-центра 3.4 и выше."

msgctxt "ACTION_TYPE_FAILED_STORAGE_CONNECTIONS_CANNOT_BE_ADDED_TO_ISCSI_BOND"
msgid ""
"Cannot ${action} ${type}. The following storage connections ${connectionIds} "
"cannot be added to the specified iSCSI bond. \n"
"Possible reasons:\n"
"- They are not of type iSCSI.\n"
"- Their status is not one of the following: Unknown, Active. Inactive.\n"
"- They already exist in the iSCSI bond.\n"
"- They do not belong to the same Data Center as the specified iSCSI bond."
msgstr ""
"Невозможно ${action} ${type}. Следующие подключения к хранилищу невозможно "
"добавить к указанной связи iSCSI: ${connectionIds}.\n"
"Возможные причины:\n"
"— Подключения не имеют тип iSCSI\n"
"— Подключения имеют статус, отличный от: «Неизвестно», «Активно», "
"«Неактивно»\n"
"— Подключения уже имеются в связи iSCSI\n"
"— Подключения не принадлежат тому же дата-центру, что и указанная связь iSCSI."

msgctxt "ACTION_TYPE_FAILED_ISCSI_BOND_NETWORK_CANNOT_BE_REQUIRED"
msgid ""
"Cannot ${action} ${type}. Required network cannot be a part of an iSCSI bond."
msgstr ""
"Невозможно ${action} ${type}. Требуемая сеть не может быть частью связи iSCSI"

msgctxt "ACTION_TYPE_FAILED_INTERFACE_DOES_NOT_SUPPORT_READ_ONLY_ATTR"
msgid "Cannot ${action} ${type}. ${interface} disks can't be read-only."
msgstr ""
"Невозможно ${action} ${type}. Диски ${interface} не могут быть только для "
"чтения"

msgctxt "ACTION_TYPE_FAILED_RNG_NOT_FOUND"
msgid ""
"Cannot ${action} ${type}. Trying to manipulate with Random Number Generator "
"device but none is found."
msgstr ""
"Невозможно ${action} ${type}. Попытка работы с устройством-генератором "
"случайных чисел, "
"но ни одного такого устройства не было найдено"

msgctxt "ACTION_TYPE_FAILED_RNG_ALREADY_EXISTS"
msgid ""
"Cannot ${action} ${type}. Random Number Generator device already exists."
msgstr ""
"Невозможно ${action} ${type}. Устройство-генератор случайных чисел уже "
"существует."

msgctxt "ACTION_TYPE_FAILED_RNG_SOURCE_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. Random Number Generator device is not supported in "
"cluster."
msgstr ""
"Невозможен ${action} ${type}. Устройство-генератор случайных чисел не "
"поддерживается "
"на кластере."

msgctxt "ACTION_TYPE_RNG_MUST_BE_SPECIFIED"
msgid ""
"Cannot ${action} ${type}. Random Number Generator must be specified in "
"parameters."
msgstr ""
"Невозможно ${action} ${type}. Генератор случайных чисел должен быть указан в "
"свойствах."

msgctxt "KDUMP_DETECTION_NOT_ENABLED_FOR_VDS"
msgid ""
"Cannot ${action} ${type}. Kdump integration is not enabled for host "
"'${VdsName}'."
msgstr ""
"Невозможно ${action} ${type}. Интеграция с Kdump не включена для "
"хоста «${VdsName}»."

msgctxt "KDUMP_DETECTION_NOT_CONFIGURED_ON_VDS"
msgid ""
"Cannot ${action} ${type}. Kdump is not properly configured on host "
"'${VdsName}'."
msgstr ""
"Невозможно ${action} ${type}. Неправильная конфигурация Kdump на хосте "
"«${VdsName}»."

msgctxt "BALLOON_REQUESTED_ON_NOT_SUPPORTED_ARCH"
msgid ""
"Cannot ${action} ${type}. Balloon is not supported on '${clusterArch}' "
"architecture."
msgstr ""
"Невозможно ${action} ${type}. Balloon не поддерживается на архитектуре "
"«${clusterArch}»."

msgctxt "SOUND_DEVICE_REQUESTED_ON_NOT_SUPPORTED_ARCH"
msgid ""
"Cannot ${action} ${type}. Sound device is not supported on '${clusterArch}' "
"architecture."
msgstr ""
"Невозможно ${action} ${type}. Звуковое устройство не поддерживается на "
"архитектуре «${clusterArch}»."

msgctxt "CPU_TYPE_UNSUPPORTED_FOR_THE_GUEST_OS"
msgid ""
"The guest OS doesn't support the following CPUs: ${unsupportedCpus}. Its "
"possible to change the cluster cpu or set a different one per VM"
msgstr ""
"Гостевая ОС не поддерживает следующие CPU: ${unsupportedCpus}. Есть "
"возможность сменить ЦПУ кластера или настроить разные "
"ЦПУ для каждой машины."

msgctxt "CPU_TYPE_UNKNOWN"
msgid ""
"The CPU type of the cluster is unknown. Its possible to change the cluster "
"cpu or set a different one per VM."
msgstr ""
"Тип ЦПУ кластера неизвестен. Есть возможность сменить ЦПУ кластера или "
"настроить разные "
"ЦПУ для каждой машины."

msgctxt "ACTION_TYPE_FAILED_DEVICE_MUST_BE_SPECIFIED"
msgid "Cannot ${action} ${type}. Device must be specified."
msgstr "Невозможно ${action} ${type}. Должно быть указано устройство."

msgctxt "ACTION_TYPE_FAILED_GRAPHIC_TYPE_MUST_BE_SPECIFIED"
msgid "Cannot ${action} ${type}. Graphics Device Type must be specified."
msgstr ""
"Невозможно ${action} ${type}. Должен быть указан тип графического устройства."

msgctxt "ACTION_TYPE_REMOVE_GRAPHICS_DEV_INVALID_PARAMS"
msgid "Cannot ${action} ${type}. Graphics device ID or VM/Template ID is null."
msgstr ""
"Невозможно ${action} ${type}. Нулевой ID графического устройства или "
"нулевой ID шаблона/ВМ."

msgctxt "ACTION_TYPE_FAILED_ONLY_ONE_DEVICE_WITH_THIS_GRAPHICS_ALLOWED"
msgid ""
"Cannot ${action} ${type}. One VM can not contain more than one device with "
"the same graphics type."
msgstr ""
"Невозможно ${action} ${type}. Одна ВМ не может содержать больше одного "
"графического устройства каждого типа."

msgctxt "ACTION_TYPE_FAILED_SNAPSHOT_ALREADY_EXISTS"
msgid "Cannot ${action} ${type}. Snapshot ${snapname} already exists."
msgstr "Невозможно ${action} ${type}. Снимок ${snapname} уже существует."

msgctxt "ACTION_TYPE_FAILED_PROVIDER_NOT_SUPPORTED"
msgid ""
"Cannot ${action} ${type}. The current type ${providerType} is not supported."
msgstr ""
"Невозможно ${action} ${type}. Данный тип ${providerType} не поддерживается."

msgctxt "ACTION_TYPE_FAILED_CINDER_ALREADY_EXISTS"
msgid ""
"Cannot ${action} ${type}. There is already an existing Cinder provider with "
"the same IP address."
msgstr ""
"Невозможно ${action} ${type}. Уже существует поставщик Cinder с таким "
"адресом IP."

msgctxt "ACTION_TYPE_FAILED_CINDER"
msgid ""
"Cannot ${action} ${type}. An error occurred on Cinder - '${cinderException}'."
msgstr "Невозможно ${action} ${type}. Ошибка Cinder: «${cinderException}»."

msgctxt "CANNOT_ADD_CINDER_DISK_VOLUME_LIMIT_EXCEEDED"
msgid ""
"Cannot ${action} ${type}. Maximum number of volumes allowed "
"(${maxTotalVolumes}) exceeded - could not create Cinder disks on Storage "
"Domain ${storageName}."
msgstr ""
"Невозможно ${action} ${type}. Превышено максимально допустимое количество "
"томов (${maxTotalVolumes}) — не удалось создать диски Cinder в домене "
"хранилища "
"${storageName}."

msgctxt "CANNOT_ADD_CINDER_DISK_SNAPSHOT_LIMIT_EXCEEDED"
msgid ""
"Cannot ${action} ${type}. Maximum number of snapshots allowed "
"(${maxTotalSnapshots}) exceeded - could not create Cinder disk snapshot on "
"Storage ${storageName}."
msgstr ""
"Невозможно ${action} ${type}. Превышено максимально допустимое число снимков "
"(${maxTotalSnapshots}) — не удалось создать снимок диска Cinder в хранилище "
"${storageName}."

msgctxt "CINDER_DISK_ALREADY_REGISTERED"
msgid ""
"Cannot ${action} ${type}. Cinder disk is already registered (${diskAlias})."
msgstr ""
"Невозможно ${action} ${type}. Диск Cinder уже зарегистрирован (${diskAlias})."

msgctxt "CINDER_VOLUME_TYPE_NOT_EXISTS"
msgid ""
"Cannot ${action} ${type}. Cinder volume type '${cinderVolumeType}' does not "
"exist."
msgstr ""
"Невозможно ${action} ${type}. Тип тома Cinder «${cinderVolumeType}» не "
"существует."

msgctxt "ERROR_CANNOT_DETACH_CINDER_PROVIDER_WITH_IMAGES"
msgid ""
"Cannot detach a non empty Cinder provider.\n"
"-Please remove all VMs / Templates / Disks and try again."
msgstr ""
"Невозможно отсоединить не пустого поставщика Cinder.\n"
"— Удалите все ВМ/шаблоны/диски и повторите попытку."

msgctxt "LIBVIRT_SECRET_UUID_CANNOT_BE_EMPTY"
msgid "Cannot ${action} ${type}. Authentication Key UUID cannot be empty."
msgstr ""
"Невозможно ${action} ${type}. UUID ключа аутентификации не может быть пустым."

msgctxt "LIBVIRT_SECRET_VALUE_CANNOT_BE_EMPTY"
msgid "Cannot ${action} ${type}. Authentication Key value cannot be empty."
msgstr ""
"Невозможно ${action} ${type}. Значение ключа аутентификации не может быть "
"пустым."

msgctxt "LIBVIRT_SECRET_VALUE_ILLEGAL_FORMAT"
msgid ""
"Cannot ${action} ${type}. Authentication Key value should be encoded in "
"Base64."
msgstr ""
"Невозможно ${action} ${type}. Значение ключа аутентификации должно иметь "
"кодировку Base64."

msgctxt "LIBVIRT_SECRET_UUID_ALREADY_EXISTS"
msgid "Cannot ${action} ${type}. Authentication Key UUID already exists."
msgstr "Невозможно ${action} ${type}. UUID ключа уже существует."

msgctxt "LIBVIRT_SECRET_UUID_NOT_EXISTS"
msgid "Cannot ${action} ${type}. Authentication Key UUID does not exist."
msgstr "Невозможно ${action} ${type}. UUID ключа аутентификации не существует."

msgctxt "ACTION_TYPE_FAILED_VM_USES_SCSI_RESERVATION"
msgid "Cannot ${action} ${type}. VM uses SCSI reservation."
msgstr "Невозможно ${action} ${type}. ВМ использует резервирование SCSI."

msgctxt "ACTION_TYPE_FAILED_SCSI_RESERVATION_NOT_VALID_FOR_FLOATING_DISK"
msgid ""
"Cannot ${action} ${type}. SCSI reservation cannot be set when adding "
"floating disks."
msgstr ""
"Невозможно ${action} ${type}. Нельзя установить резервирование SCSI при "
"добавлении "
"плавающих дисков."

msgctxt "ACTION_TYPE_FAILED_SGIO_IS_FILTERED"
msgid ""
"Cannot ${action} ${type}. SCSI reservation can be set only when SGIO is "
"unfiltered."
msgstr ""
"Невозможно ${action} ${type}. Резервирование SCSI можно установить только "
"если SGIO не отфильтрован."

msgctxt "ACTION_TYPE_FAILED_VM_NOT_PINNED_TO_HOST"
msgid "Cannot ${action} ${type}. VM must be pinned to a host."
