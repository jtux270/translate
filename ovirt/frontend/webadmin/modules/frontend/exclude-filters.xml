<FindBugsFilter>
    <Match>
          <Class name="com.google.gwt.core.client.GWTBridge" />
          <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS" />
    </Match>
    <Match>
          <Class name="org.ovirt.engine.ui.frontend.gwtservices.GenericApiGWTService" />
          <Bug pattern="NM_METHOD_NAMING_CONVENTION" />
	  <Method name=".*"/>
    </Match>
    <Match>
          <Class name="org.ovirt.engine.ui.frontend.server.gwt.GenericApiGWTServiceImpl" />
          <Bug pattern="NM_METHOD_NAMING_CONVENTION" />
          <Method name=".*"/>
    </Match>

    <!--
      findbugs complain of unused field in class.
      This is a GWT class and use to generate code

      findbugs reason:
      UuF: Unused field (UUF_UNUSED_FIELD)
     -->
     <Match>
       <Class name="org.ovirt.engine.ui.frontend.communication.RefreshActiveModel" />
       <Bug code="UuF"/>
     </Match>

    <!--
      findbugs complain of unused field in class.
      This is a GWT class and use to generate code

      findbugs reason:
      UuF: Unused field (UUF_UNUSED_FIELD)
     -->
     <Match>
       <Class name="org.ovirt.engine.ui.frontend.communication.SSOTokenChange" />
       <Bug code="UuF"/>
     </Match>

    <!--
      findbugs complains about unused field(s) in this class.
      This is a GWT class and used to generate code.

      findbugs reason:
      UuF: Unused field (UUF_UNUSED_FIELD)
     -->
     <Match>
       <Class name="org.ovirt.engine.ui.frontend.communication.EngineSessionRefreshed" />
       <Bug code="UuF"/>
     </Match>

     <Match>
       <Class name="org.ovirt.engine.ui.frontend.AsyncQuery" />
       <Field name="converterCallback"/>
       <Bug pattern="UWF_NULL_FIELD"/>
     </Match>

     <!--
       Supress inefficient "new Boolean" calls which are generated by GWT
     -->
     <Match>
       <Bug pattern="DM_BOOLEAN_CTOR"/>
     </Match>

     <!--
      findbugs complains that class is too big of analysis.
      This class cannot be smaller since it should contain all the errors
     -->
     <Match>
       <Class name="org.ovirt.engine.ui.frontend.AppErrors" />
       <Bug pattern="SKIPPED_CLASS_TOO_BIG"/>
     </Match>
</FindBugsFilter>
