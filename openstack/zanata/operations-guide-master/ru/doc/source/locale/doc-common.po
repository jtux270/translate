msgstr ""

#: ../../source/common/glossary.rst:2242
msgid "The collaboration site for OpenStack."
msgstr ""

#: ../../source/common/glossary.rst:2243
msgid "Layer-2 (L2) agent"
msgstr ""

#: ../../source/common/glossary.rst:2246
