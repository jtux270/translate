#: cinder/context.py:142
#, python-format
msgid "read_deleted can only be one of 'no', 'yes' or 'only', not %r"
msgstr ""
"значением для read_deleted может быть: 'no', 'yes' или 'only', но не %r"

#: cinder/coordination.py:88
msgid "Coordinator uninitialized."
msgstr "Координатор не инициализирован"

#: cinder/exception.py:67
msgid "An unknown exception occurred."
msgstr "Произошло неизвестное исключение."

#: cinder/exception.py:128
#, python-format
msgid ""
"Bad or unexpected response from the storage volume backend API: %(data)s"
msgstr ""
"Неверный или непредвиденный ответ от API серверной части тома"
" хранилища: %(data)s"

#: cinder/exception.py:133
#, python-format
msgid "Volume driver reported an error: %(message)s"
msgstr "Драйвер тома сообщил об ошибке: %(message)s"

#: cinder/exception.py:137
#, python-format
msgid "Backup driver reported an error: %(reason)s"
msgstr "Резервный драйвер сообщил об ошибке: %(reason)s"

#: cinder/exception.py:141
#, python-format
msgid "Canceled backup %(back_id)s restore on volume %(vol_id)s"
msgstr ""
"Восстановление из рез. копии %(back_id)s было отменено на томе %(vol_id)s"

#: cinder/exception.py:145
#, python-format
msgid "Connection to glance failed: %(reason)s"
msgstr "Сбой создания подключения к glance: %(reason)s"

#: cinder/exception.py:149
#, python-format
msgid "Programming error in Cinder: %(reason)s"
msgstr "Программная ошибка Cinder: %(reason)s"

#: cinder/exception.py:153
msgid "Not authorized."
msgstr "Отсутствует авторизация."

#: cinder/exception.py:158
msgid "User does not have admin privileges"
msgstr "У пользователя нет привилегий администратора"

#: cinder/exception.py:162
#, python-format
msgid "Policy doesn't allow %(action)s to be performed."
msgstr "Политика не разрешает выполнение %(action)s"

#: cinder/exception.py:166
#, python-format
msgid "Not authorized for image %(image_id)s."
msgstr "Отсутствует авторизация на образ %(image_id)s."

#: cinder/exception.py:170
msgid "Volume driver not ready."
msgstr "Драйвер тома не готов"

#: cinder/exception.py:174
msgid "Unacceptable parameters."
msgstr "Неприемлемые параметры."

#: cinder/exception.py:179
#, python-format
msgid "Invalid snapshot: %(reason)s"
msgstr "Недействительный снимок: %(reason)s"

#: cinder/exception.py:183
#, python-format
msgid "Invalid attaching mode '%(mode)s' for volume %(volume_id)s."
msgstr "Недействительный режим прикрепления '%(mode)s' для тома %(volume_id)s."

#: cinder/exception.py:188
#, python-format
msgid "Volume %(volume_id)s is still attached, detach volume first."
msgstr "Том %(volume_id)s ещё прикреплён, открепите сначала том."

#: cinder/exception.py:192
msgid "The results are invalid."
msgstr "Результаты недействительны"

#: cinder/exception.py:196
#, python-format
msgid "Invalid input received: %(reason)s"
msgstr "Получен недействительный ввод: %(reason)s"

#: cinder/exception.py:200
#, python-format
msgid "Availability zone '%(az)s' is invalid."
msgstr "Недействительная зона доступности '%(az)s'"

#: cinder/exception.py:204
#, python-format
msgid "Volume type's availability zones are invalid %(az)s."
msgstr "Зоны доступности типа тома недействительны %(az)s."

#: cinder/exception.py:208
#, python-format
msgid "Invalid volume type: %(reason)s"
msgstr "Недействительный тип тома: %(reason)s"

#: cinder/exception.py:212
#, python-format
msgid "Invalid group type: %(reason)s"
msgstr "Недействительный тип группы: %(reason)s"

#: cinder/exception.py:216
#, python-format
msgid "Invalid volume: %(reason)s"
msgstr "Недействительный том: %(reason)s"

#: cinder/exception.py:220
#, python-format
msgid "Invalid content type %(content_type)s."
msgstr "Недопустимый тип содержимого %(content_type)s."

#: cinder/exception.py:224
#, python-format
msgid "Invalid host: %(reason)s"
msgstr "Недействительный хост: %(reason)s"

#: cinder/exception.py:234
#, python-format
msgid "Invalid auth key: %(reason)s"
msgstr "Недействительный ключ аутентификации: %(reason)s"

#: cinder/exception.py:238
#, python-format
msgid ""
"Value \"%(value)s\" is not valid for configuration option \"%(option)s\""
msgstr ""
"Значение \"%(value)s\" не является допустимым для параметра конфигурации \""
"%(option)s\""

#: cinder/exception.py:243
msgid "Service is unavailable at this time."
msgstr "В данный момент служба недоступна"

#: cinder/exception.py:247
#, python-format
msgid "Cannot perform %(action)s during system upgrade."
msgstr "Невозможно выполнить %(action)s во время системного обновления"

#: cinder/exception.py:251
#, python-format
msgid "Image %(image_id)s is unacceptable: %(reason)s"
msgstr "Образ %(image_id)s неприемлем: %(reason)s"

#: cinder/exception.py:255
#, python-format
msgid "Image %(image_id)s size exceeded available disk space: %(reason)s"
msgstr ""
"Размер образа %(image_id)s превышает доступное пространство на диске: "
"%(reason)s"

#: cinder/exception.py:260
#, python-format
msgid "The device in the path %(path)s is unavailable: %(reason)s"
msgstr "Устройство по пути %(path)s недоступно: %(reason)s"

#: cinder/exception.py:264
#, python-format
msgid "The snapshot is unavailable: %(data)s"
msgstr "Этот снимок недоступен: %(data)s"

#: cinder/exception.py:268
#, python-format
msgid "Expected a UUID but received %(uuid)s."
msgstr "Ожидался UUID, но получено %(uuid)s."

#: cinder/exception.py:272
#, python-format
msgid ""
"API Version String %(version)s is of invalid format. Must be of format "
"MajorNum.MinorNum."
msgstr ""
"Строка версии API %(version)s имеет неверный формат. Правильный формат:"
" MajorNum.MinorNum."

#: cinder/exception.py:277
#, python-format
msgid "API version %(version)s is not supported on this method."
msgstr "Версия API %(version)s не поддерживается этим методом."

#: cinder/exception.py:281
#, python-format
msgid ""
"Version %(req_ver)s is not supported by the API. Minimum is %(min_ver)s "
"and maximum is %(max_ver)s."
msgstr ""
"Версия %(req_ver)s не поддерживается API. Минимальная: %(min_ver)s,"
"максимальная: %(max_ver)s."

#: cinder/exception.py:290
#, python-format
msgid "Error while requesting %(service)s API."
msgstr "Ошибка при запросе %(service)s API."

#: cinder/exception.py:299
#, python-format
msgid "Timeout while requesting %(service)s API."
msgstr "Истечение времени ожидания при запросе %(service)s API."

#: cinder/exception.py:303
#, python-format
msgid "Timeout while requesting capabilities from backend %(service)s."
msgstr "Истечение времени ожидания при запросе %(service)s серверной части."

#: cinder/exception.py:313
msgid "Resource could not be found."
msgstr "Ресурс не найден"

#: cinder/exception.py:319
#, python-format
msgid "Store %(store_id)s not enabled in glance."
msgstr "Использование хранилища %(store_id)s не включено в glance."

#: cinder/exception.py:323
#, python-format
msgid "Store %(store_id)s is read-only in glance."
msgstr "Хранилище %(store_id)s доступно в glance только для чтения."

#: cinder/exception.py:327
#, python-format
msgid "Volume %(volume_id)s could not be found."
msgstr "Не удалось найти том %(volume_id)s."

#: cinder/exception.py:331
#, python-format
msgid "Message %(message_id)s could not be found."
msgstr "Не удалось найти сообщение %(message_id)s."

#: cinder/exception.py:335
#, python-format
msgid "Volume attachment could not be found with filter: %(filter)s."
msgstr "Не удалось найти прикрепление тома с фильтром: %(filter)s."

#: cinder/exception.py:340
#, python-format
msgid "Volume %(volume_id)s has no metadata with key %(metadata_key)s."
msgstr "У тома %(volume_id)s нет метаданных с ключом %(metadata_key)s."

#: cinder/exception.py:345
#, python-format
msgid "Invalid metadata: %(reason)s"
msgstr "Недействительные метаданные: %(reason)s"

#: cinder/exception.py:349
#, python-format
msgid "Invalid metadata size: %(reason)s"
msgstr "Недействительный размер метаданных: %(reason)s"

#: cinder/exception.py:353
#, python-format
msgid "Snapshot %(snapshot_id)s has no metadata with key %(metadata_key)s."
msgstr "У снимка %(snapshot_id)s нет метаданных с ключом %(metadata_key)s."

#: cinder/exception.py:358
#, python-format
msgid "Volume type %(volume_type_id)s could not be found."
msgstr "Не удалось найти тип тома %(volume_type_id)s"

#: cinder/exception.py:362
#, python-format
msgid "Volume type with name %(volume_type_name)s could not be found."
msgstr "Не удалось найти тип тома с именем %(volume_type_name)s"

#: cinder/exception.py:367
#, python-format
msgid ""
"Volume type access not found for %(volume_type_id)s / %(project_id)s "
"combination."
msgstr ""
"Не удалось найти доступ к типу тома для сочетания %(volume_type_id)s / "
"%(project_id)s"

#: cinder/exception.py:372
#, python-format
msgid ""
"Volume Type %(volume_type_id)s has no extra specs with key "
"%(extra_specs_key)s."
msgstr ""
"У типа тома %(volume_type_id)s нет дополнительных спецификаций "
"с ключом %(extra_specs_key)s."

#: cinder/exception.py:377
#, python-format
msgid ""
"Volume Type %(volume_type_id)s deletion is not allowed with volumes "
"present with the type."
msgstr ""
"Удаление типа тома %(volume_type_id)s не разрешено, пока существуют тома "
"с таким типом"

#: cinder/exception.py:382
#, python-format
msgid ""
"The volume type %(volume_type_id)s is the only currently defined volume "
"type and cannot be deleted."
msgstr ""
"Тип тома %(volume_type_id)s является единственным настроенным на данный"
" момент типом тома, "
"и его нельзя удалить"

#: cinder/exception.py:387
#, python-format
msgid ""
"The volume type %(volume_type_id)s is a default volume type and cannot be"
" deleted."
msgstr ""
"Тип тома %(volume_type_id)s является типом тома по умолчанию, и его нельзя"
" удалить"

#: cinder/exception.py:392
#, python-format
msgid ""
"The request cannot be fulfilled as the default volume type "
"%(volume_type_name)s cannot be found."
msgstr ""
"Не удалось выполнить запрос, т.к. тип тома по умолчанию %(volume_type_name)s "
"найти не удалось."

#: cinder/exception.py:397
#, python-format
msgid "Default type for project %(project_id)s not found."
msgstr "Не удалось найти тип по умолчанию для проекта %(project_id)s"

#: cinder/exception.py:401
#, python-format
msgid "Group type %(group_type_id)s could not be found."
msgstr "Не удалось найти тип группы %(group_type_id)s"

#: cinder/exception.py:405
#, python-format
msgid "Group type with name %(group_type_name)s could not be found."
msgstr "Не удалось найти тип группы с именем %(group_type_name)s"

#: cinder/exception.py:410
#, python-format
msgid ""
"Group type access not found for %(group_type_id)s / %(project_id)s "
"combination."
msgstr ""
"Не удалось найти доступ к типу группы для сочетания %(group_type_id)s / "
"%(project_id)s"

#: cinder/exception.py:415
#, python-format
msgid "Group Type %(group_type_id)s has no specs with key %(group_specs_key)s."
msgstr ""
"У типа групп %(group_type_id)s нет спецификаций с ключом %(group_specs_key)s."

#: cinder/exception.py:420
#, python-format
msgid ""
"Group Type %(group_type_id)s deletion is not allowed with groups present "
"with the type."
msgstr ""
"Удаление типа группы %(group_type_id)s не разрешено, пока существуют группы "
"с таким типом"

#: cinder/exception.py:425
#, python-format
msgid "Snapshot %(snapshot_id)s could not be found."
msgstr "Не удалось найти снимок %(snapshot_id)s"

#: cinder/exception.py:429
#, python-format
msgid "Instance %(uuid)s could not be found."
msgstr "Не удалось найти экземпляр %(uuid)s."

#: cinder/exception.py:433
#, python-format
msgid "No snapshots found for volume %(volume_id)s."
msgstr "Для тома %(volume_id)s снимков не найдено"

#: cinder/exception.py:437
#, python-format
msgid "deleting volume %(volume_name)s that has snapshot"
msgstr "удаляется том %(volume_name)s, у которого есть снимки"

#: cinder/exception.py:441
#, python-format
msgid "deleting snapshot %(snapshot_name)s that has dependent volumes"
msgstr "удаляется снимок %(snapshot_name)s, у которого есть зависимые тома"

#: cinder/exception.py:446
#, python-format
msgid "Invalid image href %(image_href)s."
msgstr "Недопустимый href образа %(image_href)s."

#: cinder/exception.py:450
#, python-format
msgid "Signature metadata is incomplete for image: %(image_id)s."
msgstr "Неполные метаданные сигнатуры образа: %(image_id)s."

#: cinder/exception.py:455
#, python-format
msgid "Failed to verify image signature, reason: %(reason)s."
msgstr "Не удалось подтвердить сигнатуру образа: %(reason)s."

#: cinder/exception.py:459
#, python-format
msgid "Image %(image_id)s could not be found."
msgstr "Не удалось найти образ %(image_id)s."

#: cinder/exception.py:467
#, python-format
msgid "Service %(service_id)s could not be found on host %(host)s."
msgstr "Службу %(service_id)s не удалось найти на хосте %(host)s."

#: cinder/exception.py:470
#, python-format
msgid "Service %(service_id)s could not be found."
msgstr "Не удалось найти службу %(service_id)s."

#: cinder/exception.py:475
msgid "Service is too old to fulfil this request."
msgstr "Служба слишком старая, чтобы выполнить запрос"

#: cinder/exception.py:479
#, python-format
msgid "Worker with %s could not be found."
msgstr "Не удалось найти рабочий процесс с %s"

#: cinder/exception.py:489
#, python-format
msgid "Worker for %(type)s %(id)s already exists."
msgstr "Рабочий процесс с %(type)s %(id)s уже существует"

#: cinder/exception.py:493
#, python-format
msgid ""
"%(type)s with id %(id)s is already being cleaned up or another host has "
"taken over it."
msgstr ""
"%(type)s с ID %(id)s уже в процессе очистки, либо он был передан под"
" управление "
"другого хоста"

#: cinder/exception.py:498
#, python-format
msgid "Cluster %(id)s could not be found."
msgstr "Не удалось найти кластер %(id)s."

#: cinder/exception.py:502
#, python-format
msgid "Cluster %(id)s still has hosts."
msgstr "В кластере %(id)s ещё остались хосты."

#: cinder/exception.py:506
#, python-format
msgid "Cluster %(name)s already exists."
msgstr "Кластер %(name)s уже существует."

#: cinder/exception.py:510
#, python-format
msgid "Host %(host)s could not be found."
msgstr "Не удалось найти хост %(host)s."

#: cinder/exception.py:514
#, python-format
msgid "Scheduler Host Filter %(filter_name)s could not be found."
msgstr "Не удалось найти фильтр хостов %(filter_name)s планировщика"

#: cinder/exception.py:518
#, python-format
msgid "Scheduler Host Weigher %(weigher_name)s could not be found."
msgstr ""
"Не удалось найти инструмент планировщика для измерения веса хостов "
"%(weigher_name)s."

#: cinder/exception.py:522
#, python-format
msgid "Invalid reservation expiration %(expire)s."
msgstr "Недопустимое окончание срока действия резервирования %(expire)s."

#: cinder/exception.py:526
#, python-format
msgid ""
"Change would make usage less than 0 for the following resources: "
"%(unders)s"
msgstr ""
"В случае изменений, потребление следующих ресурсов будет меньше 0: %(unders)s"

#: cinder/exception.py:531
msgid "Quota could not be found"
msgstr "Не удалось найти квоту"

#: cinder/exception.py:535
#, python-format
msgid "Unknown quota resources %(unknown)s."
msgstr "Неизвестные ресурсы квоты %(unknown)s."

#: cinder/exception.py:539
#, python-format
msgid "Quota for project %(project_id)s could not be found."
msgstr "Не удалось найти квоту для проекта %(project_id)s."

#: cinder/exception.py:543
#, python-format
msgid "Quota class %(class_name)s could not be found."
msgstr "Не удалось найти класс квоты %(class_name)s."

#: cinder/exception.py:547
#, python-format
msgid "Quota usage for project %(project_id)s could not be found."
msgstr ""
"Не удалось найти статистику потребления для квоты проекта %(project_id)s."

#: cinder/exception.py:551
#, python-format
msgid "Quota exceeded for resources: %(overs)s"
msgstr "Квоты следующих ресурсов были превышены: %(overs)s"

#: cinder/exception.py:555
#, python-format
msgid "File %(file_path)s could not be found."
msgstr "Не удалось найти файл %(file_path)s."

#: cinder/exception.py:559
#, python-format
msgid "Volume Type %(id)s already exists."
msgstr "Тип томов %(id)s уже существует."

#: cinder/exception.py:563
#, python-format
msgid ""
"Volume type access for %(volume_type_id)s / %(project_id)s combination "
"already exists."
msgstr ""
"Доступ к типу тома для сочетания %(volume_type_id)s / %(project_id)s уже"
" существует"

#: cinder/exception.py:568
#, python-format
msgid "Volume type encryption for type %(type_id)s already exists."
msgstr "Шифрование типа тома для типа %(type_id)s уже существует"

#: cinder/exception.py:572
#, python-format
msgid "Volume type encryption for type %(type_id)s does not exist."
msgstr "Шифрование типа тома для типа %(type_id)s не существует"

#: cinder/exception.py:576
#, python-format
msgid "Group Type %(id)s already exists."
msgstr "Тип группы %(id)s уже существует."

#: cinder/exception.py:580
#, python-format
msgid ""
"Group type access for %(group_type_id)s / %(project_id)s combination "
"already exists."
msgstr ""
"Доступ к типу группы для сочетания %(group_type_id)s / %(project_id)s уже"
" существует"

#: cinder/exception.py:585
#, python-format
msgid ""
"Group volume type mapping for %(group_id)s / %(volume_type_id)s "
"combination already exists."
msgstr ""
"Сопоставление типа группы для сочетания %(group_id)s / %(volume_type_id)s уже"
" существует"

#: cinder/exception.py:590
#, python-format
msgid "Malformed message body: %(reason)s"
msgstr "Неправильный формат тела сообщения: %(reason)s"

#: cinder/exception.py:594
#, python-format
msgid "Could not find config at %(path)s"
msgstr "Не удалось найти файл конфигурации по пути %(path)s"

#: cinder/exception.py:598
#, python-format
msgid "Could not find parameter %(param)s"
msgstr "Не удалось найти параметр %(param)s"

#: cinder/exception.py:602
#, python-format
msgid "No valid backend was found. %(reason)s"
msgstr "Действительной серверной части найдено не было. %(reason)s"

#: cinder/exception.py:606
#, python-format
msgid "Quota exceeded: code=%(code)s"
msgstr "Превышена квота: code=%(code)s"

#: cinder/exception.py:613
#, python-format
msgid ""
"Requested volume or snapshot exceeds allowed %(name)s quota. Requested "
"%(requested)sG, quota is %(quota)sG and %(consumed)sG has been consumed."
msgstr ""
"Запрошенный том или снимок превышает квоту, установленную для %(name)s quota."
" Запрошено: "
"%(requested)sG, квота: %(quota)sG, было потреблено: %(consumed)sG."

#: cinder/exception.py:624
#, python-format
msgid ""
"Requested volume size %(size)dG is larger than maximum allowed limit "
"%(limit)dG."
msgstr ""
"Запрошенный размер тома %(size)dG превышает разрешённый макс. лимит "
"%(limit)dG."

#: cinder/exception.py:629
#, python-format
msgid ""
"Requested backup exceeds allowed Backup gigabytes quota. Requested "
